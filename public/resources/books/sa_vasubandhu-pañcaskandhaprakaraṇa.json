  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "ācārya vasubandhuviracitam Pañcaskandhaprakaraṇam"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prathamādhikāraḥ - skandhoddeśanirdeśakaḥ"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pañcaskandhāḥ"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "rūpaskandhaḥ vedanāskandhaḥ saṃjñāskandhaḥ saṃskāraskandhaḥ vijñānaskandhaśca // 1 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "rūpasya dvau bhedau\nrūpaṃ katamat / yat kiñcid rūpaṃ sarvaṃ taccatvāri mahābhūtāni catvāri ca mahābhūtānyupādāya // 2 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "mahābhūtanāmāni"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katamāni catvāri mahābhūtāni / pṛthivī dhātuḥ abdhātuḥ tejodhātuḥvāyudhātuśca // 3 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "mahābhūtalakṣaṇāni\ntatra pṛthivīdhātuḥ katamaḥ / kaṭhinatā // 4 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "abdhātuḥ katamaḥ / niṣyandatā // 5 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tejodhātuḥ katamaḥ uṣṇatā // 6 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vāyudhātuḥ katamaḥ / paunaḥ punyakampanatā // 7 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "upādāyarūpanāmāni\nupādayarūpaṃ katamat / cakṣurindriyam, śrotrendriyam, ghraṇendriyam, jihvendriyam, kāyendriyam,rūpam, śabdaḥ, gandhaḥ, rasaḥ, spraṣṭavyaikadeśaḥ, abijñaptiśca // 8 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "upādāyarūpalakṣaṇāni cakṣurindrayaṃ katamat / varṇabiṣayo rūpaprasādaḥ // 9 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "śrotrendrayaṃ katamat / śabdaviṣayo rūpaprasādaḥ // 10 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "ghrāṇendriyam katamat / gandhaviṣayo rūpaprasādaḥ // 11 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "jihvendriyam katamat / rasaviṣayo rūpaprasādaḥ // 12 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kāyendriyam katamat / spraṣṭavyaviṣayo rūpaprasādaḥ // 13 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "rūpaṃ katamat / cakṣurgocaraḥ / varṇaḥ saṃsthānam bijñaptiśca // 14 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "śabdaḥ katamaḥ / śrotragocaraḥ / niṣpannḥ aniṣpannaḥ ubhayaḥ / upāttacatumarhābhutahetukaḥ anupāttacaturmahābhūtahetukaḥ ubhayacaturmahābhūtahetukaśca // 15 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "gandhaḥ katamaḥ / ghrāṇagocaraḥ / surabhiḥ asurabhiḥ tadanyaśca // 16 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "rasaḥ katamaḥjihvāgocaraḥ / madhuraḥ amalaḥ lavaṇaḥ kaṭuḥ tiktaḥ kaṣāyaśc // 17 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "spraṣṭavyaḥ katamaḥ / kāyagocaraḥ / mahābhūtānyupādāya ślakṣṇatvam karkasatbam gurutvam laghutvam śītatvam jighatsā pipāsā ca // 18 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "abijñaptirūpaṃ katamat / vijñaptisamādhisaṃbhūtaṃ rūpam anidarśanam apratigham // 19 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "rūpaskandhaḥ soniddeśaḥ pariniṣṭhitaḥ / atha vedanā salakṣaṇaprabhedā vedanā katamā / trividho 'nubhavaḥ / sukhaḥ duḥkhaḥ aduḥkhāsukhaśca // 20 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sukho yasmin niruddhe saṃyogecchā jāyate // 21 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "duḥkho yasminnutpanne viyogecchā jāyate // 22 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aduḥkhāsukho yasminnutpanne nobhayecchā jāyane // 23 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vedanaskandhaḥ sonideśanirdeśaḥ parinisṭhataḥ /\natha saṃjñā lakṣaṇa prabhedā saṃjñā katamā / viṣayanimittod grahaṇam // 24 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sā tridhā parittā mahadgatā apramāṇā ca // 25 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "saṃjñā skandhaḥ soddeśanirdeśaḥ parinisṭhitaḥ /"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "saṃskārāṇāṃ dvaubhedau"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "saṃskārāḥ katame / vedanāsaṃjñābhyāmanye caitāḥ cittaviprayuktāśca // 26 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "tatra caitadharmāḥ katame / ye dharmārścittena saṃprayuktāḥ // 27 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "caitaparisaṃkhyānam\nte katame / sparśaḥ manaskāraḥ vedanā saṃjñā cetanā chandaḥ adhimokṣaḥ smṛtiḥ samādhiḥ prajñā śraddhā hrīḥ apatrapā alobhaḥ kuśalamūlam adveṣaḥkuśalamūlam amohaḥkuśalamūlam vīryam praśrabdhiḥ apramādaḥ upekṣā avihiṃsā rāga pratighaḥ mānaḥ avidyā dṛṣṭiḥ vicikitsā krodha upanāhaḥ mrakṣaḥ pradāśaḥ īrṣyā mātsaryarm māyā śāṭhyam madaḥ ahiṃsā āhrikyam anapatrāpyam styānam, auddhatyam, āśraddhyam, kauśīddhyam pramādaḥ muṣitāsmṛti, bikṣepaḥ, aprasajanyam, kaukrityam, middham,vitarkaḥ, vicāraśca // 28 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "eteśāṃ paśca sarvatragāḥ / pañcapratiniyataviṣayā / ekādaśakuśalāḥ / ṣaṭkleśāḥ / śiṣṭā upakleśāḥ / catvāro 'nyatrāpi bhavantītyaniyatāḥ // 29 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "caittalakṣaṇāni\nsparśaḥkatamaḥ / trikasaṃnipāte indriyavikāraparicchedaḥ // 30 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "manaskāraḥ katamaḥ / cetasa ābhogaḥ // 31 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "cetanā katamā / guṇadoṣānubhayeṣu cittābhisaṃskāro manaskarma // 32 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "chandaḥ katamaḥ / abhiprete vastunyabhilāṣaḥ // 33 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "adhimokṣaḥ katamaḥ / niścite vastuni tathaivāvadhāraṇam // 34 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "smṛtiḥ katamā / saṃstute vastunyasaṃpramoṣaḥ // 35 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "samādhiḥ katamaḥ / upaparīkṣye vastuni cittasyaikāgratā // 36 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "prajñā katamā / tatraiva pravicayo yogāyogavihito 'nyathā ca // 37 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "śraddā katamā / karmaphalāsatyaratneṣvabhisaṃpratyayaḥ / cetaso 'bhilāṣaḥ cetasaḥ prasādaḥ // 38 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "hrīḥ / katamā ātmānaṃ dharmaṃ vādhipatiṃ kṛtvā svayamavayena lajjā // 39 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "apatrapā katamā / lokamadhipatiṃ kṛtvā parato 'vadhena lajjā // 40 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "alobhaḥ katamaḥ / lobha pratipakṣaḥ / cetaso dānakṣetrāparigrahaḥ // 41 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "adveṣaḥ katamaḥ / dveṣapratipakṣaḥ / maitri // 42 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "amohaḥ katamaḥ / mohapratipakṣaḥ / yathābhūtasaṃpratipattiḥ // 43 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vīryaṃ katamat kausīddyapratipakṣaḥ kuśale cetaso 'bhyutsāhaḥ // 44 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "praśrabdhiḥ katamā / dauṣṭhulyapratipakṣaḥ kāyacittakarmaṇyatā // 45 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "apramādaḥ katamaḥ / pramādapratipakṣaḥ / alobhādīn vīryāntān niśritya akuśalānāṃ dharmāṇāṃ prahāṇam / tatpratipakṣāṇāṃ kuśalānāṃ dharmāṇāṃ bhāvanā // 46 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "upekṣā katamā / alobhādīn vīryantān niśritya cittasamatā / cittapraśastā / cittānābhogatā / yayākliṣṭadharmānavakāśenākliṣṭeṣu sthitiḥ // 47 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "avihiṃsā katamā / vihiṃsāpratipakṣaḥ karuṇā // 48 ///"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "rāgaḥ katamaḥ / upādānaskandheṣvabhyarthanā / adhyavasānam // 49 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "pratighaḥ katamaḥ / satveṣvāghātacittatā // 50 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "mānaḥ katamaḥ / mānaḥ saptavidhaḥ / atimānaḥ, mānātimānaḥ asmimānaḥ, abhimānaḥ, ūnamānaḥ,mithyāmānaśca // 51 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atimānaḥ katamaḥ / hīnāt śreyānahamasmi sadṛśena vā sadṛśo 'smīti cintayato yā cittasyonnatiḥ // 53 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "mānātimānaḥ katamaḥ / śreyaso 'hameva śreyāniti cintayato yā cittasyonnatiḥ // 54 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "asmimānaḥ katamaḥ / pañcasūpādānaskandheṣvātmānamātmīyaṃ vā paśyato yā cittasyonnati // 55 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "abhimānaḥ katamaḥ / aprāpta uttare viśeṣādhigame prāpto mayeti cintayato yā cittasyonnatiḥ // 56 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "ūnamānaḥ katamaḥ / bahṛvantaraviśiṣṭādalpāntarahino 'smīti cintayato yā cittasyonnatiḥ // 57 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "mithyāmānaḥ katamaḥ / aguṇavato guṇavānahamasmīti cintayato yā cittasyonnatiḥ // 58 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "aviddhyā katamā / karmaphalasatyaratnānāmajñānam / sā punaḥ sahajā parikalpitā ca // 59 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "rāgacaritasya rāgapratighayoḥ rāgacaritasyāviddyāyāścetyakuśalānāṃ trīṇī mūlāni / lobho 'kuśalamūlaṃ ddheṣo 'kuśalamūlaṃ, moho 'kuśalamūlaṃ ca // 60 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dṛṣṭiḥ katamā pañcadṛṣṭyaḥ - satkāyadṛṣṭiḥ, antargrāhyadṛṣṭiḥ, mithyādṛṣṭiḥ, dṛṣṭiparāmarśaḥ, śīlavrataparāmarśaśca // 61 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "satkāyadṛṣṭiḥ katamā / pañcopādāna - skandhānātmata ātmīyato vā paśyato yā kliṣṭā prajñā // 62 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "antargrāhyadṛṣṭiḥ katamā / tānevādhipatiṃ kṛtvā śāśvatata ucchedato vā paśyato yā kliṣṭā prajñā // 63 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "mithyādṛṣṭiḥ katamā / hetuṃ vā phalaṃ vā kriyāṃ vāpavadataḥ sadāvastu vināśayato yā kliṣṭā prajñā // 64 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "dṛṣṭiparāmarśaḥ katamaḥ / tā eva tikhro dṛṣṭistadāśrayāṃśca skandhān śreṣṭhataḥ paramataśca paśyato yā kliṣṭā prajñā // 65 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "śīlabrataparāmarśaḥ katamaḥ / śīlaṃ vrataṃ tadāśrayāṃśca skandhānaśuddhito muktito naiyāṇikataśca paśyato yā kliṣṭā prajñā // 66 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vicikitsā katamā / satyādiṣu yā dvikoṭikā matiḥ // 67 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "eteṣāṃ kleśānāṃ paścimāstistro dṛṣṭayaḥ vīcikitsā ca parikalpitāḥ / śiṣṭāḥ sahajāḥ parikalpitāśca // 68 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "krodhaḥ katamaḥ / vartaṃmānamapakāramāgamya yā cetasa āghātacetanā // 69 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "upanāhaḥ katamaḥ / vairānubandha // 70 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mrakṣaḥ katamaḥ ātmano 'vaddhapracchādanā // 71 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pradāśaḥ katamaḥ caṇḍavacodāśitā // 72 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "īrṣyā katamā parasaṃpattau cetaso vyāropa // 73 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "mātsarya katamat / dānavirodhī cetasa āgrahaḥ // 74 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "māyā katamā / paravaścanābhūtārthasadarśanatā // 75 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "śāṭhyaṃ katamat / svadoṣapracchādanopāyasaṃgṛhitaṃ cetasaḥ kauṭilyam // 76 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "madaḥ katamaḥ / svasaṃpattau raktasyoddharṣaḥ / cetasaḥ paryādānam // 77 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vihiṃsā katamā / satveṣu viheṭhanā // 78 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "āhrīkyaṃ katamat / avadyena svayamalajjā // 79 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "anapatrāp katamat / avadyena parato 'lajjā // 80\nsatyānaṃ katamat / cittākarmaṇyatā / staimityam // 81 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "auddhatyaṃ katamat / cittasyāvyupasamaḥ // 82 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "āśraddhyaṃ katamat / karmaphalasatyaratneṣvanabhisaṃpratyayaḥ / śraddhāvipakṣaścetaso 'prasādaḥ // 83 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kauśiddyaṃ katamatṃḥ kuśale cetaso 'nabhyutsāho vīryavipakṣa // 84 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "pramādaḥ katamaḥ / yā lobhadveṣamohekauśīddyaiścitasyānārakṣā kuśalasyābhāvanā // 85 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "muṣitāsmṛtiḥ katamā / kliṣṭāsmṛtiḥ / kuśalāpratipatiḥ // 86 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vikṣepaḥ katamaḥ / rāgadveṣamohāṃśikaḥ pañcakāmaguṇeṣu cittasya visāraḥ // 87 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "aprasajanyaṃ katamat kleśasaṃprayuktā prajñā / tayā kāyavākcittacaryā asaṃviditeva pravartate // 88 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kaukṛtyaṃ katamat / cetaso vipratisāraḥ // 89 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "middhaṃ katamat / asvataṃtravṛtticetaso 'bhisaṃkṣepaḥ // 90 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vitarkaḥ katamaḥ paryeṣako manojalpaḥ cetanāprajñāviśeṣaḥ yā cittasyaudārikatā // 91 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vicāraḥ katamaḥ / paryeṣako manojalpaḥ tathaiva yā cittasya sūkṣmatā // 92 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// ittaviprayuktaparisaṃkhyānam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "cittaviprayuttasaṃskārāḥ katame / ye rūpacitacaittadhikāre prajñaptāḥ, ta evānyatrāprajñaptāḥ // 93 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "te katame / prāpti, asaṃjñisamāpattiḥ, nirodhasamāpattiḥ, āsaṃjñikam, jīvitendriyam, nikāyasabhāgatā, jāti, jarā, sthitiḥ, anityatā, nāmakāyaḥ, padakāyaḥ, vyañjanakāyaḥ, pṛthagjanatvamityevamādibhedasamādānāḥ // 94 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "ittaviprayuktalakṣaṇāni // tatra prāptiḥ katamā / pratilambhaḥ samanvāgamaśca / bījam, vaśitā,abhimukhībhāvaśca yathāyogam // 95 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "asaṃjñisamāpattiḥ katamṃ / śubhakṛtsnavītarāgasyoparyavītarāgasya niḥsaraṇasaṃjñāpūrvakeṇa manasikāreṇāvasthāvarāṇāṃ cittacaittadharmāṇāṃ yo nirodhaḥ // 96 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "nirodhasamāpattiḥ katamā / ākiñcanyāyatanabītarāgasya bhavāgratāduccalitasya śāntavihāra - saṃjñāpūrvakeṇa manasikāreṇāsthāvarāṇāṃ cittacaittadharmāṇāṃ tadekatyānāṃ ca sthāvarāṇāṃ yo nirodhaḥ // 97 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "āsaṃjñikaṃ katamat / asaṃjñisatvanikāyadeveṣupapannasyāsthāvarāṇāṃ cittacaittadharmāṇāṃ nirodhaḥ // 98 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "jīvitendriyam katamat / nikāyasabhāgeṣu pūrvakarmānuviddhyeṣu yaḥ saṃskārāṇāṃ sthitikālaniyamaḥ // 99 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "nikāyasabhāgaḥ katamaḥ / yā satvānāmātmabhāvasadṛśatā // 100 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "jātiḥkatamā / nikāyasabhāge saṃskārāṇāmabhūtvā yo bhāṣaḥ // 101 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "jarā katamā / tathā teṣāṃ prabandhānyathātvam // 102 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "sthiti katamā / tathā teṣāṃ prabandhanvayaḥ // 103 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "anityatā katamā / tathā teṣāṃ prabandhavināśaḥ // 104 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "nāmakāyaḥ katamaḥ / dharmāṇāṃ svabhāvādhivacanam // 105 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "pādakāyaḥ katamaḥ / dharmāṇāṃ viśeṣādhivacanam // 106 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vyañjanakāyaḥ katamaḥ / akṣarāṇi / tadubhayābhivyañjanatāmupādāya // 107 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vākyāni tāni / nāmapadāśreyeṇārthavacanatāmupādāya // 108 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "akṣaraṃ punaḥ paryāyākṣaraṇatāmupādāya // 109 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "pṛthagjanatvaṃ katamat / āryadharmāṇāmapratilambhaḥ // 110 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iti saṃskāraskandho nāma //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃskāraskandhaḥ soddyeśanirdeśaḥ pariniṣṭhitaḥ // 111 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// vijñānalakṣaṇam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vijñānaṃ katamat / ālambanaṃbijñaptiḥ // 112 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "cittamanasī vijñānaparyāyau sanirvacano taccitaṃ mano 'pi citrīkāratā manoniśrayatāṃ ca upādāya // 113 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// salakṣaṇacittanirdeśaḥ //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mūlacittamālayavijñānaṃ tadyathedaṃ sarvasaṃskārāṇāṃ saṃcitaṃ bījam // 114 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "tatpunarālambanamapyuparichinnamekasantānavarti / yathā nirodhasamāpatyasaṃjñisamāpatyā - saṃjñikebhyovyutthitasyāsmād viṣayavijñaptikaṃ nāma pravṛtti vijñānaṃ jāyate / ālambanapratyayamapekṣya bhinnākāreṣu vṛttitāmucchiddya punarjātatāṃ saṃsāre pravṛttinivṛtitāṃ copādāya tadālambanaṃ nāma vijñānam // 115 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ālayavijñānaṃ hi sarvabījālayatāmātmabhāvālayahetutāmātmabhāve sthititāṃ copādāya // 116 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "ādānavijñānamapi tat / ātmabhāvādānatāmupādāya // 117 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// salakṣaṇamanonirdeśaḥ //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mūlamana ālayavijñānamāśritya nityamātmamohātmadṛṣṭyātmamānātmasnehaiḥ saṃprayuktaṃ vijñānamekajātīyasantānavarti arhatvāryamārganirodhasamāpattikāle vyāvartate // 118 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vijñānaskandhaḥ soniddeśanirdeśaḥ pariniṣṭhitaḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// skandhanirvacanam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kimarthaḥ skandhaḥ rāśyarthaḥ / kālagotrākāragativiṣayabhinnānāṃ rūpādīnāmabhisaṃkṣepatāmupādāya // 119 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "skandhoddyeśanirdeśakaḥ prathamādhikāraḥ pariniṣṭhitaḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "dvitīyādhikāraḥ\nāyatanoddyeśanirdeśakaḥ\ndvādaśāyatanāni / cakṣurāyatanam, rūpāyatanam, śrotrāyatanam, śabdāyatanam, ghrāṇāyatanam, gandhāyatanam, jihvāyatanam, rasāyatanam, kāyāyatanam, spraṣṭavyāyatanam, mana āyatanam,dharmāyatanaṃ ca // 120 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "/ 'ayatanānāṃ salakṣaṇaprapañcaḥ //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "cakṣurādīni rūpaśabdagandharasāyatanāni yathoktapūrvāṇi // 121 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "spraṣṭavyāyatanaṃ catvāri mahābhūtāni uktaśca yaḥ spraṣṭabyaikadeśaḥ // 122 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mana āyatanaṃ yo vijñānaskandhaḥ // 123 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dharmāyatanaṃ vedanā, saṃjñā, saṃskārāḥ, avijñaptiḥ, asaṃskṛtāśca // 124 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "/ 'saṃskṛtāḥ salakṣaṇaprabhedāḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "asaṃskṛtā katame / ākāśam, apratisaṃkhyānirodhaḥ, pratisaṃkhyānirodhaḥ tathatā ca // 125 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "tatrākāśaṃ katamat / rūpāvakāśaḥ // 126 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "apratisaṃkhyānirodhaḥ katamaḥ / yo nirodho na visaṃyogaḥ // 127 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sakleśāpratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 128 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "pratisaṃkhyānirodhaḥ katamaḥ / yo nirodho visaṃyogaḥ // 129 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sakleśapratipakṣeṇa skandhānāmātyantiko nirodhaḥ // 130 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "tathatā katamā / yā dharmāṇāṃ dharmatā / dharmanairātmyam // 131 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "/ 'ayatananirvacanam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kimupādāyāyatanamiti / vijñānotpādadvāratāmupādāya // 132 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "āyatanoddyeśanirdeśako dvitīyādhikāraḥ pariniṣṭhitaḥ /\ntṛtīyādhikāraḥ"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "dhātūddeśanirdeśakaḥ\n// dhātunāmāni //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "aṣṭadaśa dhātavaḥ / cakṣurdhātuḥ, rūpadhātuḥ, cakṣurvijñānadhātu, śrotradhātuḥ, śabdadhātuḥ, śrotravijñānadhātuḥ, ghrāṇadhātuḥ, gandhadhātuḥ, ghrāṇavijñānadhātuḥ, jihvādhātuḥ, rasadhātuḥ, jihvāvijñānadhātuḥ, kāyadhātuḥ, spraṣṭavyadhātuḥ, kāyabijñānadhātuḥ, manodhātuḥ, dharmadhātuḥ, manovijñānadhātuśca // 133 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// dhātuprapaścaḥ //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "cakṣurādayo dhātavo rūpādayo dhātavaśca yathāyatanāni // 134 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ṣaḍvijñānadhātavaścakṣurāddyāśrayā rūpādyālambanāvijñaptayaḥ // 135 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "manodhātusteṣāmeva samanantaranirudhyaṃ ṣaḍvijñānadeśanāśrayatāmupādāya // 136 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// dhātuvyavasthanam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "ebamaṣṭādaśadhātuvyavasthānam / yo rūpaskandhastāni daśāyatanadhātavaḥ // 137 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dharmāyatanaṃ dharmadhātvekadeśaḥ // 138 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "yo vijñānaskandhastanmana āyatanam, saptama manodhātavaśca // 139 // anye ye trayaḥ skandhā rūpadhātvekadeśaśca, te 'saṃskṛtai sahitā dharmāyatanaṃ dharmadhātuśca // 140 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// dhātunirvacanam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kimupādāya dhātava iti / ākārakātmalakṣaṇadhāraṇatāmupādāya // 141 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// skandhāyatanadhātukrameṇa dharmadeśanāyāḥ prayojanam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kimartha skandhādikrameṇa deśanā / trividhātmagrāhapratipakṣeṇāyaṃ kramaḥ // 142 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmagrāhastrividhaḥ ekātmagrāhaḥ, vedakātmagrāhaḥ, kārakātmagrāhaśca // 143 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// 'ṣṭādaśadhātuvikalpāḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "aṣṭādaśadhātuṣu kati rūpiṇaḥ / ye rūpaskandhasvabhāvāḥ // 144 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyarūpiṇaḥ / śiṣṭāḥ // 145 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati sanidarśanāḥ / rūpadhāturekaḥ sanidarśanaviṣayaḥ // 146 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyanidarśanāḥ / śiṣṭāḥ // 147 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati sapratidhāḥ / daśa rūpiṇaḥ / yatra yatpratidhātastatra tat sapratigham // 148 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyapratighāḥ / śiṣṭāḥ // 149 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati sāsravāḥ / pañcadaśa paścimānāṃ trayāṇāmekadeśaśca / te kleśotpādapratyakṣacaryāviṣayatāmupādāyaṃ // 150 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyayanāstravāḥ / paścimānāṃ trayāṇāmekadeśaḥ // 151 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati kāmapratisaṃyuktāḥ / sarva // 152 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati rūpapratisaṃyuktāḥ / caturdaśa sthāpayitvā gandharasaghrāṇajihvāvijñānadhātūn // 153 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyārūpyasaṃprayuktāḥ / paścimāśrayaḥ // 154 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyapratisaṃyuktāḥ / trayāṇāmekadeśaḥ // 155 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati skandhasaṃgrahītāḥ / asaṃskṛtān sthāpayitvā // 156 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "katyupādānaskandhasaṃgṛhītāḥ ye sāsravāḥ // 157 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati kuśalākuśalāvyākṛtāḥ / daśa trividhāḥ / sapta manodhātavaḥ, rūpaśabdadharmadhātavaśca / śiṣṭā avyākṛtāḥ // 158 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyādhyātmikāḥ / dvādaśa / sthāpayitvā rūpaśabda gandharasaspraṣṭāvyadharmadhātūn // 159 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati bāhyāḥ / ṣaṭ / ye sthāpitāḥ // 160 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati sālambanāḥ / saptacittadhātavaḥ dharmadhātvekadeśo 'pi yaścaittaḥ // 161 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "katyanālambanāḥ / śiṣṭā daśa dharmadhātvekadeśaśca // 162 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati savitarkāḥ / manodhātuḥ manovijñānadhātuḥ dharmadhātvekadeśaśca // 163 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati niṣpannāḥ / pañcādhyātmikāḥ / caturṇāmekadeśaḥ // 164 //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "katyaniṣpannāḥ śiṣṭā daśa, caturṇāmekadeśaśca // 165 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati sabhāgāḥ / ādhyātmikāḥ pañca rūpiṇaḥ / svavijñānasahitaviṣayasadṛśatāmupādāya // 166 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kati tatsabhāgāḥ / ta eva svavijñānavirahitasvānvayasadṛśatāmupādāya // 167 //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "dhātūddyeśanirdeśakastṛtīyādhikaraḥ pariniṣṭitaḥ /"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "ācāryavasubandhuviracit pañcaskandhaprakaraṇaṃ samāptam /\npañcaskandhaprakaraṇaṃ śāstriṇā śāntibhikṣuṇā /",
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhoṭānuvādamāgamya saṃskṛte punaruddhṛtam"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "yadatra sukṛtaṃ sarva tadācāryasya dhīmataḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "yat kukṛtaṃ tadāgo me kṣantavyaṃ sādhusūribhiḥ //"
            }
          ]
        }
      ]
    }
  ]
}