  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "paṇḍitarājaśrījagannāthaviracitaṃ prāṇābharaṇam /\ntatkṛtayaiva ṭippaṇyā sametam /"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vidvāṃso vasudhātale paravacaḥślāghāsu vācaṃyamā bhūpālāḥ kamalāvilāsamadironmīlanmadāghūrṇitāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āsye ghāsyati kasya lāsyamadhunā dhanyasya kāmālasasvarvāmādharamādhurīmadharayanvācāṃ vilāso mama // JPran_1 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "vidvāṃsa ityādi prāyadarśanābhiprāyametat /\ntena sahṛdayairna manāgapi vimanāyitavyam /",
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhāvadhvaniścāyam /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "uttarārdhapratipādyārthālambanāyā etatpadyaprayogānubhāvāyāḥ kavigatacintāyāḥ prādhānyenābhivyakteḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "anupāttobhayanimittako vyatirekaḥ sphuṭo 'laṃkāraḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "kāmālasatvaṃ vāmāviśeṣamadharamādhurīprakarṣakam //"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vidrāṇaiva guṇajñatā samudito bhūyānasūyābharaḥ kālo 'yaṃ kalirājagāma jagatīlāvaṇyakukṣiṃbhariḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "evaṃ bhāvanayā madīyakavite maunaṃ kimālambase jāgartu kṣitimaṇḍale ciramiha śrīkāmarūpeśvaraḥ // JPran_2 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kavigatarājaviṣayakaratibhāvadhvaniścāyam /",
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "ita ūrdhvamayameva ā caramapadyamanuvartayiṣyate /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "asya cātra maunānubhāvito varṇanīyālambano nirvedo guṇa iti preyolaṃkārāspadam /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "atra cācetanāyāṃ kavitāyāṃ cetanatvādhyavasāyamūlāsaṃbandhe saṃbandhātmikātiśayoktirvivakṣitā /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tena bhāvanāmaunanirvedānāṃ saṃbodhanasya ca nānupapattiḥ //"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pārīndrāṇāṃ ghurīṇairavanitalaguhāgarbhataḥ saṃpatadbhiḥ svāpabhraṃśāparādhapracalitanayanaprāntamākarṇyamānaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tvatprasthānāntarudyatpralayajaladharadhvānadhikkāradhīro ghṛṣṭakṣīrodatīro jagati vijayate dundubhidvandvanādaḥ // JPran_3 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra rājālambanasya tādṛśanādaśravaṇoddīpitasya nayanapracalanānubhāvitasya giriguhāgarbhotpatanābhivyaktenāmarṣeṇa saṃcāriṇā paripoṣitasya pārīndragatotsāhasya sthāyino rājaviṣayakaratibhāvāṅgatvādrasālaṃkāratvam /",
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "yadāhuḥ -"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "'pradhāne 'nyatra vākyārthe yatrāṅgaṃ tu rasādayaḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "kāvye tasminnalaṃkāro rasādiriti me matiḥ'; //"
            }
          ]
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iti /\ntṛtīyacaturthacaraṇayostu sphuṭāveva vyatirekātiśayau /",
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "ghṛṣṭāśabdena tādṛśanādo velācalapratibaddhatvādagre na gataḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "anyathā lokālokācalamapi spṛśediti gamyate /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "eva mṛṣṭeti noktam /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tathā sati śaithilyapratyayāpatteḥ //"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kiṃ brūmastava vīratāṃ vayamamī yasmindharākhaṇḍala krīḍākuṇḍalitabhru śoṇanayanaṃ dormaṇḍalaṃ paśyati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māṇikyāvalikāntidanturatarairbhūṣāsahasrotkarairvindhyāraṇyaguhāgṛhāvaniruhāstatkālamullāsitāḥ // JPran_4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra vindhyāraṇyagatānāṃ guhāgṛhāṇāmavaniruhāṃ ca vindhyagatānāṃ guhāgṛhāṇāmaraṇyagatānāmavaniruhāṇāṃ vā bhūṣaṇena kāryeṇāprastutena tvadarinārīṇāṃ svanagarāṇi parityajya niśi guhāgṛheṣu tarutaleṣu ca vinyasya sakalābharaṇāni kṛtaśayanānāṃ prātastvadāgamanasaṃbhrameṇa tatkarmakaṃ vismaraṇaṃ bhāravaśātparityāgo vā prastuto gamyata ityaprastutapraśaṃsā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryasya yathākathaṃcitprastutatve tu paryāyoktamalaṃkāraḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "māhātmyasya paro 'vadhirnijagṛhaṃ gambhīratāyāḥ pitā ratnānāmahameka eva bhuvane ko vāparo mādṛśaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ityevaṃ paricintya mā sma sahasā garvāndhakāraṃ gamo dugdhābdhe bhavatā samo vijayate śrīprāṇanārāyaṇaḥ // JPran_5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atropamānasya guṇaviśeṣaprayuktasādṛśyābhāvanibandhanamutkarṣaṃ parihartuṃ varṇyamānasādṛśyātmakaḥ pratīpālaṃkārabhedaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sa copamāviśeṣa ityeke /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vicchittivailakṣaṇyādatirikta evetyapare //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvo janma sitāṃśuśekharatanujyotsnānimagnātmano dugdhāmbhonidhimugdhavīcivalayaiḥ sākaṃ parikrīḍanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃvāsaḥ suralokasindhupuline vādaḥ sudhāṃśoḥ karaiḥ kasmānnojjvalimānamañcatutamāṃ deva tvadīyaṃ yaśaḥ // JPran_6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra yaśasi dhavalatātiśayastaddharmisaṃbandhaprayuktatvena kathita iti samasya viṣayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aṃśukṛtaścandre tatkṛtaśca bhagavati bhagavatkṛtaśca rājanītyevamuttarottaramupacīyamāno rājagata utkarṣaḥ pratīyata iti sāraviṣayaḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ābadhnāsyalakānnirasyasitamāṃ colaṃ rasākāṅkṣayā laṅkāyāvaśatāṃ tanoṣi kuruṣe jaṅghālalāṭakṣatam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyaṅgaṃ parimardanirdayamaho cetaḥ samālambase vāmānāṃ viṣaye nṛpendra bhavataḥ prāgalbhyamadbhutam // JPran_7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra prakṛtidharmigatayoḥ prakṛtāprakṛtayoḥ prakṛtayoreva vā vṛttāntayoḥ śleṣaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sa ca ṣaṭsu sthaleṣu śabdanānātvanibandhano dvayoścārthanānātvanibandhanaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dvāvapyetau śabdālaṃkārāviti prāñcaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ādyo jatukāṣṭhanyāyena śabdaśleṣaṇācchabdālaṃkāraḥ, dvitīyastvekavṛntagataphaladvayanyāyenārthaśleṣaṇādarthālaṃkāra iti navyāḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "deva tvāṃ paritaḥ stavantu kavayo lobhena kiṃ tāvatā stavyastvaṃ bhavitāsi yasya taruṇaścāpapratāpo 'niśam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kroḍāntaḥ kurutetarāṃ vasumatīmāśāḥ samāliṅgati dyāṃ cumbatyamarāvatī ca sahasā gacchatyagamyāmapi // JPran_8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra pratāpagataḥ pṛthivyādisaṃbandho liṅgaviśeṣāvacchinnatattatsādhāraṇaviśeṣaṇābhivyaktakāmukavṛttāntābhinnatayā sthita iti samāsoktiḥ kāryarūpadharmaprayuktaśuddhasādhāraṇyena viśeṣaṇasāmyamālambya pravṛttā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sā ca nindotthāpakatvādadvyājastutau guṇaḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "lokānāṃ vipadaṃ dhunoṣi kuruṣe saṃpattimatyutkaṭāmityalpetarajalpitairjaḍadhiyāṃ bhūpāla mā gā madam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yatkīrtistava vallabhā laghutarabrahmāṇḍabhāṇḍodare piṇḍīkṛtya mahonnatāmapi tanuṃ kaṣṭena hā vartate // JPran_9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atrāpi prāgvat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "paraṃ tvādhārādheyānyataravistṛtatvasiddhiphalakānyataranyūnatvakalpanātsādhikālaṃkāro 'pi tasyāṃ guṇaḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kṣoṇīṃ śāsati mayyupadravalavaḥ kasyāpi na syāditi prauḍhaṃ vyāharato vacastava kathaṃ deva pratīmo vayam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyakṣaṃ bhavato vipakṣanivahairdyāmutpatadbhiḥ krudhā yadyuṣmatkulakoṭimūlapuruṣo nirbhidyate bhāskaraḥ // JPran_10 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iha tvadhikasamāsoktibhyāmanāliṅgitaiva sā [vyājastutiḥ] //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āsvādena raso rasena kavitā kāvyena vaṇī tayā lokāntaḥkaraṇānurāgarasikaḥ sabhyaḥ sabhā cāmunā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dāridryānaladahyamānajagatīpīyūṣadhārādhara kṣoṇīnātha tathā bhavāṃśca bhavatā bhūmaṇḍalaṃ bhāsate // JPran_11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mālādīpakametaditi prāñcaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dīpakasya sādṛśyamūlakatāniyamānna mālādīpakamapi tvekāvalībheda iti tu vayam //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "amlāyanyadarātikairavakulānyamlāsiṣuḥ satvaraṃ dainyadhvāntakadambakāni parito neśustamāṃ tāmasāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sanmārgāḥ prasaranti sādhunalinānyullāsamātanvate tanmanye bhavataḥ pratāpatapano deva prabhātonmukhaḥ // JPran_12 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iha rūpakaniṣpāditaliṅgakamanumānaṃ nimittavirahādutprekṣāyā ayogādvācakamanumitiparam //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "utkṣiptāḥ kabarībharaṃ vivalitāḥ pārśvadvayaṃ nyakkṛtāḥ pādāmbhojayugaṃ ruṣā parihṛtā dūreṇa celāñcalam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "gṛhṇanti tvarayā bhavatpratibhaṭakṣmāpālavāmabhruvāṃ yāntīnāṃ gahaneṣu kaṇṭakacitāḥ ke ke na bhūmīrūhāḥ // JPran_13 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra kaṇṭakacitatvena kabarīgrahaṇādeḥ saṃkīrṇatvātkāryadharmāntarā saṃkīrṇaśuddhasādhāraṇyena viśeṣaṇasāmyamālambya pravṛttā samāsoktiḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛṣṭiḥ saṃbhṛtamaṅgalā budhamayī deva tvadīyā sabhā kāvyasyāśrayabhūtamāsyamaruṇādhāro 'dharaḥ sundaraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "krodhaste 'śanimūrudāradhiṣaṇa svāntaṃ tu somāspadaṃ rājannūnamanūnavikrama bhavānsarvagrahālambanam // JPran_14 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atrotprekṣyamānasarvagrahālambanatvasya samānādhikaraṇeṣu dharmeṣu tattadgrahāśritāṅgakatveṣu viśeṣaṇībhūtaistattadgrahaiḥ saha viṣayasya rājño kalyāṇāśrayatvādiṣu viśeṣaṇānāṃ kalyāṇādīnāṃ śleṣeṇābhedasaṃpādanadvārā tādṛśadharmasādhāraṇatāsaṃpattau tannimittakotprekṣāsiddhiḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sṛṣṭiḥ sṛṣṭibhuvā purā kila paritrātuṃ jaganmaṇḍalaṃ tvaṃ caṇḍātapanirdayaṃ tapasi yajjvālājaṭālaiḥ karaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃrambhāruṇalocano raṇabhuvi prasthātukāmo 'dhunā jānīmo bhavatā na hanta viditaḥ śrīkāmarūpeśvaraḥ // JPran_15 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra rājavarṇanāṅgatvena raverbhayotpādane varṇyatvena prastute sākṣāttadananuguṇatvenāprastutena prasthānena sākṣāttadanuguṇaṃ ripukartṛkaṃ sūryamaṇḍalabhedanaṃ kāryaṃ gamyate //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āyātā kamalāsanasya bhavanāddraṣṭuṃ trilokītalaṃ gīrvāṇeṣu dināni kānicidaho nītvā punaḥ kautukāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhrāntvā bhūvalaye mahākavikulopāsyā tavāsyāmbuje rājansaṃprati satyadhāmani girāṃ devī sukhaṃ vartate // JPran_16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atraikasyādheyasyānekādhārasaṃbandhātparyāyaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatra prathamacaraṇagatamadhikaraṇamārthaviśleṣāvadhikapañcamyā viśleṣasyopaśleṣāpekṣatvenaupaśleṣikādhikaraṇasyākṣepagamyatvāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "satyadhāmanīti śleṣabhittikābhedādhyavasānena mukhasya satyalokatāsiddhau sukhavartanasiddhiḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vidvaddainyatamastrimūrtirathavā vairīndravaṃśāṭavīdāvāgniḥ kimaho mahojjvalayaśaḥśītāṃśudugdhāmbudhiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kiṃ vānaṅgabhujaṃgadaṣṭavanitājīvāturevaṃ nṛṇāṃ keṣāmeṣa narādhipo na janayatyalpetarāḥ kalpanāḥ // JPran_17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra koṭīnāmāropāntaramūlakatvātparamparitasaṃśayaḥ sa cāhāryaḥ, mūlāropasya tathātvāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kavāviva kavinibaddhapramātrantare 'pyāhāryabuddheravirodhāt //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nadanti madadantinaḥ parilasanti vājivrajāḥ paṭhanti birudāvalīmahitamandire bandinaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "idaṃ tadavadhi prabho yadavadhi pravṛddhā na te yugāntadahanopamā nayanakoṇaśoṇadyutiḥ // JPran_18 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra mukhyārthasya rājaviṣayāyāḥ kaviraterupakārakasya yadaiva tava kopodayastadaiva tava ripūṇāṃ saṃpado bhasmasādbhaviṣyantīti vastuna upakārikā nayanakoṇaśoṇadyuteryugāntadahanopamā //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mayi tvadupamāvidhau vasumatīśa vācaṃyame na varṇayati māmayaṃ kaviriti krudhaṃ mā kṛthāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "carācaramidaṃ jagajjanayato vidhermānase padaṃ na vidadhetarāṃ tava samo dvitīyo naraḥ // JPran_19 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra tvatsamo 'nyo nāstīti pratyayādupamānaluptopamā vyaṅgyeti prāñcaḥ /",
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "sarvathaiva sāmyasyāpratiṣṭhānānneyamupamā /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "anyathā vyatirekasyāpi tattvāpatteḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "'ḍhuṃḍhollanto marisasi kaṃṭakakaliāiṃ keaivaṇāiṃ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "mālaikusumasaricchaṃ bhamara bhamaṃto ṇa pāvesi //"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "'; ityatra tu na prāpsyasītyuktvā kvacittvadagocare sthale bhaviṣyatīti pratīteḥ sādṛśyapratiṣṭhānālluptopamāstu /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmādasamālaṃkāra evāyamiti tu navyāḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhujabhramitapaṭṭiśoddalitadṛptadantāvalaṃ bhavantamarimaṇḍalakrathana paśyataḥ saṃgare /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "karālakuliśāhatisphuṭavibhinnavindhyācalo na kasya hṛdayaṃ jhaṭityadhiruroha jambhāhitaḥ // JPran_20 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra smaraṇālaṃkāraḥ paraṃtu lakṣyaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yamaḥ pratimahībhṛtāṃ hutavaho 'si tannīvṛtāṃ satāṃ khalu yudhiṣṭhiro dhanapatirdhanākāṅkṣiṇām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "gṛhaṃ śaraṇamicchatāṃ kuliśakoṭibhirnirmitaṃ tvameka iha bhūtale bahuvidho vidhātrā kṛtaḥ // JPran_21 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra kavinā yamatvādinā rūpeṇa rājño rūpavataḥ karaṇādrūpakeṇa vipakṣabhūpālādīnāmetasminnāgate yamatvādinā bhrānterapi saṃbhavādbhrāntimatā tairevānekairgrahītṛbhiranekairdharmairullekhanādullekhaviśeṣeṇa ca saha saṃkīrṇo 'pi saṃbandhiṣaṣṭhyantabhedaprayuktavarṇyānekavidhatvaka ullekhaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dvinetra iva vāsavo mitakaro vivasvāniva dvitīya iva candramāḥ śritavapurmanobhūriva /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "narākṛtirivāmbudhirgururiva kṣamāmāgato nuto nikhilabhūsurairjayati kāmarūpeśvaraḥ // JPran_22 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra rājagatānāṃ dvinetratvādīnāṃ vāsavāditādātmyavirodhināṃ virodhanivartanāya viṣayiṣu vāsavādiṣvāropeṇa sādhāraṇīkaraṇāttannimittakotprekṣā /\nsā ceha mālārūpā /\nna cātropamā śakyarūpaṇā /",
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "dvinetratvādyukterniṣprayojanakatvāpatteḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "na copamāniṣpādakaṃ teṣāṃ sādhāraṇyaṃ tadabhāve 'pi paramaiśvaryādibhiḥ pratīyamānaistasyā niṣpatteḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "asundaratvenopamāniṣpādakatayā kaveranabhipretatvācca /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "evaṃ dvitīyādīnāṃ candrādiṣvāropo 'pyupamāyāṃ satyāmanarthaka eva syāt //"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dīnavrāte dayārdrā nikhilaripukule nirdayā kiṃ ca mṛdvī kāvyālāpeṣu tarkaprativacanavidhau karkaśatvaṃ dadhānā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "lubdhā dharmeṣvalubdhā vasuni paravipaddarśane kāṃdiśīkā rājannājanmaramya sphurati bahuvidhā tāvakī cittavṛttiḥ // JPran_23 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra viṣayānekatvaprayuktacittavṛtteranekavidhatvamityullekhaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatra ca tadīyacittavṛttitvenaikatvādhyavasānaṃ tantram //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "devāḥ ke pūrvadevāḥ samiti mama naraḥ santi ke vā purastādeva jalpanti tāvatpratibhaṭapṛtanāvartinaḥ kṣatravīrāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yāvannāyāti rājannayanaviṣayatāmantakatrāsimūrtermugdhāriprāṇadugdhāśanamasṛṇarucistvatkṛpāṇo bhujaṃgaḥ // JPran_24 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra śuddhaparamparitarūpakam //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prācīsaṃdhyāsamudyanmahimadinamaṇermānamāṇikyakāntirjvālāmālā karālā kavalitajagataḥ krodhakālānalasya /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āśākāntāpadāmbhoruhatalavigalanmañjulākṣārasābhā sā bhāti kṣoṇiśobhākaraṇa tava dṛśoḥ saṃgare śoṇimaśrīḥ // JPran_25 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atrāpi tadeva paraṃtu mālātmakam //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tvāṃ sundarīnivahaniṣṭhuradhairyagarvanirvāsanaikarasikaṃ samare nirīkṣya /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kā vā ripukṣitibhṛtāṃ bata rājalakṣmīḥ svāmivratatvamapariskhalitaṃ babhāra // JPran_26 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra śatrūṇāṃ rājyalakṣmīstvāṃ prāpteti vivakṣito 'rthaḥ pātivratyaskhalitarūpeṇābhihita iti paryāyoktam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tacca rājyalakṣmyā nāyikātvasiddhyarthaṃ samāsoktimapekṣata iti sā tatra guṇaḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nāsatyayogo vacaneṣu kīrtau tathārjunaḥ karmaṇi cāpi dharmaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "citte jagatprāṇabhavo yadāste vaśaṃvadāste kimu pānḍuputrāḥ // JPran_27 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra pāṇḍuputreṣu viṣayeṣu rājavaśaṃvadatādātmyotprekṣāyāṃ rājāśritatvarūpo viṣayidharmaḥ śleṣeṇa viṣayāṇāṃ tadāśritānāṃ cāsatyābhāvaśuklagunapuṇyaparameśvarāṇāmabhedasaṃpādanadvārā viṣayasādhāraṇīkṛtaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "manthācalabhramaṇaveghavaśaṃvadā ye dugdhāmbudherudapatannaṇavaḥ sudhāyāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tairekatāmupagatairvividhauṣadhībhirdhātā sasarja tava deva dayādṛgantān // JPran_28 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra dṛganteṣu na kevalaṃ saṃjīvakatvādayo 'mṛtamātraguṇa eva kaverbubodhayiṣitā api tu nikhilajanavaśīkāratvādayo 'nye 'pīti sudhākaṇeṣvoṣadhīsaṃsargo 'tiśayārthamupāttaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "keśairvadhūnāmatha sarvakoṣaiḥ prāṇaiśca sākaṃ pratibhūpatīnām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tvayā raṇe niṣkaruṇena gāḍhaṃ cāpasya jīvā cakṛṣe javena // JPran_29 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra cāpakarṣaṇakāryāṇānāṃ keśākarṣaṇādīnāṃ paurvāparyaviparyayātmanātiśayenānuprāṇitā sahoktiḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mahendratulyaṃ kavayo bhavantaṃ vadantu kiṃ tāniha vārayāmaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhavānsahasraiḥ samupāsyamānaḥ kathaṃ samānastridaśādhipena // JPran_30 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra śleṣotthāpitatridaśatvasaṃkhyāmādāya vyatireka upāttobhayanimittakaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sa tu varṣatu vāri vāridastvamudārāśaya ratnavarṣaṇaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sa kuhūrajanīmalīmasastvamihāntarbahireva nirmalaḥ // JPran_31 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atrāpi sa eva paraṃ tu śleṣo 'trānutthāpako niṣidhyamānaṃ sāmyaṃ ca na śābdamiti viśeṣaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "katipayairniraṇāyi janādhipastadaparairudaṭaṅki dhanādhipaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ajani kevalameṣa girāṃ patirbhuvi madekamate kamateśvaraḥ // JPran_32 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra nāyaṃ rājā kiṃ tu girāṃ patirityākārāpahnutiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatra niṣedhabhāgaḥ paramatattvoktyā pūrvārdhena gamyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āropāṃśastūttarārdhena //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "makarapratimairmahābhaṭaiḥ kavibhī ratnanibhaiḥ samanvitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kavitāmṛtakīrticandrayostvamihorvīramaṇāsi bhājanam // JPran_33 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra rājño jaladhyupamāyāḥ śabdenābhidhāne 'pyaṅgopamābhirākṣepādekadeśavivartinyupamā tenottarārdha upamitasamāsa eva /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśeṣaṇasamāsavedyasya tādātmyasya prakṛte 'nupayogāt //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "puraḥ purastādaribhūpatīnāṃ bhavanti bhūvallabha bhasmaśeṣāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anantaraṃ te bhrukuṭīviṭaṅkātpatanti roṣānalavisphuliṅgāḥ // JPran_34 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra prayojakātiśayakṛtaḥ prayojyaśaighryātiśayo gamyaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryakāraṇapaurvāparyaviparyayarūpā ceyamatiśayoktiḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhuvanatritaye 'pi mānavaiḥ paripūrṇe vibudhaiśca dānavaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na bhaviṣyati nāsti nābhavannṛpa yaste bhajate tulāspadam // JPran_35 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atropamānaluptopameti prāñcaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asamākhyamalaṃkārāntaramiti tu vayam //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pīyūṣayūṣakalpāmalpāmapi te giraṃ nipītavatām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "toṣāya kalpate no yoṣādharabimbamadhurimodrekaḥ // JPran_36 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra saṃbandhe 'pyasaṃbandha ityatiśayoktibhedaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "upameyopamānaviśeṣaṇābhyāmalpatvodriktatvābhyāmalpayāpi saha bhūyānapi madhurimā sāmyaṃ kartuṃ yatrānīśastatra kiṃ vācyaṃ bhūyasyeti vailakṣaṇyātmā vyatirekaśca //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāsayati vyomasthā jagadakhilaṃ kumudinīrvikāsayati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kīrtistava dharaṇigatā sagarasutāyāsamaphalatāṃ nayate // JPran_37 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra vyajamānacandrikārūpakasaṃkīrṇa ullekhadhvaniḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāgyena saha ripūṇāmuttiṣṭhasi viṣṭarātkṛdhāviṣṭaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sahasaiva patasi teṣu kṣitiśāsana mṛtyunā sākam // JPran_38 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "'keśairvadhūnām'; ityatra karmaṇaḥ sahoktiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "iha tu karturiti viśeṣaḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tvayi pākaśāsanasame śāsati sakalaṃ vasuṃdharāvalayam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vipine vairivadhūnāṃ varṣanti vilocanāni ca dināni // JPran_39 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra varṣavadācarantītyācārakvibantena śleṣācchleṣamūlikā tulyayogitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ripukāminīvarṇanaviṣayatvenopamānopameyayordvayorapi prakṛtatvāt //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ahitāpakaraṇabheṣaja naranātha bhavānkarasthito yasya /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasya kuto 'hibhayaṃ syādakhilāmapi medinīṃ carataḥ // JPran_40 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra śleṣaniveditasya sarpabhayābhāvasyopapādakatayopāttasya rājani bheṣajatādātmyāropasyāpyupapādakatayā sthitaṃ rājasaṃbandhini dviṣadapakaraṇarūpe dharme śleṣaniveditasarpatāpakaraṇatādātmyamiti prathamacaraṇe śliṣṭaparamparitarūpakaṃ tṛtīye tu śleṣa eva /"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kuvalayalakṣmīṃ harate tava kīrtistatra kiṃ citram /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yasmānnidānamasyā lokanamasyāṅghripaṅkajo hi bhavān // JPran_41 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atrotpādakasamānaguṇatvādutpādyasyotpādakasaṃsargānurūpaḥ samālaṃkāraviśeṣaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śleṣaścāsminguṇaḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛṣṭaḥ sadasi cedugrāścandracandanacandrikāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "atha tvaṃ saṃgare saumyāḥ śeṣakālānalāsayaḥ // JPran_42 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra dṛṣṭaḥ sadasīti vākye tvamityasyāpakarṣaṇāt, atha tvaṃ saṃgara ityatra ca tvamityasyānuvartanādvākyayoḥ padavinimayātmālaṃkāraḥ pūrvārdhe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "uttarārdhe prakṛtānekadharmasaṃbandhāttulyayogitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "upameyasyotkṛṣṭaguṇatvasiddhaya upamānasya tadviruddhaguṇakalpanātmakenālaṃkārāntareṇa śabalitā //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "apāre khalu saṃsāre vidhinaiko 'rjunaḥ kṛtaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kīrtyā nirmalayā bhūpa tvayā sarve 'rjunāḥ kṛtāḥ // JPran_43 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iha śleṣamūlo vyatirekaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dadhīcibalikarṇeṣu himahemācalābdhiṣu /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "adātṛtvamadhairyaṃ ca dṛṣṭe bhavati bhāsate // JPran_44 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atrāpi saiva tathāvidhā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yathāsaṃkhyasaṃkarastu viśeṣaḥ //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śāsati tvayi he rājannakhaṇḍāvanimaṇḍalam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na manāgapi niścinte maṇḍale śatrumittrayoḥ // JPran_45 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "ihāpi tulyayogitā mittraśabdaśleṣotthāpitā vyājastutiśca //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhānuragniryamo vāyaṃ baliḥ karṇo 'thavā śibiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyarthino 'rthinaścetthaṃ tvayi yānti vikalpanām // JPran_46 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iha tu saṃśaya ullekhāliṅgitaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kamalāvāsakāsāraḥ kṣamādhṛtiphaṇīśvaraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhavānkuvalayasyendurānandayati mānavān // JPran_47 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iha śliṣṭaparamparitaṃ rūpakaṃ mālārūpam //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "gagane candrikāyante himāyante himācale /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pṛthivyāṃ sāgarāyante bhūpāla tava kīrtayaḥ // JPran_48 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iha mālopamāliṅgita ullekhaḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mṛgatāṃ harayanmadhye vṛkṣatāṃ ca paṭīrayan /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nakṣatratāṃ mahīpānāṃ tvamindavasi bhūpate // JPran_49 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iha śuddhaparamparitā mālārūpopamā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ācārakvibantācchatari rūpāṇi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tiṅi caikam [indavasi] //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "madhye sudhāsamdurasya sitāmayagṛhodare /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pūrṇenduviṣṭare deva sthātuṃ yogyāstavoktayaḥ // JPran_50 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "atra trayāṇāṃ sudhāsamudrādīnāṃ viśeṣaṇaviśeṣyabhāvena madhurimaṇi parāṃ kāṣṭhāmadhirūḍhe tvadīyoktipadasaṃbandho yukto na tu viśakalita ityatiśāyakāsadviśeṣaṇanibandhanaḥ prauḍhoktiḥ //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "amṛtalaharīcandrajyotsnāramāvadanāmbujānyadharitavato nirmaryādaprasādamahāmbudheḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ubhayavadayaṃ deva tvattaḥ kathaṃ paramolbaṇaḥ pralayadahanajvālājālākulo mahasāṃ gaṇaḥ // JPran_51 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra kāraṇaguṇaviruddhasvaguṇasya kāryasyotpatterviṣamālaṃkāraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "amṛtalaharyādīnāṃ trayāṇāmadharīkaraṇātmanā vyatirekeṇa saṃjīvakatvaparamaśītalatvaviśuddhatvasaundaryāṇāmatiśayo gamyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "evameṣu padyeṣu saṃbhavanto 'pyanye 'laṃkārāḥ sphuṭatvānna vivecitāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sahṛdayānāṃ prītyāvaśyakaṃ kiṃcidvyākhyātamanyattu tairevollāsanīyamityalaṃ pallavitena //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tailaṅgānvayamaṅgalālayamahālakṣmīdayālalitaḥ śrīmatperamabhaṭṭasūnuraniśaṃ vidvallalāṭaṃtapaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃtuṣṭaḥ kamatādhipasya kavitāmākarṇya tadvarṇanaṃ śrīmatpaṇḍitarājapaṇḍitajagannātho vyadhāsīdidam // JPran_52 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dordaṇḍadvayakuṇḍalīkṛtalasatkodaṇḍācaṇḍāśugadhvastoddaṇḍavipakṣamaṇḍalamiha tvāṃ vīkṣya madhyeraṇam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "valgadgāṇḍivamuktakāṇḍavalayajvālāvalītāṇḍavabhraśyatkhāṇḍavaruṣṭapāṇḍavamaho ko na kṣitīśaḥ smaret // JPran_53 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra smaraṇālaṃkāraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāva iti tu na bhrāmitavyam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sādṛśyamūlakatvātanmūlakatvābhyāṃ bhāvatvālaṃkāratvayoriha vyavasthiteḥ śabdavedyatvācca //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iti paṇḍitarājaśrījagannāthaviracitaṃ prāṇābharaṇaṃ tatkṛtayaiva ṭippaṇyā sametaṃ samāptam /"
    }
  ]
}