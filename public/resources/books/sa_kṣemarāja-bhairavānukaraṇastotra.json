  "body": [
    {
      "tag": "div",
      "attributes": {},
      "children": [
        {
          "tag": "p",
          "attributes": {},
          "text": "oṃ namaś cidbhairavāya |"
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_1"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "citibhūmisamāśrayaṇāś citibhūtiṃ śrayata mā mudhā bhramata |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "prathayann iti bhaktimatāṃ jayati śivo 'nugrahapravaṇaḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_2"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "cidbhairavam eva paraṃ paramāmṛtarūpam ekam atidīptam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ullasitakaraṇacakragrastasamastaṃ śivaṃ vande ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_3"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "stutyaḥ stotā stutir iti yad api vibhinnaṃ na kiṃcid astīha |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "mṛṣati yathā yad rūpaṃ cidrūpatayā tathā bhavaty etat ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_4"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "vigalitasarvavibhedaṃ sarvavibhedātma cidghanānandam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "yat tava tattvaṃ bhagavaṃs tasyānukṛtiṃ tvadākṛtiṃ vande ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_5"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "rekhāpuruṣaḥ puruṣaṃ varṇalipir varṇasaṃcayaṃ yadvat |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tadvat vicitrarūpaṃ tvām ākāro vyanakti paramātman ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_6"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "tatrāvikalpam eva tvāṃ cidrūpaṃ samāviśya |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "vimṛśāmaḥ kim api manāṅ na hi tattvaṃ kalpanāviṣayaḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_7"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "svātmaprabhāṃ vidhāya vāgdevīṃ viṣayakusumakḷptārcaḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "prathamonmeṣaṃ gurum atha vanditvā hṛdayam āviśya ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_8"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "staumi vimṛśan maheśvaram ātmānaṃ svaṃ cidekaghanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "srāvitapāśakadambakam etasmin dehadevagṛhe ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_9"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "cityānandecchāvitkaraṇākhyāḥ śaktayo jayanti vibho |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "sūkṣmasthūlabhidāptā vaktratvaṃ brahmamuṇḍabhidhā ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_10"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "sarvajñatādiviṣayaṃ hṛdayādikam aṅgaṣaṭkaṃ te |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "sakalaṃ sadāśivāntaṃ pretātmatayā tavāsanaṃ svāmin ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_11"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "paramaprakāśavapuṣo vimarśaśaktiḥ prabho parā devī |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "asyā eva prasaraḥ sarvā brāhmyādikā devyaḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_12"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "dahanajalāmṛtalekhāviṣādi bibhrat tvam ādiśasy etat |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "viśvaṃ viruddham api saccidrūpe mayi virudhyate naitat ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_13"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "saṃvitsūtranilīno niḥsāro mātṛnivaho 'yam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "karaṇamuṇḍamālabhārin  darśayasītīva naḥ svāmin ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_14"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "paśyata paśyata pāśā viṣamās ta ime bahiṣkṛtāḥ śaśvat |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ity antradhāraṇamiṣat kathayaṃs tvaṃ naḥ samābhāsi ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_15"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "māyāpaṭaḥ sarāgo vyomni pare 'smin sa eṣa nikṣiptaḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "iti gajacarma saraktaṃ mūrdhni vahan kathayasīśa ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_16"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "antaḥśaktikṛpāṇīm vyanakṣi saṃsṛtivibhedinīm asinā |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "nijaśaktimahimasvīkṛtasamastaviśvā hi vīravarāḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_17"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhavabhayahantā so 'ham sānāthye 'vasthito 'smi mā bhaiṣṭa |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ity āsphoṭitakheṭakadarśanato diśasi naḥ svāmin ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_18"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "nijaśaktipāśavalitaś cidvapuṣā kalyate mayā kālaḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "iti pāśadhāraṇavaśāt prathayasi naḥ kālakālas tvam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_19"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhedamayam akhilam etan nijaśaktyaivākṣipāmi saṃhartum |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ity aṅkuśadhāraṇataḥ sphuṭayati parabhairavo 'smākam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_20"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "kodaṇḍārūdhaśarapradarśanād brahmaviṣṇurudreśān |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "sasadākhyaśivāñ chaktyā kāraṇahariṇaṃ bhinatsi mṛgavat tvam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_21"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "varadena pāṇinā tvaṃ viśvavibhūtipradatvam abhinayasi |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "na khalu paratattvaniṣṭho vibhavamalaiḥ spṛśyate jātu ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_22"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "abhayena ca bhavabhayāni unmūlayatā prakāśyate satatam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "viśvānugrahakāraṇasvabhāvatā tava karatalena ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_23"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "akhyātirūpam asamaṃ māyāyāḥ śakalitaṃ śiro 'traitat |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "parabodhamayena mayā muṇḍaṃ bibhrat prakāśayasy evam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_24"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "niḥśeṣahṛtasārā mayy eva jagatsthitiś cidekaghane |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "iti khaṭvāṅgakaraṅkodvahanacchalato dadāsy ājñām ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_25"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "vīṇāghaṇtāḍamarūn uḍḍāmara vādayann idaṃ diśasi |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "vṛttitribhedabhinnaṃ nādaṃ śṛṇvan sthito 'smīti ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_26"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "kiṃ ca parādikaśaktitritayārāsundarām mahāśaktim |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "devānukaroṣi bahis triśūladhāraṇamiṣeṇa nityaṃ tvam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_27"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "icchādikanijaśaktiprakāśitādhaḥsthagocaratritayām |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "svām eva parāṃ śaktiṃ vajramayīṃ vahasi ṣaḍarāṃ tvam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_28"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "jagad akhilaṃ macchaktyā damitaṃ sarvavyāvasthitīr dhatte |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "iti daṇḍadhāraṇavaśād vyanakṣi cidbhairavāsmākam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_29"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "mudgaraparaśū bibhrad baindavanādānukārarūpau tvam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "bhedavibhedanaśakalanaparatvam īśāna nirdiśasi ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_30"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "darśanabhedāt svāminn anyānyām āhṛtiṃ dadhānas tvam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "bhairavaparabodhamayaṃ prathayasi nijadhāma sarvatra ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_31"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "śāktāṇḍakhaṇḍamadhye viśvarasāḥ sarvam ahaṃ pibāmi sadā |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "vyañjayasi narakapālagarudhiramiṣād etad iva me 'ntaḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_32"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "abhimānahṛdayabandhānutkhātau tāv imau saśikhau |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "mūrtāv iva muṇdābjau prathayan dhatse mahākālaḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_33"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "akhilajagadbījabhuvaṃ prathamasthām anukaroṣi bodhanidhe |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "karatalagabījapūrakadarśanalīlāyitād deva ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_34"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhairavarūpo 'py asi yat tatrāpi kare 'kṣamālikāṃ dhatse |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tat sarvākṣavikāsād viśvaṃ parivartayasy antaḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_35"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "asthikaramuṇḍamālābhasmābharaṇo nṛraktaruciḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "viśvaṃ brahmamayatvāc chuddham itīśaḥ samādiśasi ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_36"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "svātmārāmamayānāṃ viṣamā api viṣayabudbudhāḥ kim amī |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "smaradahanam alikanayanam prathayan paramāgnirūpam ādiśasi ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_37"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "antarnibaddhalakṣyaḥ pravikāsikaraṇo 'pi yat sadā bhavasi |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tat prathayasi sarvadaśaṃ gatamohakalaṅkam ātmānam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_38"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "deva digambaratā te vadati nirāvaraṇam ātmano rūpam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "paramabrahmamayārkān māyāvaraṇam kathaṃ vṛṇuyāt ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_39"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "saṃhartā sarveṣām arkādīnām ahaṃ tamobhūmiḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "nirmalaśāradagaganaprabheṇa vapuṣeti darśayasi ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_40"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "paramabrahmamayas tvaṃ deva yadāśrayasi bhairavākāram |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tat prathayasi tattvajñaḥ saty api bhede vimukta iti ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_41"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "vividhaśmaśānamālāvetālāvṛtaḥ prakāśayase |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "saṃvittiviṣayakalanāmadhyaś citibhairavo 'smīti ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_42"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "tvayy evāmṛtalekhā paraśaktimayī parapramātātaḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "nātha tvam eva jagatām jīvanam iti bhāsi no hṛdaye ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_43"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "tava śabdarāśivapuṣaḥ pratyaṣṭakam eva bāhyaparivāraḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "parasūkṣmādivibhedād bhairava lokeśvarapramukhāḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_44"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "mudrayati nikhilam etan niḥśeṣasvasvarūpasaṃkramaṇāt |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "modayati bhaktavargaṃ gatimudaṃ pūrṇatāprathanāt ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_45"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "mocayati pāśajālaṃ drāvayati vibhedam īśa te mūrtiḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "mudrayati vividhasaṃviddraviṇaṃ mudrā tataḥ kathitā ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_46"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "jayati paratattvabhūmipravare",
              "children": [
                {
                  "tag": "sic",
                  "attributes": {},
                  "text": "nipāyabhūtaiṣa"
                },
                {
                  "tag": "text",
                  "text": "|"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "sphuṭam adhivasatu satattvaprathayā hṛdayasya meyamalam ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_47"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "pratikalam udeti saṃvid yā kila tatra sphuraty adho viśvam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "śivaśaktivibhavarūpaṃ jānanti maheśa yāgasthāḥ ||"
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "KsBhAS_48"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "itthaṃ sphurasi sadā me cidbhairava sarvabhūtātma |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tad yāgādijapāntaṃ sampannam ayatnataḥ sarvam ||"
            }
          ]
        },
        {
          "tag": "trailer",
          "attributes": {},
          "text": "iti śrīmahāmāheśvarācāryaśrīkṣemarājaviracitaṃ bhairavānukaraṇastotraṃ samāptam ||"
        }
      ]
    }
  ]
}