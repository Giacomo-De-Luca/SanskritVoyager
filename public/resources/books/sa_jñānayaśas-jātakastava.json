  "body": [
    {
      "tag": "note",
      "attributes": {},
      "text": "metre: Śārdūlavikrīḍita"
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_1"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "kvedaṃ dāśabalaṃ guṇair aśabalaṃ caryāvibhutvaṃ vibho"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "janmasyandanikāvivartanajaḍaṃ cetaḥ kva cedaṃ mama"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "yenāvandhyaphalodayās tvayi bhavanty abhyāhitāḥ pāṃsavas"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tena stutyanubandhinā mukharatām ālambate bhāratī"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_2"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "cittālambanalaṅghinaḥ śamadṛśām āveṇikāḥ sadguṇā"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "vācāṃ gocaratāṃ tu yānti kalayā sthūlā hi vo mādṛśām"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "ratnāny ambunidher upāmbupulineṣv āvir bhavanty arthinām"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "antarbhinnataraṅgadyotarucakajyotsnāni gṛhṇāti kaḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_3"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "yā vo bodhipathānuyānamahatām utsāhinī karmaṇām"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "āsīl lokahitodaye jina kṛpāviṣyandinī santatiḥ"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "sānekāpacitaḥ punāti bhagavan saṃkīrtyamānādhunā"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "jñeyābhogavisāriṇo gatamalāḥ prāg eva bodhau guṇāḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_4"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "uddāmadviradasya rāgavaśino vṛttiṃ puronmādinīm"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "ālokyāvyativartinā praṇihitā bodhau dhiyo yat tvayā"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "vinyasyanta ivāmarās tvayi ciraṃ dharmābhiṣekaśriyaṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tenākampitakeśarāntam asṛjan puṣpopahāraṃ divaḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_5"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "kṣudduḥkhaglapitodarīṃ giridarīrandhrāntarālasthitāṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "dṛṣtvā śāvakaghālanaikarabhasāṃ vyāghrīṃ dayāghrātadhīḥ"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "yat tvaṃ valkalinīṃ jaṭājanavatīṃ mūrtiṃ samutsṛṣṭavāṃs"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "trāṇaṃ tena bhavān bhavādrivivaravyāvartināṃ dehinām"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_6"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "yat prāleyasitopalādribahalāṃ mātaṅgatāṃ bibhratā"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "kāntārāntaravartināṃ tanubhṛtām ucchittaye vyāpadām"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "śailendraṃ pṛthulopalaṃ calayatā muktas tvayātmā taṭān"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "manye matsariṇāṃ nitāntamalinās tena kṣatā vṛttayaḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_7"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "yal lokāmbaracandracandravapuṣā candraprabhatve tvayā"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "kalmāṣāntaśikhaṃ śikhāmaṇirucā dattaṃ dvijāyārthine"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "nālāl lūnam ivāravindam asinā vyutkṛttamūlaṃ śiras"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tenaiṣāṃ jagatāṃ gato 'si gurutām usṇīṣavan mūrdhani"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_8"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "bālān bālasarojakomalatanūn yat putrakān arthine"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "paryastākṣacalacchikhaṇḍakabhṛto dārāṃs ca bhāvanugān"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "prādās tvaṃ jina tena nūnam ahhavan mārāṅganānāṃ mukhaṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "saṃkocāñcitapakṣmalocanadalaṃ lajjālasāmantharam"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_9"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "śayyāsvapnavicāriṇā vanabhuvāṃ bhūtvā purā pakṣiṇā"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "bhinnāśeṣakale jagaty avikale jyāyaḥpathāśaṃsinā"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "vistāraṃ gamitaṃ vrataṃ vratabhṛtāṃ yan nātha kāpiñjalaṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tenājñānarujāṃ kirīṭakiraṇacchāyās padaṃ vo gatāḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_10"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "yat parṇāmbuphalāśinīṃ śaśatanuṃ tvaṃ nātha bibhrat purā"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "tatkālātithaye kṛpāparicayād ātithyam āvedayan"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "ārūḍhaḥ sphuṭavisphuliṅgam analaṃ tenādhunāpi kṣitāv"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "indor maṇḍalalāñchanaṃ śaśamayaṃ yuṣmadyaśo laksyate"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_11"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "yat kopāruṇadṛṣṭinā kalabhuvā kṛtvā bhruvor bhaṅginīṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "lekhām ullasitāsinā vidalitapratyaṅgam āśaṅkinā"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "tvām āsīt kṣaṇam apy avikṣatadhiyaṃ tyaktukṣamā na kṣamā"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "manye tena niraṅkuśāḥ smaraśarāḥ puṅkheṣu bhaṅgaṃ gatāḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_12"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "yad dveṣī kariṇā cirātatadhanur dantais tvayābhyarcitaḥ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "ṣaḍdantena satā sitonnatabṛhajjīmūtapuñjatviṣā"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "kośākarṣaṇavāntabhūrirudhirakṣodāruṇais tena vaḥ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "pātāle 'pi bhujaṃgamaiḥ ślathaphaṇair adyāpi daṃṣtrārcyate"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_13"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "saṃrambhāt phaṇināṃ phaṇāhaticalād bhīmormimālād apāṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "patyur yan makaracchaṭāvilulitāt paryastanaukā narāḥ"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "premṇā kāyamahāplavena bhavatā tīrāntam āpāditās"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tatkarmātiśayena tena nikhilā lokāḥ kalatrīkrtāḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_14"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "śailān maṇḍalino vilaṅghya viṣayāṃs toyāvalīmekhalān"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "yac cintāmaṇinā narendra badaradvīpāntarālodarāt"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "labdhvā pūrṇaphalās tvayā praṇayinām āśāvabandhāḥ kṛtās"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tvattas tena samudbhavanti viduṣāṃ saddharmaratnāṅkurāḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_15"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "sāndrābhrāntavitānanīlavapuṣām ojomuṣāṃ rakṣasāṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "khaḍgotkṛttadalair vipāṭitaśirārandhrasravallohitaiḥ"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "yan māṃsair akṛthāḥ kṣudhāvyupaśamaṃ svair avyapetoṣmabhir"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "yogā janmarudho bhavodbhavatṛṣām ucchedinas tena vaḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_16"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "śyenottrāsaviśuṣkakampitagalasyotsaṅgasaṃsarpiṇo"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "yan niḥsaṅgaratā śivāya śibinā rājñā satā pakṣiṇaḥ"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "raktodgārakalaṅkitātulatulām āropitā te tanur"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "māras tena dukūlapakṣmataralaṃ nītas tulālāghavam"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_17"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "lakṣmīm apy avadhūya vāñchitaphalām ākhaṇḍam ādhvaṃsinīṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "yat tiryakpratisandhibandham adhamaṃ kṛtvā bhavāñ janmanām"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "cakre marmabhidāṃ rujāṃ svapiśitaiḥ prābandhakīnāṃ śamaṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tena kleśaviṣacchidāṃ tava dhiyāṃ avyāhatāḥ śaktayaḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_18"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "yat sandoham iva śriyāḥ phalam iva procchrāyi puṇyaujasāṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "digdhaṃ raktakaṇaiḥ sphuratkuvalayacchāyābhinīlāyitam"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "utpāṭyākṣatalocanadvayam adād viprāya niścakṣuṣe"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "tenāndhasya tamasvato 'sya jagatas tuṅgaikadīpo bhavān"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_19"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "ity ātmārthanibandhinedam ṛṣivākyālaṃkṛtaṃ darśitaṃ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "leśasyāpi na kīrtanaṃ jina kṛtaṃ tvadvarṇarāśer mama"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "tārābdhānavatāṃ jalaṃ jalapurām ākarṣatā vāridher"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "velānteṣv iva rodhasāṃ jalalavacchedo hi saṃlakṣyate"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "Js_20"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "a"
              },
              "text": "tvaccaryātiśayāṃśaleśaracanād yaḥ śreyasām udbhavaḥ"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "b"
              },
              "text": "sambhāro vipulaprasādavikasadbhaktyācitāṃśasya me"
            }
          ]
        },
        {
          "tag": "l",
          "attributes": {},
          "children": [
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "c"
              },
              "text": "tenātyantaparopakāranirataḥ svārthe 'pi mandādaro"
            },
            {
              "tag": "seg",
              "attributes": {
                "type": "pāda",
                "n": "d"
              },
              "text": "bhūyāt sarvavidāṃ parārthaviduṣāṃ yānaikatāno janaḥ"
            },
            {
              "tag": "text",
              "text": "//"
            }
          ]
        }
      ]
    }
  ]
}