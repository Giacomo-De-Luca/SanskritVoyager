  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "vaiśeṣikasūtram"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "athāto dharmaṃ vyākhyāsyāmaḥ | KVs_1,1.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yato 'bhyudayaniḥśreyasasiddhiḥ sa dharmaḥ | KVs_1,1.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadvacanād āmnāyaprāmāṇyam (U,V,BhV āmnāyasya prāmāṇyam) | KVs_1,1.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pṛthivy āpas tejo vāyur ākāśaṃ kālo dig ātmā mana iti dravyāṇi | KVs_1,1.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "rūparasagandhasparśāḥ saṃkhyāḥ parimāṇāni pṛthaktvaṃ saṃyogavibhāgau paratvāparatve buddhayaḥ sukhaduḥkhe icchādveṣau prayatnaś ca guṇāḥ | KVs_1,1.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "utkṣepaṇam avakṣepaṇam ākuñcanaṃ prasāraṇaṃ gamanam iti karmāṇi | KVs_1,1.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sad anityaṃ dravyavat kāryaṃ kāraṇaṃ sāmānyaviśeṣavad iti dravyaguṇakarmaṇām aviśeṣaḥ | KVs_1,1.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyāṇi dravyāntaram ārabhante | KVs_1,1.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇāś ca guṇāntaram | KVs_1,1.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "karma karmasādhyaṃ na vidyate | KVs_1,1.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāryāvirodhi dravyaṃ kāraṇāvirodhi ca | KVs_1,1.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ubhaytathā guṇaḥ | KVs_1,1.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāryavirodhi karma | KVs_1,1.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kriyāvad guṇavat samavāyikāraṇam iti dravyalakṣaṇam | KVs_1,1.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyāśrayy aguṇavān saṃyogavibhāgeṣv akāraṇam anapekṣa iti guṇalakṣaṇam | KVs_1,1.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekadravyam aguṇaṃ saṃyogavibhāgeṣv anapekṣaṃ kāraṇam iti karmalakṣaṇam | KVs_1,1.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyaguṇakarmaṇāṃ dravyaṃ kāraṇaṃ sāmānyam | KVs_1,1.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā guṇaḥ | KVs_1,1.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogavibhāgānāṃ karma | KVs_1,1.19 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na dravyāṇāṃ vyatirekāt | KVs_1,1.20 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇavaidharmyān na karmaṇāṃ | KVs_1,1.21 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyāṇāṃ dravyaṃ kāryaṃ sāmānyam | KVs_1,1.22 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dvitvaprabhṛtayaś ca saṃkhyāḥ pṛthaktvaṃ saṃyogavibhāgāś ca | KVs_1,1.23 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asamavāyāt sāmānyaṃ karma kāryaṃ na vidyate | KVs_1,1.24 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogānāṃ dravyam | KVs_1,1.25 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "rūpānāṃ rūpam | KVs_1,1.26 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "gurutvaprayatnasaṃyogānām utkṣepaṇam | KVs_1,1.27 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogavibhāgāḥ karmaṇām | KVs_1,1.28 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇasāmānye dravyakarmaṇāṃ karmākāraṇam uktam | KVs_1,1.29 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇābhāvāt kāryābhāvaḥ | KVs_1,2.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na tu kāryābhavāt kāraṇābhāvaḥ | KVs_1,2.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyaṃ viśeṣa iti buddhyapekṣam | KVs_1,2.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bhāvaḥ sāmānyam eva | KVs_1,2.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyatvaṃ guṇatvaṃ karmatvaṃ ca sāmānyāni viśeṣāś ca | KVs_1,2.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anyatrāntyebhyo viśeṣebhyaḥ | KVs_1,2.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sad iti yato dravyaguṇakarmasu | KVs_1,2.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyaguṇakarmabhyo 'rthāntaraṃ sattā | KVs_1,2.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekadravyavattvān na dravyam | KVs_1,2.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇakarmasu ca bhāvān na karma na guṇaḥ | KVs_1,2.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyaviśeṣābhāvāc ca | KVs_1,2.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekadravyavattvena dravyatvam uktam | KVs_1,2.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyaviśeṣābhāvena ca | KVs_1,2.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇe bhāvād guṇatvam uktam | KVs_1,2.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyaviśeṣābhāvāc ca | KVs_1,2.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "karmaṇi bhāvāt karmatvam uktam | KVs_1,2.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyaviśeṣābhāvāc ca | KVs_1,2.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "salliṅgāviśeṣād viśeṣaliṅgābhāvāc caiko bhāvaḥ | KVs_1,2.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "rūparasagandhasparśavatī pṛthivī | KVs_2,1.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "rūparasasparśavatya āpo dravāḥ snigdhāś ca | KVs_2,1.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tejo rūpasparśavat | KVs_2,1.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vāyuḥ sparśavān | KVs_2,1.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ta ākāśe na vidyante | KVs_2,1.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sarpirjatumadhūcchiṣṭānāṃ pārthivānām agnisaṃyogād dravatādbhiḥ sāmānyam | KVs_2,1.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "trapusīsaloharajatasuvarṇānāṃ taijasānām agnisaṃyogād dravatādbhiḥ sāmānyam | KVs_2,1.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viṣāṇī kakudmān prāntevāladhiḥ sāsnāvān iti gotve dṛṣṭaṃ liṅgam | KVs_2,1.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sparśaś ca | KVs_2,1.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na ca dṛṣṭānāṃ sparśa ity adṛṣṭaliṅgo vāyuḥ | KVs_2,1.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "adravyavattvād dravyam (C,V,BhV U adravyavattvena) | KVs_2,1.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kriyāvattvād guṇavattvāc ca (V omits) | KVs_2,1.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "adravyavattvena nityatvam uktam (C,BhV) (V adravyatvenānityam, U adravyatvena) | KVs_2,1.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vāyor vāyusaṃmūrcchanaṃ nānātve liṅgam | KVs_2,1.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vāyor iti sati snnikarṣe pratyakṣābhāvād dṛṣṭaṃ liṅgaṃ na vidyate | KVs_2,1.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyatodṛṣṭāc cāviśeṣaḥ | KVs_2,1.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasmād āgamikam | KVs_2,1.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃjñākarma tv asmadviśiṣtānāṃ liṅgam | KVs_2,1.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pratyakṣapūrvakatvāt saṃjñākarmaṇaḥ | KVs_2,1.19 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "niṣkramaṇaṃ praveśanam ity ākāśasya liṅgam | KVs_2,1.20 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad aliṅgam ekadravyavattvāt karmaṇaḥ | KVs_2,1.21 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇāntarānukṜptivaidharmyāc ca | KVs_2,1.22 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogād abhāvaḥ karmaṇaḥ | KVs_2,1.23 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇaguṇapūrvaḥ kārye guṇo dṛṣṭaḥ kāryāntarāprādurbhāvāc ca śabdaḥ sparśavatām aguṇaḥ | KVs_2,1.24 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "paratra samavāyāt pratyakṣatvāc ca nātmaguṇo na manoguṇaḥ | KVs_2,1.25 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "liṅgam ākāśasya | KVs_2,1.26 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyatvanityatve vāyunā vyākhyāte | KVs_2,1.27 |"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "28 tattvaṃ bhāvena | KVs_2,1 |",
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "puṣpavastrayoḥ sati sannikarṣe gandhāntarāprādurbhāvo vastre gandhābhāvaliṅgam | KVs_2,2.1 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "etenāpsūṣṇatā vyākhyātā | KVs_2,2.2 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vyavasthitaḥ pṛthivyāṃ gandhaḥ | KVs_2,2.3 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tejasy uṣṇatā | KVs_2,2.4 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "apsu śītatā | KVs_2,2.5 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aparasmin paraṃ yugapad ayugapac ciraṃ kṣipram iti kālaliṅgāni | KVs_2,2.6 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dravyatvanityatve vāyunā vyākhyāte | KVs_2,2.7 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvaṃ bhāvena | KVs_2,2.8 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryaviśeṣeṇa nānātvam | KVs_2,2.9 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nityeṣv abhāvād anityeṣu bhāvāt | KVs_2,2.10 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāraṇe kālākhyā | KVs_2,2.11 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ita idam iti yatas tad diśo liṅgam | KVs_2,2.12 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dravyatvanityatve vāyunā vyākhyāte | KVs_2,2.13 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvaṃ bhāvena | KVs_2,2.14 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryaviśeṣeṇa nānātvam | KVs_2,2.15 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ādityasaṃyogād bhūtapūrvād bhaviṣyato bhūtāc ca prācī | KVs_2,2.16 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathā dakṣiṇā pratīcy udīci ca | KVs_2,2.17 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "etena digantarāṇi vyākhyātāni | KVs_2,2.18 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānyapratyakṣād viśeṣāpratyakṣād viśeṣasmṛteś ca saṃśayaḥ | KVs_2,2.19 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛṣṭam adṛṣṭām | KVs_2,2.20 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛṣṭaṃ ca dṛṣṭāvat | KVs_2,2.21 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛṣṭaṃ yathādṛṣṭām ayathādṛṣṭām ubhayathādṛṣṭatvāt | KVs_2,2.22 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vidyāvidyātaś ca saṃśayaḥ | KVs_2,2.23 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śrotragrahaṇo yo 'rthaḥ sa śabdaḥ | KVs_2,2.24 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmin dravyaṃ karma guṇa iti saṃśayaḥ | KVs_2,2.25 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tulyajātīyeṣv arthāntarabhūteṣu ca viśeṣasyobhayathā dṛṣṭatvāt | KVs_2,2.26 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ekadravyavattvān na dravyam | KVs_2,2.27 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "acākṣuṣatvān na karma | KVs_2,2.28 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "guṇasya sato 'pavargaḥ karmabhiḥ sādharmyam | KVs_2,2.29 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sato liṅgābhāvāt | KVs_2,2.30 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nityavaidharmyāt | KVs_2,2.31 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryatvāt | KVs_2,2.32 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "abhāvāt | KVs_2,2.33 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāraṇo vikārāt | KVs_2,2.34 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "doṣāt | KVs_2,2.35 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃyogād vibhāgāc chabdāc ca śabdaniṣpatteḥ | KVs_2,2.36 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "liṅgāc cānityaḥ | KVs_2,2.37 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dvayos tu pravṛttyor abhāvāt | KVs_2,2.38 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃkhyābhāvāt | KVs_2,2.39 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prathamāśabdāt | KVs_2,2.40 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sampratipattibhāvāc ca | KVs_2,2.41 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sandigdhāḥ satibahutve | KVs_2,2.42 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃkhyābhāvaḥ sāmānyataḥ | KVs_2,2.43 |"
        }
      ]
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prasiddhā indriyārthāḥ | KVs_3,1.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "indriyārthaprasiddhir indriyārthebhyo 'rthāntaratve hetuḥ | KVs_3,1.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "so 'napadeśaḥ | KVs_3,1.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇājñānāt | KVs_3,1.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāryājñānāt | KVs_3,1.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ajñānāc ca | KVs_3,1.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anya eva hetur ity anapadeśaḥ | KVs_3,1.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogi samavāyy ekārthasamavāyi virodhi ca, kāryaṃ kāryāntarasya, kāraṇaṃ kāraṇāntarasya, virodhy abhūtaṃ bhūtasya, bhūtam abhūtasya, abhūtam abhūtasya, bhūtaṃ bhūtasya | KVs_3,1.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prasiddhapūrvakatvād apadeśasya | KVs_3,1.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aprasiddho 'napadeśaḥ | KVs_3,1.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asan sandigdhaś cānapadeśaḥ | KVs_3,1.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viṣāṇī tasmād aśvo viṣāṇī tasmād gaur iti ca | KVs_3,1.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmendriyamano'rthasannikarṣād yan niṣpadyate tad anyat | KVs_3,1.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pravṛttinivṛttī ca pratyagātmani dṛṣṭe paratra liṅgam | KVs_3,1.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmendriyārthasannikarṣe jñānasyābhāvo bhāvaś ca manaso liṅgam | KVs_3,2.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyatvanityatve vāyunā vyākhyāte | KVs_3,2.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prayatnāyaugapadyāj jñānāyaugapadyāc caikaṃ manaḥ | KVs_3,2.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prāṇāpānanimeṣonmeṣajīvanamanogatīndriyāntaravikārāḥ sukhaduḥkhe icchādveṣau prayatnaś cety ātmaliṅgāni | KVs_3,2.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyatvanityatve vāyunā vyākhyāte | KVs_3,2.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yajñadatta iti sati sannikarṣe pratyakṣābhāvād dṛṣṭaṃ liṅgaṃ na vidyate | KVs_3,2.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyatodṛṣṭāc cāviśeṣaḥ | KVs_3,2.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasmād āgamikam | KVs_3,2.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aham iti śabdavyatirekān nāgamikam | KVs_3,2.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yadi ca dṛṣṭapratyakṣo 'haṃ devadatto 'haṃ yajñadatta iti | KVs_3,2.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "devadatto gacchati viṣṇumitro gacchatīti copacārāc charīrapratyakṣaḥ | KVs_3,2.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sandigdhas tūpacāraḥ | KVs_3,2.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aham iti pratyagātmani bhāvāt paratrābhāvād arthāntarapratyakṣaḥ | KVs_3,2.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na tu śarīraviśeṣād yajñadattaviṣṇumitrayor jñānaviśeṣaḥ | KVs_3,2.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sukhaduḥkhajñānaniṣpattyaviśeṣād aikātmyam | KVs_3,2.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nānā vyavasthātaḥ | KVs_3,2.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śāstrasāmarthyāc ca | KVs_3,2.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sad akāraṇavat tan nityam | KVs_4,1.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasya kāryaṃ liṅgam | KVs_4,1.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇabhāvād dhi kāryabhāvaḥ | KVs_4,1.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anityam iti ca viśeṣapratiṣedhabhāvaḥ | KVs_4,1.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "avidyā ca | KVs_4,1.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mahaty anekadravyavattvād rūpāc copalabdhiḥ | KVs_4,1.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "adravyavattvāt paramāṇāv anupalabdhiḥ | KVs_4,1.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "rūpasaṃskārābhāvād vāyāv anupalabdhiḥ | KVs_4,1.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anekadravyeṇa dravyeṇa samavāyād rūpaviśeṣāc copalabdhiḥ | KVs_4,1.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena rasagandhasparśeṣu jñānaṃ vyākhyātam | KVs_4,1.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadabhāvād avyabhicāraḥ | KVs_4,1.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃkhyāḥ parimāṇāni pṛthaktvaṃ saṃyogavibhāgau paratvāparatve karma ca rūpidravyasamavāyāc cākṣuṣāṇi | KVs_4,1.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "arūpiṣv acākṣuṣatvāt | KVs_4,1.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena guṇatve bhāve ca sarvendriyajñānaṃ vyākhyātam | KVs_4,1.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pratyakṣāpratyakṣāṇām apratyakṣatvāt saṃyogasya pañcātmakaṃ na vidyate | KVs_4,2.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇāntarāprādurbhāvāc ca tryātmakam api na | KVs_4,2.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmasaṃyogas tv avipratiṣiddho mithaḥ pañcānām | KVs_4,2.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anekadeśapūrvakatvāt | KVs_4,2.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dharmaviśeṣāt | KVs_4,2.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāryaviśeṣāt | KVs_4,2.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "samākhyābhāvāt | KVs_4,2.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃjñādimattvāt | KVs_4,2.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "santy ayonijā vedaliṅgāc ca | KVs_4,2.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmasaṃyogaprayatnābhyāṃ haste karma | KVs_5,1.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā musalakarma hastasaṃyogāc ca | KVs_5,1.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "abhighātaje musalakarmaṇi vyatirekād akāraṇaṃ hastasaṃyogaḥ | KVs_5,1.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathātmasaṃyogo hastamusalakarmaṇi | KVs_5,1.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "musalābhighātāt tu musalasaṃyogād dhaste karma | KVs_5,1.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathātmakarma hastasaṃyogāc ca | KVs_5,1.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogābhāve gurutvāt patanam | KVs_5,1.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nodanaviśeṣābhāvād nordhvaṃ na tiryag gamanam | KVs_5,1.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prayatnaviśeṣān nodanaviśeṣaḥ | KVs_5,1.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nodanaviśeṣād udasanaviśeṣaḥ | KVs_5,1.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "hastakarmaṇā dārakakarma vyākhyātam | KVs_5,1.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā dagdhasya visphoṭanam | KVs_5,1.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prayatnābhāve gurutvāt suptasya patanam | KVs_5,1.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tṛṇakarma vāyusaṃyogāt | KVs_5,1.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "maṇigamanaṃ sūcyabhisarpaṇam ity adṛṣṭakāritāni | KVs_5,1.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "iṣāv ayugapat saṃyogaviśeṣāḥ karmānyatve hetuḥ | KVs_5,1.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nodanād ādyam iṣoḥ karma karmakāritāc ca saṃskārād uttaraṃ tathottaram uttaraṃ ca | KVs_5,1.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃskārābhāve gurutvāt patanam | KVs_5,1.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nodanād abhighātāt saṃyuktasaṃyogāc ca pṛthivyāṃ karma | KVs_5,2.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad viśeṣeṇādṛṣṭakāritam | KVs_5,2.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "apāṃ saṃyogābhāve gurutvāt patanam | KVs_5,2.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad viśeṣeṇādṛṣṭakāritam | KVs_5,2.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravatvāt syandanam | KVs_5,2.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nāḍyā vāyusaṃyogād ārohaṇam | KVs_5,2.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nodanāt pīḍanāt saṃyuktasaṃyogāc ca | KVs_5,2.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vṛkṣābhisarpaṇam ity adṛṣṭakāritam | KVs_5,2.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "apāṃ saṅghāto vilayanaṃ ca tejasaḥ saṃyogāt | KVs_5,2.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatrāvasphūrjathur liṅgam | KVs_5,2.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vaidikaṃ ca | KVs_5,2.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "apāṃ saṃyogād vibhāgāc ca stanayitnuḥ | KVs_5,2.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pṛthivīkarmaṇā tejaḥkarma vāyukarma ca vyākhyātam | KVs_5,2.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "agner ūrdhvajvalanaṃ vāyoś ca tiryagpavanam aṇumanasoś cādyaṃ karmety adṛṣṭakāritāni | KVs_5,2.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "hastakarmaṇā manasaḥ karma vyākhyātam | KVs_5,2.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmendriyamano'rthasannikarṣāt sukhaduḥkhe tadanārambhaḥ | KVs_5,2.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmasthe manasi saśarīrasya sukhaduḥkhābhāvaḥ sa yogaḥ | KVs_5,2.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāyakarmaṇātmakarma vyākhyātam | KVs_5,2.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "apasarpaṇam upasarpaṇam aśitapītasaṃyogaḥ kāryāntarasaṃyogāś cety adṛṣṭakāritāni | KVs_5,2.19 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadabhāve saṃyogābhāvo 'prādurbhāvaḥ sa mokṣaḥ | KVs_5,2.20 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyaguṇakarmavaidharmyād bhāvābhāvamātraṃ tamaḥ | KVs_5,2.21 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tejaso dravyāntareṇāvaraṇāc ca | KVs_5,2.22 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dikkālāv ākāśaṃ ca kriyāvadbhyo vaidharmyān niṣkriyāṇi | KVs_5,2.23 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena karmāṇi guṇāś ca vyākhyātāḥ | KVs_5,2.24 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "niṣkriyāṇāṃ samavāyaḥ karmabhyaḥ pratiṣiddhaḥ | KVs_5,2.25 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇaṃ tv asamavāyino guṇāḥ | KVs_5,2.26 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇair dig vyākhyātā | KVs_5,2.27 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇena kālaḥ | KVs_5,2.28 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "buddhipūrvā vākyakṛtir vede | KVs_6,1.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na cāsmadbuddhibhyo liṅgam ṛṣeḥ | KVs_6,1.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā brāhmaṇe saṃjñākarmasiddhir liṅgam | KVs_6,1.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "buddhipūrvo dadātiḥ | KVs_6,1.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā pratigrahaḥ | KVs_6,1.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tayoḥ kramo yathānitaretarāṅgabhūtānām | KVs_6,1.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmaguṇeṣv ātmāntaraguṇānām akāraṇatvāt | KVs_6,1.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aduṣṭabhojanāt samabhivyāhārato 'bhyudayaḥ | KVs_6,1.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad duṣṭabhojane na vidyate | KVs_6,1.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "duṣṭaṃ hiṃsāyām | KVs_6,1.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "samabhivyāhārato doṣaḥ | KVs_6,1.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad aduṣṭe na vidyate | KVs_6,1.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viśiṣṭe pravṛttiḥ | KVs_6,1.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "same hīne cāpravṛttiḥ | KVs_6,1.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena hīnasamaviśiṣtadhārmikebhyaḥ parādānaṃ vyākhyātam | KVs_6,1.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā viruddhānāṃ tyāgaḥ | KVs_6,1.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sama ātmatyāgaḥ paratyāgo vā | KVs_6,1.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viśiṣṭe ātmatyāgaḥ | KVs_6,1.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dṛṣṭānāṃ dṛṣṭaprayojanānāṃ dṛṣṭābhāve prayogo 'bhyudayāya.(C,V) (U dṛṣṭādṛṣṭaprayojanānāṃ dṛṣṭābhāve prayojanam abhyudayāya.) | KVs_6,2.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "abhiṣecanopavāsabrahmacaryagurukulavāsavānaprasthyayajñadānaprokṣaṇadiṅnakṣatramantrakālaniyamāś cādṛṣṭāya | KVs_6,2.2 |"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "cāturāśramyam upadhāc cānupadhāc ca/ | KVs_6,2.3 |",
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvadoṣa upadhā | KVs_6,2.4 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "adoṣo 'nupadhā | KVs_6,2.5 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "iṣṭarūparasagandhasparśaṃ prokṣitam abhyukṣitaṃ ca tac chuci | KVs_6,2.6 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aśucīti śucipratiṣedhaḥ | KVs_6,2.7 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "arthāntaraṃ ca | KVs_6,2.8 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ayatasya śucibhojanād abhyudayo na vidyate yamābhāvāt | KVs_6,2.9 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vidyate cānarthāntaratvād yamasya | KVs_6,2.10 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asati cābhāvāt | KVs_6,2.11 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sukhād rāgaḥ | KVs_6,2.12 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tanmayatvāt | KVs_6,2.13 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tṛpteḥ | KVs_6,2.14 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "adṛṣṭāt | KVs_6,2.15 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jātiviśeṣāc ca rāgaviśeṣaḥ | KVs_6,2.16 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "icchādveṣapūrvikā dharmādharmayoḥ pravṛttiḥ | KVs_6,2.17 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tataḥ saṃyogo vibhāgaś ca | KVs_6,2.18 |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ātmakarmasu mokṣo vyākhyātaḥ | KVs_6,2.19 |"
        }
      ]
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "uktā guṇāḥ | KVs_7,1.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇalakṣaṇaṃ coktam | KVs_7,1.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "idam evaṃguṇam idam evaṃguṇam iti coktam | KVs_7,1.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pṛthivyāṃ rūparasagandhasparśā dravyānityatvād anityāḥ | KVs_7,1.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "agnisaṃyogāc ca | KVs_7,1.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇāntaraprādurbhāvāt | KVs_7,1.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena nityeṣv anityatvam uktam | KVs_7,1.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "apsu tejasi vāyau ca nityā dravyanityatvāt | KVs_7,1.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anityeṣv anityā dravyānityatvāt | KVs_7,1.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇaguṇapūrvāḥ pṛthivyāṃ pākajāś ca | KVs_7,1.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "apsu tejasi vāyau ca kāraṇaguṇapūrvāḥ pākajā na vidyante | KVs_7,1.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aguṇavato dravyasya guṇārambhāt karmaguṇā aguṇāḥ | KVs_7,1.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena pākajā vyākhyātāḥ | KVs_7,1.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekadravyavattvāt | KVs_7,1.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aṇor mahataś copalabdhyanupalabdhī nitye vyākhyāte | KVs_7,1.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇabahutvāt kāraṇamahattvāt pracayaviśeṣāc ca mahat | KVs_7,1.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadviparītam aṇu | KVs_7,1.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aṇu mahad iti tasmin viśeṣabhāvād viśeṣābhāvāc ca | KVs_7,1.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekakālatvāt | KVs_7,1.19 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aṇutvamahattvayor aṇutvamahattvābhāvaḥ karmaguṇair vyākhyātaḥ | KVs_7,1.21 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aṇutvamahattvābhyāṃ karmaguṇā aguṇāḥ | KVs_7,1.22 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena dīrghatvahrasvatve vyākhyāte | KVs_7,1.23 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "karmabhiḥ karmāṇi guṇair guṇāḥ | KVs_7,1.24 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad anitye | KVs_7,1.25 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nityaṃ parimaṇḍalam | KVs_7,1.26 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "avidyā vidyāliṅgam | KVs_7,1.27 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vibhavān mahān ākāśaḥ | KVs_7,1.28 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā cātmā | KVs_7,1.29 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadabhāvād aṇu manaḥ | KVs_7,1.30 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇair dig vyākhyātā | KVs_7,1.31 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇena kālaḥ | KVs_7,1.32 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "rūparasagandhasparśavyatirekād arthāntaram ekatvaṃ tathā pṛthaktvam | KVs_7,2.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tayor nityatvānityatve tejaso rūpasparśābhyāṃ vyākhyāte | KVs_7,2.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "niṣpattiś ca | KVs_7,2.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekatvapṛthaktvayor ekatvapṛthaktvābhāvo | KVs_7,2.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "karmabhiḥ karmāṇi guṇair guṇāḥ | KVs_7,2.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "niḥsaṃkhyatvāt karmaguṇānāṃ sarvaikatvaṃ na vidyate | KVs_7,2.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekatvasyābhāvād bhāktaṃ na vidyate | KVs_7,2.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāryakāraṇaikatvapṛthaktvābhāvād ekatvapṛthaktve na vidyete | KVs_7,2.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etad anityanitayor vyākhyātam | KVs_7,2.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anyatarakarmaja ubhayakarmajaḥ saṃyogajaś ca saṃyogaḥ | KVs_7,2.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena vibhāgo vyākhyātaḥ | KVs_7,2.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogavibhāgayoḥ saṃyogavibhāgābhāvo 'ṇutvamahattvābhyāṃ vyākhyātaḥ | KVs_7,2.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "karmabhiḥ karmāṇi guṇair guṇāḥ | KVs_7,2.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yutasiddhyabhāvāt kāryakāraṇayoḥ saṃyogavibhāgau na vidyete | KVs_7,2.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇatvāt | KVs_7,2.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇe ca bhāṣyate | KVs_7,2.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "niṣkriyatvāt | KVs_7,2.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asati nāstīti ca prayogāt | KVs_7,2.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śabdārthāv asaṃbaddhau | KVs_7,2.19 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃyogino daṇḍāt samavāyino viṣāṇāc ca | KVs_7,2.20 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dṛṣṭatvād ahetuḥ pratyayaḥ | KVs_7,2.21 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā pratyayābhāvaḥ | KVs_7,2.22 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃbaddhasaṃbandhād iti cet sandehaḥ | KVs_7,2.23 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmayikaḥ śabdād arthapratyayaḥ | KVs_7,2.24 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekadikkālābhyāṃ sannikṛṣṭaviprakṛṣṭābhyāṃ param aparam | KVs_7,2.25 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāraṇaparatvāt kāraṇāparatvāc ca | KVs_7,2.26 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "paratvāparatvayoḥ paratvāparatvābhāvo 'ṇutvamahattvābhyāṃ vyākhyātaḥ | KVs_7,2.27 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "karmabhiḥ karmāṇi guṇair guṇāḥ | KVs_7,2.28 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "iheti yataḥ kāryakāraṇayoḥ sa samavāyaḥ | KVs_7,2.29 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyatvaguṇatvakarmatvapratiṣedho bhāvena vyākhyātaḥ | KVs_7,2.30 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tattvaṃ ca | KVs_7,2.31 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyeṣu jñānaṃ vyākhyātam | KVs_8.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mana ātmā ca | KVs_8.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "jñānanirdeśe jñānaniṣpattir uktā | KVs_8.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇakarmasv asannikṛṣṭeṣu jñānaniṣpatter dravyaṃ kāraṇaṃ kāraṇakāraṇaṃ ca | KVs_8.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyaviśeṣeṣu sāmānyaviśeṣābhāvāt tata eva jñānam | KVs_8.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sāmānyaviśeṣāpekṣaṃ dravyaguṇakarmasu | KVs_8.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravye dravyaguṇakarmāpekṣam | KVs_8.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "guṇakarmasu guṇakarmābhāvad guṇakarmāpekṣaṃ na vidyate | KVs_8.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "samavāyinaḥ śvaityāc chvaityabuddheḥ śvete buddhis te kāryakāraṇabhūte | KVs_8.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyeṣv anitaretarakāraṇāt kāraṇāyaugapadyāt | KVs_8.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā dravyaguṇakarmasu kāraṇāviśeṣāt | KVs_8.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ayam eṣa kṛtaṃ tvayā bhojayāinam iti buddhyapekṣaṃ | KVs_8.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dṛṣṭeṣu bhāvād adṛṣṭeṣv abhāvāt | KVs_8.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "artha iti dravyaguṇakarmasu | KVs_8.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dravyeṣu pañcātmakaṃ pratyuktam | KVs_8.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bhūyastvād gandhavattvāc ca pṛthivī gandhajñāne | KVs_8.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathāpas tejo vāyuś ca rasarūpasparśajñāneṣu rasarūpasparśaviśeṣāt | KVs_8.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kriyāguṇavyapadeśābhāvād asat | KVs_9.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sad asat | KVs_9.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asataḥ sat kriyāguṇavyapadeśābhāvād arthāntaram (śabhāvād ?) | KVs_9.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sac cāsat | KVs_9.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yac cānyat satas tad apy asat | KVs_9.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asad iti bhūtapratyakṣābhāvād bhūtasmṛter virodhipratyakṣatvāc ca jñānam | KVs_9.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathābāve bhāvapratyakṣatvāc ca | KVs_9.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etenāghaṭo 'gaur adharmaś ca vyākhyātaḥ | KVs_9.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "abhūtaṃ nāstīty anarthāntaram | KVs_9.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nāsti ghaṭo geha iti sato ghaṭasya gehasaṃyogapratiṣedhaḥ | KVs_9.10 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nāsty anyaś candramā iti sāmānyāc candramasaḥ pratiṣedhaḥ | KVs_9.11 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sad asator vaidharmyāt kārye sad asattā na | KVs_9.12 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmany ātmamanasoḥ saṃyogaviśeṣād ātmapratyakṣam | KVs_9.13 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā dravyāntareṣu | KVs_9.14 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmendriyamano'rthasannikarṣāc ca | KVs_9.15 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatsamavāyāt karmaguṇeṣu | KVs_9.16 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmasamavāyād ātmaguṇeṣu | KVs_9.17 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asyedaṃ kāryaṃ kāraṇaṃ saṃbandhy ekārhtasamavāyi virodhi ceti laiṅgikam | KVs_9.18 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena śābdaṃ vyākhyātam | KVs_9.19 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "hetur apadeśo liṅgaṃ nimittaṃ pramāṇaṃ kāraṇam ity anarthāntaram | KVs_9.20 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asyedam iti buddhyapekṣatvāt | KVs_9.21 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmamanasoḥ saṃyogaviśeṣāt saṃskārāc ca smṛtiḥ | KVs_9.22 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā svapnaḥ svapnāntikaṃ ca | KVs_9.23 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dharmāc ca | KVs_9.24 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "indriyadoṣāt saṃskārāc cāvidyā | KVs_9.25 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad duṣṭaṃ jñānam | KVs_9.26 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aduṣṭaṃ vidyā | KVs_9.27 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ārṣaṃ siddhadarśanaṃ ca dharmebhyaḥ | KVs_9.28 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ātmasamavāyaḥ sukhaduḥkhayoḥ pañcabhyo 'rthāntaratve hetus tadāśrayibhyaś ca guṇebhyaḥ | KVs_10.1 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "iṣṭāniṣṭakāraṇaviśeṣād virodhāc ca mithaḥ sukhaduḥkhayor arthāntarabhāvaḥ | KVs_10.2 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃśayanirṇayayor arthāntarabhāvaś ca jñānāntaratve hetuḥ | KVs_10.3 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tayor niṣpattiḥ pratyakṣalaiṅgikābhyāṃ jñānāntaratve hetuḥ | KVs_10.4 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bhūtam iti pratyakṣaṃ vyākhyātam | KVs_10.5 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bhaviṣyatīti kāryāntare dṛṣṭatvāt | KVs_10.6 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tathā bhavatīti sāpekṣebhyo 'napekṣebhyaś ca | KVs_10.7 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "abhūd ity abhūtāt | KVs_10.8 |"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sati ca kāryāsamavāyāt | KVs_10.9 |"
    },
    {
      "tag": "l",
      "attributes": {},
