  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "Nāgārjuna: Āryaśālistambakakārikā"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "āryamañjuśriye namaḥ"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "anantācintyaguṇyaṃ hi sambuddhaṃ karuṇātmakam /",
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "praṇipatya pravakṣyāmi śālistambakakārikām / 1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "munī rājagṛhasyaiva gṛdhranāmakaparvate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhikṣūṇāṃ bodhisattvānāṃ saṃghaiḥ sārdhaṃ vyavasthitaḥ // 2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetupratyayasaṃbhūtaṃ śālistambaṃ vilokya ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetupratyayajaṃ tadvad dvādaśāṅkakramodgatam // 3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pratītyamiti yaḥ praśyet dharmaṃ buddhaṃ ca paśyati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ityuktvā nāyakā bhikṣūn tūṣṇībhāvamavasthitaḥ // 4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhikṣuḥ śārisutraḥ śrutvā gatvā maitreyasannidhau /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathāgato 'dya maitreya uktyarthaṃ na vibhajya ca // 5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tūṣṇīṃ bhāve sthitaścātra tadartho gamyate katham /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kiṃ pratītyaṃ ca ko dharmo buddho 'pi katamastathā // 6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pratītyaṃ tu kathaṃ dṛṣṭvā dharma buddhaṃ ca paśyati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sandeho me 'tra brūhīti ūce śārisuto 'jitam // 7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvātmikā hi maitrī syāt maitreyo 'brūt nirṇayam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dvādaśāṅgamavidyādi maraṇāntaṃ yathākramam // 8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmāttarhibhavantyeva duḥkhaskandhā hi kevalam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmaścāṣṭāṅgiko mārgaḥ phalaṃ nirvāṇamucyate // 9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvasyādhigamādevaṃ dharmajaṃ buddhameva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathoktamāryaṃ duṣṭatvāt yaḥ paśyati sa paśyati // 10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prāṇādirahitād yaśca vyupaśāntyantasaṃyutam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratītyaṃ dharmabuddhau ca śuddhabuddhayā vi paśyati // 11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pratītyalakṣaṇaṃ tāvat sahetvādipadānvitam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "buddhotpādo bhavenno vā sthiteyaṃ dharmatā yataḥ // 12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bāhya ādhyātmikaścāpi dvividho hetupratyayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bāhyo hetustu bījādiḥ pratyayaḥ ṣaḍvidho mataḥ // 13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bījāṅkuraprakāṇḍādiḥ phale yadvat pravartate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyayastu pṛthivyādikālānto hi yathākramam // 14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dhāraṇaṃ snehanaṃ pāko dhānyavṛddhiranāvṛtiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pariṇāmastathā teṣāṃ kāryaṃ tadvat pravartate // 15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "no cet pratyayasāmagrī bīje bhūte 'pi nāṅkuraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bījābhāve tu satyeva pratyayabhāvo 'pi tādṛśaḥ // 16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetavaḥ pratyayāstadvad ātmagrāhādivarjitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetupratyayasamagryā na naśyet karmaṇaḥ phalam // 17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na svato parato nāpi na dvayoḥ kartṛkālataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "īśvarādikṛtaṃ naivaṃ svabhāvānnāpyahetutaḥ // 18 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "hetupratyayayorvṛttirbhāsate 'nādikālataḥ //"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pañcabhirhetubhirbāhyaḥ pratītyotpāda iṣyate // 19 //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śāśvatato na coccedān na saṃkranteḥ parīttataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hertormahāphalāvāptiḥ sadṛśānuprabodhataḥ // 20 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aṅkuro bījavaneṣṭo nirheturno 'ṅkurodbhavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "samo nirodha utpādastulonnāmāvanāmavat // 21 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tathaivādhyātmikasyāpi hetuśca pratyayo dvidhā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ādiheturavidyāsya mṛtyurantyo yathākramam // 22 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "samajanmakleśakarmātmā dvādaśāṅkastrikāṇḍakaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetupratyayasambhūtaḥ kartetyādivivarjitaḥ // 23 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "avidyā yadi nādau syādante mṛtyurna saṃbhaved /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tebhyo bhinno na kutrāpi hyātmātmīyaśca vidyate // 24 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "avidyāsaṃbhavādādāvante mṛtyuśca bhāsate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetorādhyātmikasyaivaṃ pratyayāḥ ṣaḍprakārakaḥ // 25 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyayo 'dhyātmikastvante vijñānaṃ cādike dharā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāṭhinyaṃ saṃgrahaḥ pākaḥ śvāsavṛddhiranāvṛtiḥ // 26 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānarūpānuvṛttiśca pañcavijñānasaṃyutam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmāt kliṣṭaṃ manaścāpi hīme 'dhyātmikapratyayāḥ // 27 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dhātūnāṃ sannipātādvai śarīrotpāda iṣyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ātmātmīyavikalpānāmutpādastairna manyate // 28 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "teṣu satsu samutpādasteṣvasatsu na saṃbhavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "naivātmādimayāste hi nāpyanyaccāpi kiñcana // 29 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yaikapiṇḍādisaṃjñā sāvidyātribhavachādikā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "rāgo dveṣaśca mohādiḥ pravṛttāḥ santyavidyayā // 30 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tataḥ saṃskṛtabhāvānāṃ jñaptirvijñānasaṃbhavā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñānena sahodbhūtāścatuskandhā arūpiṇaḥ // 31 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nāmarūpamupādāya cendriyāyatanodbhavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viṣayenriyavijñānasaṃghātāt sparśasambhavaḥ // 32 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vedanā sparśajā jñeyā tṛṣṇā ca vedanodgatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tṛṣṇāvṛddhirupādānam upādānodgato bhavaḥ // 33 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "skandhotpādo bhavājjātirjāterevaṃ jarāpi ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "skandhābhāvo jarāyā yaḥ sa mṛtyuścetyudīryate // 34 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mūḍhe tu maraṇācchokaḥ satṛṣṇe dāha āntaraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śokataścāpalāpo yo daurmanasyaṃ sa ucyate // 35 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "daurmanasyasamudbhūtaṃ pañcavijñānakāyikam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āsātaduḥkhamityuktaṃ kāyasaukhyavighātakam // 36 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "duḥkhaṃ manasikārākhyaṃ manasastūpadhātakam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "daurmanasyaṃ ca tajjñeyamanyopakleśahetukram // 37 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tamo 'bhijñānāmarūpāyatasparśavittarṣataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tṛṣṇādānabhavotpāda pākanāśaviśokataḥ // 38 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vacanādikāyasaṃpīḍācittadurmānasāstathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kleśādanvarthakaṃ nāma yathākramamudīritam // 39 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "punastattvaparijñānādavidyādeśca yathākramam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pūrvapūrvebhya utpādo 'pyākhyātaścottarottaraḥ // 40 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dvādaśāṅgautripravṛttī nityocchedau hyanādijau /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pravṛtterjaladhārāvad vartate 'nādikālataḥ // 41 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tathāpyete tu catvāraḥ saṃghātakarahetavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avidyā ca tṛṣā karma vijñānaṃ kramaśo matāḥ // 42 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetorvijñānabījaṃ hi karmakṣetramudīritam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prathamaḥ ca tṛṣā prokte hetuḥ kleśasvabhāvataḥ // 43 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "karmakleśāstuvijñānabījatvena vyavasthitāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "karma vijñānabījasya kṣetrakāryaṃ karoti ca // 44 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñānanāmakaṃ bījaṃ tṛṣṇayā snihyate param /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñānabījaṃ cāvidyā kirati snehanena vai // 45 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "karma tṛṣṇā tathāvidyā kṣetraṃsneho 'vakīrṇanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñāne na karomīdaṃ na vijñānamito matam // 46 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tathāpi bījavijñāneṃ karmakleśapratiṣṭhite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñānaṃ bījamityuktaṃ kīrṇe 'vidyāsvavaskare // 47 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tṛṣṇājalena saṃsikte hetuto nāmarūpayoḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aṅkurotpādabhāso hi na svaparobhayāditaḥ // 48 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nāmarūpamidaṃjātaṃ piturmātuḥsamāgamāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avirodhādṛtoścāpi kiñcidāsvādavedhitam // 49 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bījavijñānamityuktaṃ mātṛgarbhe kramāccayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nāmarūpāṅkurotpādastvavaikalyācca pratyayaiḥ // 50 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "avirodhatvāccahetūnāṃ māyānairātmyanigrahe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "utpādo 'pi na saṃbhāvyaḥ cakṣurvijñānamapyataḥ // 51 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pañcabhirhetubhirjātaṃ cakṣūrūpāvabhāsanaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nabhastajjamanaskāraiḥ pañcavaikalyatastathā // 52 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "cakṣurvijñānamudbhūtaṃ mayā te janitā iti /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vikalpo na yathodeti śrotrajñānādikākhilam // 53 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "utpādasya kramaścaivaṃ hetupratyayasaṅgrahāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kartrādīnāṃ ca vaikalyād ahaṅkāraviyogataḥ // 54 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "utpādo 'pi yathāpūrva tatha cāpi pratītyajam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetumatsaṃvijānīyād asmāllokāt paraṃ nahi // 55 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kaściddhamaḥ kvacid gantā hetupratyayatastathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "karmaṇaḥ phalamabhyeti, yathā darśe viśodhite // 56 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛśyante mukhabimbāni darpaṇe 'pi ca bimbakam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃkrāmitaṃ bhavetraiva tadanyonyāvikalpanam // 57 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kartṛkriyāvihīnaṃ tat tathotpādāvabhāsanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pūrvavṛddhikramācca syād dūrasthaścandramā yathā // 58 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "parīttodakapātrānte dṛśyate na ca krāmati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asti kriyā ca karmāpi tathā cāsmāccyutirna hi // 59 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "janmābhāso 'pyasaṃlloke nopādānaṃ vinānalaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jvalet sakala ujjvālo hīnopādānahānitaḥ // 60 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "skandhatvapratisandhiḥsyāt, santi te kalpanātmakāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bāhyaṃ karma kriyā heturadhyātmaparatantrataḥ // 61 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pañcavijñānasaṃbhavāḥparamārtho 'vicāryataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pariniṣpanna ākhyātaḥsahetupratyayodbhavaḥ // 62 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvadā dvividho jñeyaḥ kartrādirahitastathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tucchaśūnyāditiḥsāraḥprajñayaivaṃ ya īkṣitaḥ // 63 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kiṃ kathaṃ vā kuto kena kalpavādādihānitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "udacandrasya yathā bimbaṃ tataḥ kaścicca na cyutaḥ // 64 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "loke janmāpi cābhāti yathā pādapasaṅgataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vahnistrotaḥ pravṛtiḥsyād hetuvaikalpatastathā // 65 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nānupravartate hyagnitadvatsaṃkleśabījake /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dagdhe jñānāgninā hetorabhāvānnaphalaṃ tathā // 66 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kriyā karmāpi naivāstianantācintyaguṇyakam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śāntadharmātmakaṃ kāyamādimadhyāntavarjitam // 67 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jñātvā prāpnoti buddhatvaṃ ya evaṃ tathatākṣamaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmai vyākriyate nūnaṃ maitreyastu svayaṃ tathā // 68 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "uvāca śāriputrāya śālistambopamā kṛtā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śāriputrastu tacchruttvā devasaṃghānumoditaḥ // 69 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃstuto dhṛtasāraśca gatvotthāya praharṣitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ākhyātavāṃśca bhiksubhyaḥ // 70 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "āryānāgārjunaviracitā āryaśālistambakasūtrakārikā samāptā /"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvatīyopādhyāyena dharmaśrībhadreṇa mahāsaṃśodhakalokacakṣussādhumatijñānakumārābhyām cānūditaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "paścāt śrīkuṭīrakṣitena saṃśodhya nirṇītaḥ //"
        }
      ]
    }
  ]
}