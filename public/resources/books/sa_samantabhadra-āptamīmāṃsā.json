  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "Samantabhadra: Āptamīmāṃsā"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "devāgamanabhoyānacāmarādivibhūtayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyāviṣvapi dṛśyante nātastvamasi no mahān // SAm_1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "adhyātmaṃ bahirapyeṣa vigrahādimahodayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "divyaḥ satyo divaukassvapyasti rāgādimatsu saḥ // SAm_2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tīrthakṛtsamayānāṃ ca parasparavirodhataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarveṣāmāptatā nāsti kaścideva bhavedguruḥ // SAm_3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "doṣāvaraṇayorhānirniḥśeṣāstyatiśāyanāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kvacidyathā svahetubhyo bahirantarmalakṣayaḥ // SAm_4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sūkṣmāntaritadūrārthāḥ pratyakṣāḥ kasyacidyathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anumeyatvato 'gnyādiriti sarvajñasaṃsthitiḥ // SAm_5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sa tvamevāsi nirdoṣo yuktiśāstrāvirodhavāk /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avirodho yadiṣṭaṃ te prasiddhena na bādhyate // SAm_6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tvanmatāmṛtabāhyānāṃ sarvathaikāntavādinām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āptābhimānadagdhānāṃ sveṣṭaṃ dṛṣṭena bādhyate // SAm_7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kuśalākuśalaṃ karma paralokaśca na kvacit /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ekāntagraharakteṣu nātha svaparavairiṣu // SAm_8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvaikānte padārthānāmabhāvānāmapahnavāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvātmakamanādyantamasvarūpamatāvakam // SAm_9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryadravyamanādi syātprāgabhāvasya nihnave /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pradhvaṃsasya ca dharmasya pracyave 'nantatāṃ vrajet // SAm_10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvātmakaṃ tadekaṃ syādanyāpohavyatikrame /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anyatra samavāyena vyapadiśyeta sarvathā // SAm_11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abhāvaikāntapakṣe 'pi bhāvāpahnavavādinām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bodhavākyaṃ pramāṇaṃ na kena sādhanadūṣaṇam // SAm_12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kathaṃcitte sadeveṣṭaṃ kathañcidasadeva tat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathobhayamavācyaṃ ca nayayogānna sarvathā // SAm_14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sadeva sarvaṃ ko necchetsvarūpādicatuṣṭayāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asadeva viparyāsānna cenna vyavatiṣṭhate // SAm_15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kramārpitadvayād dvaitaṃ sahāvācyamaśaktitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avaktavyottarāḥ śeṣāstrayo bhaṅgāḥ svahetutaḥ // SAm_16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "astitvaṃ pratiṣedhyenāvinābhāvyekadharmiṇi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśeṣaṇatvātsādharmyaṃ yathā bhedavivakṣayā // SAm_17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nāstitvaṃ pratiṣedhyenāvinābhāvyekadharmiṇi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśeṣaṇatvādvaidharmyaṃ yathā 'bhedavivakṣayā // SAm_18 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vidheyapratiṣedhyātmā viśeṣyaḥ śabdagocaraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sādhyadharmo yathā heturahetuścāpyapekṣayā // SAm_19 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śeṣabhaṅgāśca netavyā yathoktanayayogataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na ca kaścidvirodho 'sti munīndra tava śāsane // SAm_20 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "evaṃ vidhiniṣedhābhyāmanavasthitamarthakṛt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "neti cenna yathākāryaṃ bahirantarupādhibhiḥ // SAm_21 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dharme dharme 'nya evārtho dharmiṇo 'nantadharmaṇaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aṅgitve 'nyatamāntasya śeṣāntānāṃ tadaṅgatā // SAm_22 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ekānekavikalpādāvuttaratrāpi yojayet /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prakriyāṃ bhaṅginīmenāṃ nayairnayaviśāradaḥ // SAm_23 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "advaitākāntapakṣe 'pi dṛṣṭo bhedo virudhyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kārakāṇāṃ kriyāyāśca naikaṃ svasmātprajāyate // SAm_24 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "karmadvaitaṃ phaladvaitaṃ lokadvaitaṃ ca no bhavet /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vidyāvidyādvayaṃ na syādbandhamokṣadvayaṃ tathā // SAm_25 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetoradvaitasiddhiśced dvaitaṃ syāddhetusādhyayoḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetunā cedvinā siddhirdvaitaṃ vāṅmātrato na kim // SAm_26 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "advaitaṃ na vinā dvaitādaheturiva hetunā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃjñinaḥ pratiṣedho na pratiṣedhyādṛte kvacit // SAm_27 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pṛthaktvaikāntapakṣe 'pi pṛthaktvādapṛthakkṛtau /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pṛthaktve na pṛthaktvaṃ syādanekastho hyasau guṇaḥ // SAm_28 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "santānaḥ samudāyaśca sādharmyaṃ ca niraṅkuśaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pretyabhāvaśca tatsarvaṃ na syādekatvanihnave // SAm_29 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sadātmanā ca bhinnaṃ cejjñānaṃ jñeyād dvidhā 'pyasat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānābhāve kathaṃ jñeyaṃ bahirantaśca te dviṣām // SAm_30 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānyārthā giro 'nyeṣāṃ viśeṣo nābhilapyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānyābhāvatasteṣāṃ mṛṣaiva sakalā giraḥ // SAm_31 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_32 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anapekṣe pṛthaktvaikyaṃ hyavastu dvayahetutaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tadevaikyaṃ pṛthaktvaṃ ca svabhedaiḥ sādhanaṃ yathā // SAm_33 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "satsāmānyāttu sarvaikyaṃ pṛthagdravyādibhedataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhedābhedavivakṣāyāmasādhāraṇahetuvat // SAm_34 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vivakṣā cāvivakṣā ca viśeṣye 'nantadharmiṇi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sato viśeṣaṇasyātra nāsatastaistadarthibhiḥ // SAm_35 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pramāṇagocarau santau bhedābhedau na saṃvṛtī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tāvekatrāviruddhau te guṇamukhyavivakṣayā // SAm_36 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nityatvaikāntapakṣe 'pi vikriyā nopapadyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prāgeva kārakābhāvaḥ kva pramāṇaṃ kva tatphalam // SAm_37 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pramāṇakārakairvyaktaṃ vyaktaṃ cedindriyārthavat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "te ca nityaṃ vikāryaṃ kiṃ sādhoste śāsanādbahiḥ // SAm_38 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yadi satsarvathā kāryaṃ puṃvannotpattumarhati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pariṇāmaprakḷptiśca nityatvaikāntabādhinī // SAm_39 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "puṇyapāpakriyā na syātpretyabhāvaḥ phalaṃ kutaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bandhamokṣau ca teṣāṃ na yeṣāṃ tvaṃ nāsi nāyakaḥ // SAm_40 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kṣaṇikaikāntapakṣe 'pi pretyabhāvādyasambhavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyabhijñādyabhāvānna kāryārambhaḥ kutaḥ phalam // SAm_41 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yadyasatsarvathā kāryaṃ tanmā jani khapuṣpavat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "mopādānaniyāmo bhūnmāśvāsaḥ kāryajanmani // SAm_42 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na hetuphalabhāvādiranyabhāvādananvayāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "santānāntaravannaikaḥ santānastadvataḥ pṛthak // SAm_43 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anyeṣvananyaśabdo 'yaṃ saṃvṛtirna mṛṣā katham /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "mukhyārthaḥ saṃvṛtirnāsti vinā mukhyānna saṃvṛtiḥ // SAm_44 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "catuṣkoṭervikalpasya sarvānteṣūktyayogataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvānyatvamavācyaṃ ca tayoḥ santānatadvatoḥ // SAm_45 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "avaktavyacatuṣkoṭivikalpo 'pi na kathyatām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asarvāntamavastu syādaviśeṣyaviśeṣaṇam // SAm_46 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dravyādyantarabhāvena niṣedhaḥ saṃjñinaḥ sataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asadbhedo na bhāvastu sthānaṃ vidhiniṣedhayoḥ // SAm_47 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "avastvanabhilāpyaṃ syātsarvāntaiḥ parivarjitam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vastvevāvastutāṃ yāti prakriyāyā viparyayāt // SAm_48 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvāntāścedavaktavyāsteṣāṃ kiṃ vacanaṃ punaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃvṛttiścenmṛṣaivaiṣā paramārthaviparyayāt // SAm_49 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aśakyatvādavācyaṃ kimabhāvātkimabodhataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ādyatoktidvayaṃ na syātkiṃ vyājenocyatāṃ sphuṭam // SAm_50 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hinastyanabhisaṃdhātṛ na hinastyabhisandhimat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "badhyate taddvayāpetaṃ cittaṃ baddhaṃ na mucyate // SAm_51 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ahetukatvānāśasya hiṃsāheturna hiṃsakaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "cittasantatināśaśca mokṣo nāṣṭāṅgahetukaḥ // SAm_52 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virūpakāryārambhāya yadi hetusamāgamaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āśrayibhyāmananyo 'sāvaviśeṣādayuktavat // SAm_53 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "skandhāḥ santatayaścaiva saṃvṛtitvādasaṃskṛtāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sthityutpattivyayāsteṣāṃ na syuḥ kharaviṣāṇavat // SAm_54 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_55 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nityaṃ tatpratyabhijñānānnākasmāttadavicchidā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kṣaṇikaṃ kālabhedātte buddhyasañcaradoṣataḥ // SAm_56 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na sāmānyātmanodeti na vyeti vyaktamanvayāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vyetyudeti viśeṣātte sahaikatrodayādi sat // SAm_57 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryotpādaḥ kṣayo heturniyamāllakṣaṇātpṛthak /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na tau jātyādyavasthānādanapekṣāḥ khapuṣpavat // SAm_58 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ghaṭamaulisuvarṇārthī nāśotpādasthitiṣvayam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śokapramohamādhyasthyaṃ jano yāti sahetukam // SAm_59 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "payovrato na dadhyatti na payo 'tti dadhivrataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "agorasavrato nobhe tasmāttattvaṃ trayātmakam // SAm_60 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryakāraṇanānātvaṃ guṇaguṇyanyatāpi ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānyatadvadanyatvaṃ caikāntena yadīṣyate // SAm_61 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ekasyānekavṛttirna bhāgābhāvādbahūni vā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāgitvādvāsya naikatvaṃ doṣo vṛtteranārhate // SAm_62 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "deśakālaviśeṣe 'pi syādvṛttiryutasiddhivat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "samānadeśatā na syānmūrtakāraṇakāryayoḥ // SAm_63 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āśrayāśrayibhāvānna svātantryaṃ samavāyinām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ityayuktaḥ sa sambandho na yuktaḥ samavāyibhiḥ // SAm_64 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānyaṃ samavāyaścāpyekaikatra samāptitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "antareṇāśrayaṃ na syānnāśotpādiṣu ko vidhiḥ // SAm_65 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvathānabhisambandhaḥ sāmānyasamavāyayoḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tābhyāmartho na sambaddhastāni trīṇi khapuṣpavat // SAm_66 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ananyataikānte 'ṇūnāṃ saṃghāte 'pi vibhāgavat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asaṃhatatvaṃ syādbhūtacatuṣkaṃ bhrāntireva sā // SAm_67 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryabhrānteraṇubhrāntiḥ kāryaliṅgaṃ hi kāraṇam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ubhayābhāvatastatsthaṃ guṇajātītaracca na // SAm_68 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ekatve 'nyatarābhāvaḥ śeṣābhāvo 'vinābhuvaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dvitvasaṅkhyāvirodhaśca saṃvṛtiścenmṛṣaiva sā // SAm_69 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_70 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dravyaparyāyayoraikyaṃ tayoravyatirekataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pariṇāmaviśeṣācca śaktimacchaktibhāvataḥ // SAm_71 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃjñāsaṃkhyāviśeṣācca svalakṣaṇaviśeṣataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prayojanādibhedācca tannānātvaṃ na sarvathā // SAm_72 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yadyāpekṣikasiddhiḥ syānna dvayaṃ vyavatiṣṭhate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anāpekṣikasiddhau ca na sāmānyaviśeṣatā // SAm_73 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_74 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmadharmyavinābhāvaḥ sidhyatyanyonyavīkṣayā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na svarūpaṃ svato hyetatkārakajñāpakāṅgavat // SAm_75 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "siddhaṃ ceddhetutaḥ sarvaṃ na pratyakṣādito gatiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "siddhaṃ cedāgamātsarvaṃ viruddhārthamatānyapi // SAm_76 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_77 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vaktaryanāpte yaddhetoḥ sādhyaṃ taddhetusādhitam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āpte vaktari tadvākyātsādhyamāgamasādhitam // SAm_78 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "antaraṅgārthataikānte buddhivākyaṃ mṛṣākhilam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pramāṇābhāsamevātastatpramāṇādṛte katham // SAm_79 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sādhyasādhanavijñapteryadi vijñaptimātratā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na sādhyaṃ na ca hetuśca pratijñāhetudoṣataḥ // SAm_80 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bahiraṅgārthataikānte pramāṇābhāsanihnavāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarveṣāṃ kāryasiddhiḥ syādviruddhārthābhidhāyinām // SAm_81 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_82 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvaprameyāpekṣāyāṃ pramāṇābhāsanihnavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bahiḥprameyāpekṣāyāṃ pramāṇaṃ tannibhaṃ ca te // SAm_83 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jīvaśabdaḥ sabāhyārthaḥ saṃjñātvāddhetuśabdavat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyādibhrāntisaṃjñāśca māyādyaiḥ svaiḥ pramoktivat // SAm_84 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "buddhiśabdārthasaṃjñāstāstisro buddhyādivācikāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tulyā buddhyādibodhāśca trayastatpratibimbikāḥ // SAm_85 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vaktṛśrotṛpramātṝṇāṃ vākyabodhapramāḥ pṛthak /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhrāntāveva pramābhrāntau bāhyārthau tādṛśetarau // SAm_86 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "buddhiśabdapramāṇatvaṃ bāhyārthe sati nāsati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "satyānṛtavyavasthaivaṃ yujyate 'rthāptyanāptiṣu // SAm_87 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "daivādevārthasiddhiśceddaivaṃ pauruṣataḥ katham /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "daivataścedanirmokṣaḥ pauruṣaṃ niṣphalaṃ bhavet // SAm_88 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pauruṣādeva siddhiścetpauruṣaṃ daivataḥ katham /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pauruṣāccedamoghaṃ syātsarvaprāṇiṣu pauruṣam // SAm_89 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_90 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abuddhipūrvāpekṣāyāmiṣṭāniṣṭaṃ svadaivataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "buddhipūrvavyapekṣāyāmiṣṭāniṣṭaṃ svapauruṣāt // SAm_91 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pāpaṃ dhruvaṃ pare duḥkhātpuṇyaṃ ca sukhato yadi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "acetanākaṣāyau ca badhyeyātāṃ nimittataḥ // SAm_92 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "puṇyaṃ dhruvaṃ svato duḥkhātpāpaṃ ca sukhato yadi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vītarāgo munirvidvāṃstābhyāṃ yuñjyānnimittataḥ // SAm_93 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_94 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "viśuddhisaṃkleśāṅgaṃ cetsvaparasthaṃ sukhāsukham /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "puṇyapāpāsravau yuktau na cedvyarthastavārhataḥ // SAm_95 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ajñānācceddhruvo bandho jñeyānantyānna kevalī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānastokādvimokṣaścedajñānād bahuto 'nyathā // SAm_96 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "virodhānnobhayaikātmyaṃ syādvādanyāyavidviṣām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avācyataikānte 'pyuktirnāvācyamiti yujyate // SAm_97 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ajñānānmohato bandho nājñānādvītamohataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānastokācca mokṣaḥ syādamohānmohito 'nyathā // SAm_98 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāmādiprabhavaścitraḥ karmabandhānurūpataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tacca karma svahetubhyo jīvāste śuddhyaśuddhitaḥ // SAm_99 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śuddhyaśuddhī punaḥ śaktī te pākyāpākyaśaktivat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sādyanādī tayorvyaktī svabhāvo 'tarkagocaraḥ // SAm_100 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvajñānaṃ pramāṇaṃ te yugapat sarvabhāsanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kramabhāvi ca yajjñānaṃ syādvādanayasaṃskṛtam // SAm_101 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "upekṣā phalamādyasya śeṣasyādānahānadhīḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pūrvaṃ vājñānanāśo vā sarvasyāsya svagocare // SAm_102 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vākyeṣvanekāntadyotī gamyaṃ prati viśeṣakaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "syānnipāto 'rthayogitvāttava kevalināmapi // SAm_103 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "syādvādaḥ sarvathaikāntayogātkiṃvṛttacidvidhiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saptabhaṅganayāpekṣo heyādeyaviśeṣakaḥ // SAm_104 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "syādvādakevalajñāne sarvatattvaprakāśane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhedaḥ sākṣādasākṣācca hyavastvanyatamaṃ bhavet // SAm_105 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sadharmaṇaiva sādhyasya sādharmyādavirodhataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "syādvādapravibhaktārthaviśeṣavyañjako nayaḥ // SAm_106 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nayopanayaikāntānāṃ trikālānāṃ samuccayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avibhrāḍ bhāvasambandho dravyamekamanekadhā // SAm_107 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mithyāsamūho mithyā cenna mithyaikāntatāsti naḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nirapekṣā nayā mithyā sāpekṣā vastu te 'rthakṛt // SAm_108 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "niyamyate 'rtho vākyena vidhinā vāraṇena vā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathānyathā ca so 'vaśyamaviśeṣyatvamanyathā // SAm_109 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tadatadvastu vāgeṣā tadevetyanuśāsati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na satyā syānmṛṣāvākyaiḥ kathaṃ tattvārthadeśanā // SAm_110 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vāksvabhāvo 'nyavāgarthapratiṣedhaniraṅkuśaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āha ca svārthasāmānyaṃ tādṛgvācyaṃ khapuṣpavat // SAm_111 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānyavāgviśeṣe cenna śabdārthā mṛṣā hi sā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "abhipretaviśeṣāpteḥ syātkāraḥ satyalāñchanaḥ // SAm_112 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vidheyamīpsitārthāṅgaṃ pratiṣedhavirodhi yat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathaivādeyaheyatvamiti syādvādasaṃsthitiḥ // SAm_113 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "itīyamāptamimāṃsā vihitā hitamicchatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "samyaṅmithyopadeśārthaviśeṣapratipattaye // SAm_114 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jayati jagati kleśāveśaprapaṃcahimāṃśumān"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vihitaviṣamaikāntadhvāntapramāṇanayāṃśumān /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yatipatirajo yasyādhṛṣṭānmatāmbunidherlavān"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svamatamatayastīrthyā nānā pare samupāsate // SAm_115 //"
        }
      ]
    }
  ]
}