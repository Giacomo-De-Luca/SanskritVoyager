  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "Ekaviṃśatistotra"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas tāre ture vīre kṣaṇadyutinibhekṣaṇe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "trailokyanāthavaktrābjavikasatkesarodbhave // Nevs_1"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas śāntasaraccandrasaṃpūrṇapaṭalānane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tāre sahasravikalpaprahasatkiraṇojjvale // Nevs_2"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ kanakanīlābjapāṇipadmavibhūṣite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dānavīryatapaḥkṣāntititikṣādhyānagocare // Nevs_3"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas tathāgatoṣṇīṣavijayānantacāriṇi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aśeṣapāramitāprāptajinaputraniṣevite // Nevs_4"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas tuttārahūṃkārapūritāśādigantare /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saptalokakramākrāntā aśeṣākarṣaṇakṣaṇe // Nevs_5"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ śakranarabrahmamarudviśveśvarārcite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhūtavetālagandharvagaṇapuraskṛte // Nevs_6"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas tratritriphaṭkāre paramantrapramardani /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyālīḍhapādanyāse śikhijvālākulojjvale // Nevs_7"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas ture mahāghore māravīravināśani /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhṛkuṭīkṛtavaktrābjasarvaduṣṭanisūdani // Nevs_8"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas triratnamudrāṅke hṛdyāṅgulivibhūṣite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhūṣitāśeṣadikcakranikare sukulākule // Nevs_9"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ pramuditāṭopamukuṭākṣiptasāriṇi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hasatprahasattuttāre māralokabhayaṃkari // Nevs_10"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ samantabhūpālāpātālākarṣaṇakṣaṇe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhṛkuṭikṛtahūṃkāre sarvāpadavimocani // Nevs_11"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ śikhaṇḍakhaṇḍendumukuṭābharaṇojjvale /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "amitābhatathābhāre bhāsvare kiraṇadhruve // Nevs_12"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ karatalāghātacaraṇāhatabhūtale /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhṛkuṭikṛtahūṃkārasaptapātālanāśini // Nevs_13"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ kalpāntahutabhugjvālāmālāntare sthite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ālīḍhamuditābaddharipucakravināśini // Nevs_14"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ śive śubhe śānte śāntanirvāṇagocare /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svāhā praṇamya saṃyukte mahāpātakanāśini // Nevs_15"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ pramuditābaddharipugātraprabhedani /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "daśākṣarapādanyāse vidyāhuṃkāradīpite // Nevs_16"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas ture pādāghāte huṃkārakārajīvite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "merumaṇḍalakailāśabhūvanatrayacāraṇi // Nevs_17"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ surāsarākārahariṇīkakare sthite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "haradviruktaphaṭkāra aśeṣaviśanāśini // Nevs_18"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaḥ suragaṇayakṣāsurakinnarasevite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ābaddhamuditābhogakari duḥsvapnanāśini // Nevs_19"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namaś candrārkasaṃpūrṇa nayanadyutisvabhāsvare /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tāra dviruktottāre viṣamajvalanāśini // Nevs_20"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "namas tritalavinyāse śivaśaktisamanvite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "grahavetālayakṣādyanāśani pravare ture // Nevs_21"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mantramūlam idaṃ stotraṃ namaskāraikaviṃśati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yaḥ paṭhet prāyaśo dhīmān devyā bhaktisamanvitaḥ // 1"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "so 'yaṃ vā prātar utthāya smaret sarvābhayapradaṃ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvapāpapraśamanaṃ sarvadurgatināśanam // 2"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abhiṣiktobhaya tūrṇaṃ asmin mahattām āsādya /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viṣaṃ tasya mahāghoraṃ smaraṇāt pralayaṃ yānti // 3"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "grahajvalaviṣārtānām anyeṣāṃ caiva satvānām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "putrakāmo labhet putraṃ sarvakāmān avāpnoti // 4"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saptābhir jinakoṭibhiḥ so 'nte bauddhapadaṃ vrajet /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sthāvaraṃ vātha jaṅgamaṃ sāḍ idaṃ pīḍam eva ca // 5"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "paramārtivināśanaṃ dvitrisaptābhivartinām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dhanakāmo labhed dhanaṃ na vighnaiḥ pratihanyate // 6"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iti śrīsaṃyaksaṃbuddhavairocanabhāṣitaṃ bhagavatyāryatāradevyā namaskāraikaviṃśatistotraṃ saṃpūrṇaṃ samāptaṃ // śubham //"
    }
  ]
}