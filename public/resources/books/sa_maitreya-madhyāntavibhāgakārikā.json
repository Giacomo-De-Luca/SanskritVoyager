  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "Āryamaitreyapraṇītā Madhyāntavibhāgakārikā (Mvk)"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "Lakṣaṇaparicchedaḥ prathamaḥ"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "lakṣaṇaṃ hyāvṛtistattvaṃ pratipakṣasya bhāvanā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatrāvasthā phalaprāptiryānānuttaryameva ca // Mvk_1.1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abhūtaparikalpo 'sti dvayaṃ tatra na vidyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śūnyatā vidyate tvatra tasyāmapi sa vidyate // Mvk_1.2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na śūnyaṃ nāpi cāśūnyaṃ tasmāt sarvaṃ vidhīyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sattvādasattvāt sattvācca madhyamā pratipacca sā // Mvk_1.3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "arthasattvātmavijñaptipratibhāsaṃ prajāyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñānaṃ nāsti cāsyārthastadabhāvāttadapyasat // Mvk_1.4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abhūtaparikalpatvaṃ siddhamasya bhavatyataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na tathā sarvathābhāvāt tatkṣayānmuktiriṣyate // Mvk_1.5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kalpitaḥ paratantraśca pariniṣpanna eva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "arthādabhūtakalpācca dvayābhāvācca deśitaḥ // Mvk_1.6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "upalabdhiṃ samāśritya nopalabdhiḥ prajāyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nopalabdhiṃ samāśritya nopalabdhiḥ prajāyate // Mvk_1.7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "upalabdhestataḥ siddhā nopalabdhisvabhāvatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmācca samatā jñeyā nopalambhopalambhayoḥ // Mvk_1.8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abhūtaparikalpaśca cittacaittāstridhātukāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatrārthadṛṣṭirvijñānaṃ tadviśeṣe tu caitasāḥ // Mvk_1.9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "evaṃ pratyayavijñānaṃ dvitīyaṃ caupabhogikam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "upabhogaparicchedaprerakāstatra caitasāḥ // Mvk_1.10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "chādanādropaṇāccaiva nayanātsaṃparigrahāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pūraṇāt triparicchedādupabhogācca karṣaṇāt // Mvk_1.11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nibandhanādābhimakhyād duḥkhanāt kliśyate jagat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tredhā dvedhā ca saṃkleśaḥ saptadhābhūtakalpanāt // Mvk_1.12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "lakṣaṇaṃ cātha paryāyastadartho bheda eva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sādhanañceti vijñeyaṃ śūnyatāyāḥ samāsataḥ // Mvk_1.13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dvayābhāvo hyabhāvasya bhāvaḥ śūnyasya lakṣaṇam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na bhāvo nāpi cābhāvao na pṛthaktvaikalakṣaṇam // Mvk_1.14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tathatā bhūtakoṭiścānimittaṃ paramārthatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmadhātuśca paryāyāḥ śūnyatāyāḥ samāsataḥ // Mvk_1.15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ananyathāviparyāsatannirodhāryagocaraiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetutvāccāryadharmaṇāṃ paryāyārtho yathākramam // Mvk_1.16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃkliṣṭā ca viśuddhā ca samalā nirmalā ca sā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "abdhātukanakākāśaśuddhivacchuddhiriṣyate // Mvk_1.17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhoktṛbhojanataddhehapratiṣṭhāvastuśūnyatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tacca yena yathā dṛṣṭaṃ yadarthaṃ tasya śūnyatā // Mvk_1.18 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śubhadvayasya prāptyartha sadā sattvahitāya ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃsārātyajanārthaṃ ca kuśalasyākṣayāya ca // Mvk_1.19 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "gotrasya ca viśuddhayarthaṃ lakṣaṇavyañjanāptaye /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śuddhaye bu[ddha]dharmāṇāṃ bodhisattvaḥ prapadyate // Mvk_1.20 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pudgalasyātha dharmāṇāmabhāvaḥ śūnyatātra hi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tadabhāvasya sadbhāvastasmin sā śūnyatāparā // Mvk_1.21 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃkliṣṭā cedbhavennāsau muktāssyuḥ sarvadehinaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśuddhā cedbhavennāsau vyāyāmo niṣphalo bhavet // Mvk_1.22 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na kliṣṭā nāpi va 'ākliṣṭā śuddhāśuddhā na caiva sā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prabhāsvaratvāccittasya kleśasyāgantukatvataḥ // Mvk_1.23 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// iti lakṣaṇaparicchedaḥ prathamaḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "Āvaraṇaparicchedo dvitīyaḥ"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vyāpi prādeśikodriktasamādānavivarjanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dvayāvaraṇamākhyātaṃ navadhā kleśalakṣaṇam // Mvk_2.1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃyojanānyāvaraṇamudvegasamupekṣayoḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvadṛṣṭeśca satkāyadṛṣṭestadvastuno 'pi ca // Mvk_2.2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nirodhamārgaratneṣu lābhasatkāra eva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṅkleśasya parijñāne śubhādau daśadhāparam // Mvk_2.3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aprayogo 'nāyatane 'yogavihitaśca yaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "notpattiramanaskāraḥ sambhārasyāprapūrṇatā // Mvk_2.4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "gotramitrasya vaidhuryaṃ cittasya parikheditā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratipatteśca vaidhuryaṃ kuduṣṭajanavāsatā // Mvk_2.5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dauṣṭhulyamavaśiṣṭatvaṃ trayāt prajñāvipakvatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prakṛtyā caiva dauṣṭhulyaṃ kausīdyaṃ ca pramāditā // Mvk_2.6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saktirbhave ca bhoge ca līnacittatvameva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aśraddhānadhimuktiśca yathārutavicāraṇā // Mvk_2.7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saddharme 'gauravaṃ lābhe gurutākṛpatā tathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śrutavyasanamalpatvaṃ samādhyaparikarmitā // Mvk_2.8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śubhaṃ bodhiḥ samādānaṃ dhīmattvābhrāntyanāvṛtī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nṛtyatrāso 'matsaritvaṃ vaśitvañca śubhādayaḥ // Mvk_2.9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "trīṇi trīṇi ca eteṣāṃ jñeyānyāvaraṇāni hi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pakṣyapāramitābhūmiṣvanyadāvaraṇaṃ punaḥ // Mvk_2.10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vastvakauśalakausīdyaṃ samādherdvayahīnatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aropaṇātha daurbalyaṃ dṛṣṭidauṣṭhulyaduṣṭatā // Mvk_2.11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aiśvaryasyātha sugateḥ sattvātyāgasya cāvṛtiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hānivṛddhyośca doṣāṇāṃ guṇānāmavatāraṇe // Mvk_2.12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vimocane 'kṣayatve ca nairantarye śubhasya ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "niyatīkaraṇe dharmasambhogaparipācane // Mvk_2.13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvatragārthe agrārthe niṣyandāgrārtha eva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "niṣparigrahatārtha ca santānābheda eva ca // Mvk_2.14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nissaṅkleśaviśuddhyarthe 'nānātvārtha eva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ahīnānadhikārthe ca caturdhāvaśitāśraye // Mvk_2.15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmadhātāvavidyeyamakliṣṭā daśadhāvṛtiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "daśabhūmivipakṣeṇa pratipakṣāstu bhūmayaḥ // Mvk_2.16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kleśāvaraṇamākhyātaṃ jñeyāvaraṇameva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvāṇyāvaraṇānīha yatkṣayānmuktiriṣyate // Mvk_2.17 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// ityāvaraṇaparicchedo dvitīyaḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "Tattvaparicchedastṛtīyaḥ"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mūlalakṣaṇatattvamaviparyāsalakṣaṇam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "phalahetumayaṃ tattvaṃ sūkṣmaudārikameva ca // Mvk_3.1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prasiddhaṃ śuddhiviṣayaṃ saṅgrāhyaṃ bhedalakṣaṇam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kauśalyatattvaṃ daśadhā ātmadṛṣṭivipakṣataḥ // Mvk_3.2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svabhāvastrividho 'sacca nityaṃ saccāpyatattvataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sadasattattvataśceti svabhāvatrayamiṣyate // Mvk_3.3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "samāropāpavādasya dharmapudgalayoriha /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "grāhyagrāhakayoścāpi bhāvābhāve ca darśanam // Mvk_3.4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yajjñānānna pravarteta taddhi tattvasya lakṣaṇam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asadartho hyanityārtha utpādadavyayalakṣaṇaḥ // Mvk_3.5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "samalāmalabhāvena mūlatattve yathākramam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "duḥkhamādānalakṣmākhyaṃ sambandhenāparaṃ matam // Mvk_3.6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abhāvaścāpyatadbhāvaḥ prakṛtiḥ śūnyatā matā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "alakṣaṇañca nairātmyaṃ tadvilakṣaṇameva ca // Mvk_3.7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svalakṣaṇañca nirdiṣṭaṃ duḥkhasatyamato matam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vāsanātha samutthānamavisaṃyoga eva ca // Mvk_3.8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svabhāvadvayanotpattirmalaśāntidvayaṃ matam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "parijñāyāṃ prahāṇe ca prāptisākṣātkṛtāvayam // Mvk_3.9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mārgasatyaṃ samākhyātaṃ prajñaptipratipattitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathodbhāvanayaudāraṃ paramārthantu ekataḥ // Mvk_3.10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "arthaprāptiprapattyā hi paramārthastridhā mataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nirvikārāviparyāsapariniṣpattito dvayam // Mvk_3.11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "lokaprasiddhamekasmāt trayād yuktiprasiddhakam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśuddhagocaraṃ dvedhā ekasmādeva kīrtitam // Mvk_3.12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nimittasya vikalpasya nāmnaśca dvayasaṅgrahaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "samyagjñānasatattvasya ekenaiva ca saṅgrahaḥ // Mvk_3.13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pravṛttitattvaṃ dvividhaṃ sanniveśakupannatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ekaṃ lakṣaṇavijñaptiśuddhisamyakprapannatā // Mvk_3.14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ekahetutvabhoktṛtvakartṛtvavaśavartane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ādhipatyārthanityatve kleśaśuddhyāśraye 'pi ca // Mvk_3.15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yogitvāmuktamuktatve ātmadarśanameṣu hi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "parikalpavikalpārthadharmatārthena teṣu te // Mvk_3.16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anekatvābhisaṅkṣepaparicchedārtha āditaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "grāhakagrāhyatadgrāhabījārthaścāparo mataḥ // Mvk_3.17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "veditārthaparicchedabhogāyadvārato 'param /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "punarhetuphalāyāsānāropānapavādataḥ // Mvk_3.18 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aniṣṭeṣṭaviśuddhīnāṃ samotpattyādhipatyayoḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "samprāptisamudācārapāratantryārthato 'param // Mvk_3.19 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "grahaṇasthānasandhānabhogaśuddhidvayārthataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "phalahetūpayogārthanopayogāttathāparam // Mvk_3.20 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vedanāsanimittārthatannimittaprapattitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tacchamapratipakṣārthayogādaparamiṣyate // Mvk_3.21 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "guṇadoṣāvikalpena jñānena parataḥ svayam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "niryāṇādaparaṃ jñeyaṃ saprajñaptisahetukāt // Mvk_3.22 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "nimittāt praśamāt sārthāt paścimaṃ samudāhṛtam //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// iti tattvaparicchedastṛtīyaḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "Pratipakṣabhāvanāvasthāphalaparicchedaścaturthaḥ"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dauṣṭhulyāt tarṣahetutvād vastutvādavimohataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "catussatyāvatārāya smṛtyupasthānabhāvanā // Mvk_4.1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "parijñāte vipakṣe ca pratipakṣe ca sarvathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tadapāyāya vīryaṃ hi caturdhā sampravartate // Mvk_4.2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "karmaṇyatā sthitestatra sarvārthānāṃ samṛddhaye /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pañcadoṣaprahāṇāṣṭasaṃskārāsevanānvayā // Mvk_4.3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kausīdyamavavādasya sammoṣo laya uddhavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asaṃskāro 'tha saṃskāraḥ pañca doṣā ime matāḥ // Mvk_4.4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āśrayo 'thāśritastasya nimittaṃ phalameva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ālambane 'sammoṣo layauddhatyānubuddhyanā // Mvk_4.5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tadapāyābhisaṃskāraḥ śāntau praśaṭhavāhitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ropite mokṣabhāgīye cchandayogādhipatyataḥ // Mvk_4.6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ālambane 'sammoṣāvisāravicayasya ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vipakṣasya hi saṃlekhāt pūrvasya phalamuttaram // Mvk_4.7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dvau dvau nirvedhabhāgīyāvindriyāṇi balāni ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āśrayāṅgaṃ svabhāvāṅgaṃ niryāṇāṅgaṃ tṛtīyakam // Mvk_4.8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "caturthamanuśaṃsāṅgaṃ niḥkleśāṅgaṃ tridhā matam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nidānenāśrayeṇeha svabhāvena ca deśitam // Mvk_4.9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "paricchedo 'tha samprāptiḥ parasambhāvanā tridhā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vipakṣapratipakṣaśca mārgasyāṅgaṃ tadaṣṭadhā // Mvk_4.10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛṣṭau śīle 'tha saṃlekhe paravijñaptiriṣyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kleśopakleśavaibhutvavipakṣapratipakṣatā // Mvk_4.11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anukūlā viparyastā sānubandhā viparyayā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aviparyastaviparyāsānanubandha ca bhāvanā // Mvk_4.12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ālambanamanaskāraprāptitastad viśiṣṭatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hetvavasthāvatārākhyā prayogaphalasaṃjñitā // Mvk_4.13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāryākāryaviśiṣṭā ca uttarānuttarā ca sā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "adhimuktau praveśe ca niryāṇe vyākṛtāvapi // Mvk_4.14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kathikatve 'bhiṣeke ca samprāptāvanuśaṃsane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kṛtyānuṣṭhāna uddiṣṭā dharmādhātau tridhā punaḥ // Mvk_4.15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aśuddhāśuddhaśuddhā ca viśuddhā ca yathārthataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pudgalānāṃ vyavasthānaṃ yathāyogamato matam // Mvk_4.16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhājanatvaṃ vipākākhyaṃ balantasyādhipatyataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "rucirvṛddhirviśuddhiśca phalametad yathākramam // Mvk_4.17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "uttarottaramādyañca tadabhyāsat samāptitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ānukūlyād vipakṣācca visaṃyogād viśeṣataḥ // Mvk_4.18 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "uttarānuttaratvācca phalamanyat samāsataḥ /"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// iti pratipakṣabhāvanādiparicchedaścaturthaḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "5 Yānānuttaryaparicchedaḥ pañcamaḥ"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ānuttaryaṃprapattau hi punarālambane matam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "samudāgama uddiṣṭaṃ pratipattistu ṣaḍ vidhā // Mvk_5.1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "paramātha manaskāre anudharme 'ntavarjane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśiṣṭā cāviśiṣṭā ca paramā dvādaśātmikā // Mvk_5.2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "audāryamāyatatvañca adhikaro 'kṣayātmatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nairantaryamakṛcchratvaṃ vittatvañca parigrahaḥ // Mvk_5.3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ārambhaprāptiniṣyandaniṣpattiḥ paramā matā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tataśca paramārthena daśa pāramitā matāḥ // Mvk_5.4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dānaṃ śīlaṃ kṣamā vīryaṃ dhyānaṃ prajñā upāyatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "praṇidhānaṃ balaṃ jñānametāḥ pāramitā daśa // Mvk_5.5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anugraho 'vighātaśca karma tasya ca marṣaṇam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "guṇavṛddhiśca sāmarthyamavatāravimocane // Mvk_5.6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "akṣayatvaṃ sadā vṛttirniyataṃ bhogapācane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yathāprajñaptito dharmamahāyānamanaskriyā // Mvk_5.7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bodhisattvasya satataṃ prajñayā triprakārayā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dhātupuṣṭayaipraveśāya cārthasiddhyai bhavatyasau // Mvk_5.8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃyuktā dharmacaritaiḥ sā jñeyā daśabhiḥ punaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "lekhanā pūjanā dānaṃ śravaṇaṃ vācanod grahaḥ // Mvk_5.9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prakāśanātha svādhyāyaścintanā bhāvanā ca tat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ameyapuṇyaskandhaṃ hi caritaṃ tad daśātmakam // Mvk_5.10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "viśeṣādakṣayatvācca parānugrahato 'śamāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avikṣiptāviparyāsapraṇatā cānudhārmikī // Mvk_5.11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vyutthānaṃ viṣaye sārastathāsvādalayoddhavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sambhāvanābhisandhiśca manaskāre 'pyahaṃkṛtiḥ // Mvk_5.12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hīnacittañca vikṣepaḥ parijñeyo hi dhīmatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vyañjanārthamanaskāre 'visāre lakṣaṇadvaye // Mvk_5.13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aśuddhaśuddhāvāgantukatve 'trāsitānunnatau /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃyogāt saṃstavāccaiva viyogādapyasaṃstavāt // Mvk_5.14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "arthasattvamasattvañca vyañjane so 'viparyayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dvayena pratibhāsatvaṃ tathā cāvidyamānatā // Mvk_5.15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "arthe sa cāviparyāsaḥ sadasattvena varjitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tajjalpabhāvito jalpamanaskārastadāśrayaḥ // Mvk_5.16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "manaskāre 'viparyāso dvayaprakhyānakāraṇe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyādivadasattvañca sattvañcārthasya tanmatam // Mvk_5.17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "so 'visāre 'viparyāso bhāvābhāvāvisārataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvasya nāmamātratvaṃ sarvakalpāpravṛttaye // Mvk_5.18 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svalakṣaṇe 'viparyāsaḥ paramārthe svalakṣaṇe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmadhātuvinirmukto yasmād dharmo na vidyate // Mvk_5.19 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānyalakṣaṇantasmāt sa ca tatrāviparyayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viparyastamanaskārāvihāniparihāṇitaḥ // Mvk_5.20 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tadaśuddhirviśuddhiśca sa ca tatrāviparyayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmadhātorviśuddhatvāt prakṛtyā vyomavatpunaḥ // Mvk_5.21 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dvayasyāgantukatvaṃ hi sa ca tatrāviparyayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃkleśaśca viśuddhiśca dharmapudgalayorna hi // Mvk_5.22 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "asattvāt trāsatāmānau nātaḥ so 'trāviparyayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pṛthaktvaikatvamantaśca tīrthyaśrāvakayorapi // Mvk_5.23 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "samāropāpavādānto dvidhā pudgaladharmayoḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vipakṣapratipakṣāntaḥ śāśvatocchedasaṃjñitaḥ // Mvk_5.24 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "grāhyagrāhakasaṃkleśavyavadāne dvidhā tridhā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vikalpadvayatāntaśca sa ca saptavidho mataḥ // Mvk_5.25 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvābhāve praśāmye 'tha śamane trāsyatadbhaye /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "grāhyagrāhe 'tha samyaktvamithyātve vyāpṛtau na ca // Mvk_5.26 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ajanmasamakālatve sa vikalpadvayāntatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśiṣṭā cāviśiṣṭā ca jñeyā daśasu bhūmiṣu // Mvk_5.27 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vyavasthānantato dhātuḥ sādhyasādhanadhāraṇā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avadhārapradhārā ca prativedhaḥ pratānatā // Mvk_5.28 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pragamaḥ praśaṭhatvañca prakarṣālambanammatam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "avaikalyāpratikṣepo 'vikṣepaśca prapūraṇā // Mvk_5.29 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "samutpādo nirūḍhiśca karmaṇyatvāpratiṣṭhitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nirāvaraṇatā tasyāpraśrabdhisamudāgamaḥ // Mvk_5.30 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śāstraṃ madhyavibhāgaṃ hi gūḍhasārārthameva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "mahārthañcaiva sarvārthaṃ sarvānarthapraṇodanam // Mvk_5.31 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "// iti yānānuttarya paricchedaḥ pañcamaḥ //"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "samāptā madhyāntavibhāgakārikāḥ"
    }
  ]
}