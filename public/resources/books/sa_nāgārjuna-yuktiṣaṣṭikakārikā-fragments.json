  "body": [
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "astināstivyatikrāntā buddhir yeṣāṃ nirāśrayā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "gambhīras tair nirālambaḥ pratyayārtho vibhāvyate || NagYsk_01"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "..."
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃsāraṃ caiva nirvāṇaṃ manyante 'tattvadarśinaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na saṃsāraṃ na nirvāṇaṃ manyante tattvadarśinaḥ || NagYsk_05"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nirvāṇaṃ ca bhavaś caiva dvayam etan na vidyate |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "parajñānaṃ bhavasyaiva nirvāṇam iti kathyate || NagYsk_06"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "..."
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tat tat prāpya yad utpannaṃ notpannaṃ tat svabhāvataḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svabhāvena yan notpannam utpannaṃ nāma tat katham || NagYsk_19"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "..."
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvam astīti vaktavyam ādau tattvagaveṣiṇaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "paścād avagatārthasya niḥsaṅgasya viviktatā || NagYsk_30"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "..."
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mamety aham iti proktaṃ yathā kāryavaśāj jinaiḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathā kāryavaśāt proktāḥ skandhāyatanadhātavaḥ || NagYsk_33"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mahābhūtādi vijñāne proktaṃ samavarudhyate |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tajjñāne vagamaṃ yāti nanu mithyā vikalpitam || NagYsk_34"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "..."
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetutaḥ saṃbhavo yasya sthitir na pratyayair vinā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vigamaḥ pratyayābhāvāt so 'stīty avagataḥ katham || NagYsk_39"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "..."
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "rāgadveṣodbhavas tīvraduṣṭadṛṣṭiparigrahaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vivādās tatsamutthāś ca bhāvābhyupagame sati || NagYsk_46"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sa hetuḥ sarvadṛṣṭīnāṃ kleśotpattir na taṃ vinā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmāt tasmin parijñāte dṛṣṭikleśaparikṣayaḥ || NagYsk_47"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "parijñā tasya keneti pratītyotpādadarśanāt |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratītya jātaṃ cājātam āha tattvavidāṃ varaḥ || NagYsk_48"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "..."
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bālāḥ sajjanti rūpeṣu vairāgyaṃ yānti madhyamāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svabhāvajñā vimucyante rūpasyottamabuddhayaḥ || NagYsk_55"
        }
      ]
    },
