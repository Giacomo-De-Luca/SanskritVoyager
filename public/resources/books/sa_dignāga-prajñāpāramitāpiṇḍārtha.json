  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "Ācāryadignāgaviracitaḥ"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "Prajñāpāramitāpiṇḍārthaḥ (Pppa)"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "/ namaḥ prajñāpāramitāyai //"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāparamitā jñānamadvayaṃ sā tathāgataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sādhyā tādarthyayogena tācchabdyaṃ granthamārgayoḥ // Pppa_1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āśrayaścādhikāraśca karma bhāvanayā saha /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prabhedo liṅgamāpacca sānuśaṃsamudāhṛtam // Pppa_2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śradvāvatāṃ pravṛtyaṅgaṃ śāstā parṣacca sākṣiṇī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "deśakālau ca nirdiṣṭau svaprāmāṇyaprasiddhaye // Pppa_3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṅgītikartā loke hi deśakālopalakṣitam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sasākṣikaṃ vadan vaktā prāmāṇyamadhigacchati // Pppa_4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvaṃ caitannipātātmaśravaṇādeḥ prakīrtanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prāsaṅgikaṃ tu evārthā mukhyā dvātriṃśadeva hi // Pppa_5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prabhedaḥ ṣoḍaśākāraḥ śūnyatāyā yathākramam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nirdiṣṭo 'ṣṭāsahastrayā sa vijñeyo 'nyāpadeśataḥ // Pppa_6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "itthamaṣṭasahasrīyamanyūnārthairyathoditaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "granthasaṅkṣepa iṣṭo 'tra ta evārthā yathoditāḥ // Pppa_7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bodhisattvaṃ na paśyāmītyuktavāṃstattvato muniḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhoktādhyātmikavastūnāṃ kathitā tena śūnyatā // Pppa_8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "rūpaṃ rūpasvabhāvena śūnyamityuktitaḥ punaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bāhyānyāyatanānīha bhojyāni pratiṣiddhavān // Pppa_9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "rūpādyabhāve taddehapratiṣṭhālakṣaṇakṣatiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "gatārthā yena taddṛṣṭaṃ tadādhyātmikamityasat // Pppa_10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ādhyātmikānāṃ śūnyatve prakṛterapi śūnyatā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñānarūpaṃ gotraṃ hi kṛpāprajñātmakaṃ matam // Pppa_11 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "notpanno na niruddho vā sattva ityādinā sphuṭam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sattvasaṃsārayoḥ kāmaṃ darśitā tena śūnyatā // Pppa_12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "buddhadharmāṃstathā bodhisattvadharmānna paśyati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ityādinā vinirdiṣṭā śūnyā daśabalādayaḥ // Pppa_13 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prati prati yato dharmāḥ kalpitā iti kīrtitam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tato na paramārtho 'sti dharmāṇāmiti coktavān // Pppa_14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ātmādidṛṣṭerucchedaṃ mahatyā prakaroti yat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tataḥ pudgalanairātmyaṃ bhagavān sarvathā jagau // Pppa_15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvadharmā anutpannā iti kīrtayatā tathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kathitaṃ dharmanairātmyaṃ sarvathā tattvavedinā // Pppa_16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāvadyaniravadyānāmavṛddhiparihāṇitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃskṛtāsaṃskṛtānāṃ ca kuśalānāṃ nirākṛtiḥ // Pppa_17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kuśalānāṃ ca śūnyatve tadgatā akṣatā tathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kalpitaiveti bhedānāṃ śūnyatāyāḥ sa saṅgrahaḥ // Pppa_18 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "daśabhiścittavikṣepaiścittaṃ vikṣiptamanyataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yogyaṃ bhavati bālānāṃ nādvayajñānasādhane // Pppa_19 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tānapākartumanyonyaṃ vipakṣapratipakṣataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitāgranthaste ca sampiṇḍaya darśitāḥ // Pppa_20 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yadāha bodhisattvaḥ sannityabhāvaprakalpanā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vikṣepaṃ vikṣipan śāstā sāṃvṛtaskandhadarśanāt // Pppa_21 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "etenāṣṭasahasryādāvādivākyātprabhṛtyapi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ā samāpterniṣeddhavyā vidhinābhāvakalpanā // Pppa_22 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetuvākyāni naitāni kṛtyamātraṃ tu sūcyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "brahmajālādisūtreṣu jñeyāḥ sarvatra yuktayaḥ // Pppa_23 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bodhisattvaṃ na paśyāmi ahamityādi vistaraiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nirākaroti bhagavān bhāvasaṅkalpavibhramam // Pppa_24 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yanna paśyati nāmāpi gocaraṃ[na] kriyāṃ tathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "skandhāṃśca sarvatastena bodhisattvaṃ na paśyati // Pppa_25 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kalpitasya niṣedho 'yamiti saṅgrahadarśanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvo jñeyatayārūḍha ākāraḥ kalpito matau // Pppa_26 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitāyāṃ hi trīn samāśritya deśanā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kalpitaṃ paratantraṃ ca pariniṣpannameva ca // Pppa_27 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nāstītyādipadaiḥ sarvaṃ kalpitaṃ vinivāryate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyopamādidṛṣṭāntaiḥ paratantrasya deśanā // Pppa_28 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "caturdhā vyavadānena pariniṣpannakīrtanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitāyāṃ hi nānyā buddhasya deśanā // Pppa_29 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "daśasaṅkalpavikṣepavipakṣe deśanākrame /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "trayāṇāmiha boddhavyaṃ samastavyastakīrtanam // Pppa_30 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yathādivākye niṣpannaparatantraparikalpitaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "abhāvakalpanārūpavikṣepavinivāraṇam // Pppa_31 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tena buddhaṃ tathā bodhiṃ na paśyāmīti vācakaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ā samāpteriha jñeyā kalpitānāṃ nirākṛtiḥ // Pppa_32 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śūnye rūpe svabhāvena samāropaḥ kva kena vā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ityanyeṣvapi vākyeṣu boddhavyaṃ tannivāraṇam // Pppa_33 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na hi śūnyatayā śūnyamiti vākyaṃ vinirdiśan /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "apavādavikalpānāṃ nirāsaṃ sarvathoktavān // Pppa_34 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "māyopamastathā buddhaḥ sa svapnopama ityapi /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ayameva kramo jñeyo vijñairvākyāntareṣvapi // Pppa_35 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmānādhikaraṇyena prokto māyopamo jinaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyopamādiśabdaiśca paratantro nigadyate // Pppa_36 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pṛthagjanānāṃ yajjñānaṃ prakṛtivyavadānikam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "uktaṃ tadbuddhaśabdena bodhisattvo yathā jinaḥ // Pppa_37 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nijaṃ svarūpaṃ pracchādya tadavidyāvaśīkṛtam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyāvadanyathā bhāti phalaṃ svapnam ivojhati // Pppa_38 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "advayasyānyathākhyātau phale vāpyapavādinām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "apavādavikalpānāmapavādo 'yamucyate // Pppa_39 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na rūpaṃ śūnyatā yuktā parasparavirodhataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nīrūpā śūnyatā nāmarūpamākārasaṅgatam // Pppa_40 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ityekatvavikalpasya bāddhā(dhyā?)nānātvakalpanam(nā?) /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ruṇāddhi nānyattadrūpaṃ śūnyatāyāṃ kathañcana // Pppa_41 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "asadeva yataḥ khyāti tadavidyāvinirmitam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "asatkhyāpanaśaktyaiva sāvidyeti nigadyate // Pppa_42 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "idamevocyate rūpaṃ prajñāpāramiteti ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "advayaṃ dvayamevaitadvikalpadvayabādhanam // Pppa_43 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yuktiṃ cāha viśuddhatvāttathā cānupalambhataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvābhāvavirodhācca nānātvamapi paśyati // Pppa_44 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nāmamātramidaṃ rūpaṃ tattvato hyasvabhāvakam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatsvabhāvavikalpānāmavakāśaṃ nirasyati // Pppa_45 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "rūpaṃ rūpasvabhāvena śūnyaṃ yatprathamoditam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatsvabhāvasamāropasaṅkalpapratiṣedhanam // Pppa_46 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "notpādaṃ na nirodhaṃ ca dharmāṇāṃ paśyatīti yat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhagavānāha, tadvayastā tadviśeṣasya kalpanā // Pppa_47 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kṛtrimaṃ nāma vācyāśca dharmāste kalpitā yataḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śabdārthayorna sambandhastena svābhāviko mataḥ // Pppa_48 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bāhyārthābhiniveśastu bhrāntyā bālasya jṛmbhate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathaiva vyavahāro 'yaṃ tvatrārtho 'sti kaścana // Pppa_49 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atra tena yathā nāma kalpyate na tathāsti tat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vācyaṃ vastu tato niṣṭhā yathānāmārthakalpanā // Pppa_50 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitā buddho bodhisattvo 'pi vā tathā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nāmamātramiti prāha vyasan satyārthakalpanam // Pppa_51 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śabdārthapratiṣedho 'yaṃ na vastu vinivāryate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "evamanyeṣvapi jñeyo vākyeṣvarthaviniścayaḥ // Pppa_52 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "naivopalabhate samyak sarvanāmāni tattvavit /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yathārthatvena tenedaṃ na dhvanervinivāraṇam // Pppa_53 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "subhūtistu dvayaṃ vyasan śabdaṃ śabdārthameva ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bodhisattvasya no nāma paśyāmīti sa uktavān // Pppa_54 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitāvākyaṃ nāsti yanneyatā gatam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ūhyāstu kevalaṃ te 'rthā[stadevaṃ]sūkṣmayā dhiyā // Pppa_55 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prakrāntārthatiraskāro yā cārthāntarakalpanā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitāyāṃ hi proktā sā prativarṇikā // Pppa_56 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "etāvānarthasaṅkṣepaḥ prajñāpāramitāśrayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āvartya(rta?)te sa evārthaḥ punararthāntarāśritaḥ // Pppa_57 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitāṃ samyak saṅgṛhyāṣṭasahasrikām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yatpuṇyamāptaṃ tenāstu prajñāpāramito janaḥ // Pppa_58 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "prajñāpāramitāpiṇḍārthasaṅgrahaḥ samāptaḥ /"
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "kṛtirācāryadignāgapādānām //"
    }
  ]
}