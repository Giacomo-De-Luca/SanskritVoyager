  "body": [
    {
      "tag": "p",
      "attributes": {},
      "text": "śrīlaṅkeśvaraviracitā śivastutiḥ /"
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "gale kalitakālima prakaṭitendu bhālasthale vināṭitajaṭotkaraṃ rucirapāṇipāthoruhe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "udañcitakapālakaṃ jaghanasīmni saṃdarśitadvipājinamanukṣaṇaṃ kimapi dhāma vandāmahe // LSiv_1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vṛṣopariparisphuraddhavaladhāma dhāmaśriyākuberagirigaurimaprabhavagarvanirvāsi tat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "rucirapunarumākucopacitakuṅkumai rañjitaṃ gajājinavirājitaṃ vṛjinabhaṅgabījaṃ bhaje // LSiv_2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "uditvaravilocanatrayavisṛtvarajyotiṣā kalākarakalākaravyatikareṇa cāharniśam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vikāsitajaṭāṭavīviharaṇotsavaprollasattarāmarataraṅgiṇītaralacūḍamīḍe mṛḍam // LSiv_3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vihāya kamalālayāvilasitāni vidyunnaṭīviḍambanapaṭūni me viharaṇaṃ vidhattāṃ manaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kapardini kumudvatīramaṇakhaṇḍacūḍāmaṇau kaṭītaṭapaṭībhavatkaraṭicarmaṇi brahmaṇi // LSiv_4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhavadbhavanadehalīvikaṭātuṇḍadaṇḍāhatitruṭanmukuṭakoṭibhirmaghavadādibhirbhūyate /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vrajema bhavadantikaṃ prakṛtimetya paiśācikīṃ kimityamarasaṃpadaḥ pramathanātha nāthāma he // LSiv_5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tvadarcanaparāyaṇapramathakanyakāluṇṭhitaprasūnasaphaladrumaṃ kamapi śailamāśāsmahe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "alaṃ taṭavitardikāśayitasiddhasīmantinīprakīrṇasumanomanoranameruṇā meruṇā // LSiv_6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na jātu hara yātu me viṣayadurvilāsaṃ mano manobhavakathāstu me na ca manorathātithyabhūḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sphuratsurataraṅgiṇītaṭakuṭīrakoṭau vasannaye śiva devāniśaṃ tava bhavāni pūjāparaḥ // LSiv_7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vibhūṣaṇasurāpagāśucitarālavālāvalīvaladbahalasīkaraprakarasekasaṃvardhitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "maheśvarasuradrumasphuritasajjaṭāmañjarī namajjanaphalapradā mama nu hanta bhūyādiyam // LSiv_8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bahirviṣayasaṃgatipratinivartitākṣāvaleḥ samādhikalitātmanaḥ paśupateraśeṣātmanaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śiraḥsurasarittaṭīkuṭilakalpakalpadrumaṃ niśākarakalāmahaṃ vaṭuvimṛṣyamāṇāṃ bhaje // LSiv_9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tvadīyasuravāhinīvimalavāridhārāvalajjaṭāgahanagāhinī matiriyaṃ mamākrāmatu /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "upottamasarittaṭīviṭapitāṭavīprollasattapasvipariṣattulāmamalamallikābha prabho // LSiv_10 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iti śrīlaṅkeśvaraviracitā śivastutiḥ samāptā /"
    }
  ]
}