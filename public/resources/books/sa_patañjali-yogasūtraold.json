    "publication_date": "2020-07-31",
    "source": "Kāśinātha Śāstrī Āgāśe: Vācaspatimiśraviracitaṭīkāsaṃvalitavyāsabhāṣyasametāni Pātañjalayogasūtrāṇi, tathā bhojadevaviracitarājamārtaṇḍābhidhavṛttisametāni pātañjalayogasūtrāṇi. sūtrapāṭhasūtravarṇānukramasūcībhyāṃ ca sanāthīkṛtāni ..."
  },
  "body": [
    {
      "tag": "l",
      "attributes": {},
      "text": "atha yogānuśāsanam || YS_1.1 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yogaś cittavṛttinirodhaḥ || YS_1.2 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadā draṣṭuḥ svarūpe 'vasthānam || YS_1.3 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vṛttisārūpyam itaratra || YS_1.4 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vṛttayaḥ pañcatayyaḥ kliṣṭākliṣṭāḥ || YS_1.5 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pramāṇaviparyayavikalpanidrāsmṛtayaḥ || YS_1.6 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pratyakṣānumānāgamāḥ pramāṇāni || YS_1.7 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viparyayo mithyājñānam atadrūpapratiṣṭham || YS_1.8 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śabdajñānānupātī vastuśūnyo vikalpaḥ || YS_1.9 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "abhāvapratyayālambanā vṛttir nidrā || YS_1.10 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anubhūtaviṣayāsaṃpramoṣaḥ smṛtiḥ || YS_1.11 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "abhyāsavairāgyābhyāṃ tannirodhaḥ || YS_1.12 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatra sthitau yatno 'bhyāsaḥ || YS_1.13 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sa tu dīrghakālanairantaryasatkārāsevito dṛḍhabhūmiḥ || YS_1.14 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dṛṣṭānuśravikaviṣayavitṛṣṇasya vaśīkārasaṃjñā vairāgyam || YS_1.15 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tat paraṃ puruṣakhyāter guṇavaitṛṣṇyam || YS_1.16 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vitarkavicārānandāsmitārūpānugamāt saṃprajñātaḥ || YS_1.17 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "virāmapratyayābhyāsapūrvaḥ saṃskāraśeṣo 'nyaḥ || YS_1.18 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bhavapratyayo videhaprakṛtilayānām || YS_1.19 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śraddhāvīryasmṛtisamādhiprajñāpūrvaka itareṣām || YS_1.20 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tīvrasaṃvegānām āsannaḥ || YS_1.21 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mṛdumadhyādhimātratvāt tato 'pi viśeṣaḥ || YS_1.22 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "īśvarapraṇidhānād vā || YS_1.23 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kleśakarmavipākāśayair aparāmṛṣṭaḥ puruṣaviśeṣa īśvaraḥ || YS_1.24 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatra niratiśayaṃ sarvajñabījam || YS_1.25 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pūrveṣām api guruḥ kālenānavacchedāt || YS_1.26 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasya vācakaḥ praṇavaḥ || YS_1.27 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tajjapas tadarthabhāvanam || YS_1.28 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tataḥ pratyakcetanādhigamo 'py antarāyābhāvaś ca || YS_1.29 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vyādhistyānasaṃśayapramādālasyāviratibhrāntidarśanālabdhabhūmikatvānavasthitatvāni cittavikṣepās te 'ntarāyāḥ || YS_1.30 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "duḥkhadaurmanasyāṅgamejayatvaśvāsapraśvāsā vikṣepasahabhuvaḥ || YS_1.31 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatpratiṣedhārtham ekatattvābhyāsaḥ || YS_1.32 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "maitrīkaruṇāmuditopekṣāṇāṃ sukhaduḥkhapuṇyāpuṇyaviṣayāṇāṃ bhāvanātaś cittaprasādanam || YS_1.33 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pracchardanavidhāraṇābhyāṃ vā prāṇasya || YS_1.34 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viṣayavatī vā pravṛttir utpannā manasaḥ sthitinibandhanī || YS_1.35 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viśokā vā jyotiṣmatī || YS_1.36 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vītarāgaviṣayaṃ vā cittam || YS_1.37 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "svapnanidrājñānālambanaṃ vā || YS_1.38 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yathābhimatadhyānād vā || YS_1.39 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "paramāṇuparamamahattvānto 'sya vaśīkāraḥ || YS_1.40 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kṣīṇavṛtter abhijātasyeva maṇer grahītṛgrahaṇagrāhyeṣu tatsthatadañjanatā samāpattiḥ || YS_1.41 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatra śabdārthajñānavikalpaiḥ saṃkīrṇā savitarkā samāpattiḥ || YS_1.42 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "smṛtipariśuddhau svarūpaśūnyevārthamātranirbhāsā nirvitarkā || YS_1.43 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etayaiva savicārā nirvicārā ca sūkṣmaviṣayā vyākhyātā || YS_1.44 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sūkṣmaviṣayatvaṃ cāliṅgaparyavasānam || YS_1.45 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tā eva sabījaḥ samādhiḥ || YS_1.46 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nirvicāravaiśāradye 'dhyātmaprasādaḥ || YS_1.47 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ṛtaṃbharā tatra prajñā || YS_1.48 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śrutānumānaprajñābhyām anyaviṣayā viśeṣārthatvāt || YS_1.49 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tajjaḥ saṃskāro 'nyasaṃskārapratibandhī || YS_1.50 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasyāpi nirodhe sarvanirodhān nirbījaḥ samādhiḥ || YS_1.51 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tapaḥsvādhyāyeśvarapraṇidhānāni kriyāyogaḥ || YS_2.1 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "samādhibhāvanārthaḥ kleśatanūkaraṇārthaś ca || YS_2.2 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "avidyāsmitārāgadveṣābhiniveśāḥ kleśāḥ || YS_2.3 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "avidyā kṣetram uttareṣāṃ prasuptatanuvicchinnodārāṇām || YS_2.4 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "anityāśuciduḥkhānātmasu nityaśucisukhātmakhyātir avidyā || YS_2.5 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dṛgdarśanaśaktyor ekātmatevāsmitā || YS_2.6 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sukhānuśayī rāgaḥ || YS_2.7 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "duḥkhānuśayī dveṣaḥ || YS_2.8 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "svarasavāhī viduṣo 'pi tathā rūḍho 'bhiniveśaḥ || YS_2.9 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "te pratiprasavaheyāḥ sūkṣmāḥ || YS_2.10 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dhyānaheyās tadvṛttayaḥ || YS_2.11 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kleśamūlaḥ karmāśayo dṛṣṭādṛṣṭajanmavedanīyaḥ || YS_2.12 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sati mūle tadvipāko jātyāyurbhogāḥ || YS_2.13 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "te hlādaparitāpaphalāḥ puṇyāpuṇyahetutvāt || YS_2.14 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pariṇāmatāpasaṃskāraduḥkhair guṇavṛttivirodhāc ca duḥkham eva sarvaṃ vivekinaḥ || YS_2.15 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "heyaṃ duḥkham anāgatam || YS_2.16 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "draṣṭṛdṛśyayoḥ saṃyogo heyahetuḥ || YS_2.17 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prakāśakriyāsthitiśīlaṃ bhūtendriyātmakaṃ bhogāpavargārthaṃ dṛśyam || YS_2.18 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viśeṣāviśeṣaliṅgamātrāliṅgāni guṇaparvāṇi || YS_2.19 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "draṣṭā dṛśimātraḥ śuddho 'pi pratyayānupaśyaḥ || YS_2.20 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadartha eva dṛśyasyātmā || YS_2.21 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kṛtārthaṃ prati naṣṭam apy anaṣṭaṃ tadanyasādhāraṇatvāt || YS_2.22 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "svasvāmiśaktyoḥ svarūpopalabdhihetuḥ saṃyogaḥ || YS_2.23 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasya hetur avidyā || YS_2.24 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadabhāvāt saṃyogābhāvo hānaṃ taddṛśeḥ kaivalyam || YS_2.25 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vivekakhyātir aviplavā hānopāyaḥ || YS_2.26 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasya saptadhā prāntabhūmiḥ prajñā || YS_2.27 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yogāṅgānuṣṭhānād aśuddhikṣaye jñānadīptir ā vivekakhyāteḥ || YS_2.28 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "yamaniyamāsanaprāṇāyāmapratyāhāradhāraṇādhyānasamādhayo 'ṣṭāv aṅgāni || YS_2.29 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ahiṃsāsatyāsteyabrahmacaryāparigrahā yamāḥ || YS_2.30 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "jātideśakālasamayānavacchinnāḥ sārvabhaumā mahāvratam || YS_2.31 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śaucasaṃtoṣatapaḥsvādhyāyeśvarapraṇidhānāni niyamāḥ || YS_2.32 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vitarkabādhane pratipakṣabhāvanam || YS_2.33 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vitarkā hiṃsādayaḥ kṛtakāritānumoditā lobhakrodhamohapūrvakā mṛdumadhyādhimātrā duḥkhājñānānantaphalā iti pratipakṣabhāvanam || YS_2.34 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ahiṃsāpratiṣṭhāyāṃ tatsaṃnidhau vairatyāgaḥ || YS_2.35 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "satyapratiṣṭhāyāṃ kriyāphalāśrayatvam || YS_2.36 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "asteyapratiṣṭhāyāṃ sarvaratnopasthānam || YS_2.37 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "brahmacaryapratiṣṭhāyāṃ vīryalābhaḥ || YS_2.38 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "aparigrahasthairye janmakathaṃtāsaṃbodhaḥ || YS_2.39 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śaucāt svāṅgajugupsā parair asaṃsargaḥ || YS_2.40 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sattvaśuddhisaumanasyaikāgryendriyajayātmadarśanayogyatvāni ca || YS_2.41 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃtoṣād anuttamaḥ sukhalābhaḥ || YS_2.42 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāyendriyasiddhir aśuddhikṣayāt tapasaḥ || YS_2.43 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "svādhyāyād iṣṭadevatāsaṃprayogaḥ || YS_2.44 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "samādhisiddhir īśvarapraṇidhānāt || YS_2.45 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sthirasukham āsanam || YS_2.46 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prayatnaśaithilyānantasamāpattibhyām || YS_2.47 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tato dvandvānabhighātaḥ || YS_2.48 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasmin sati śvāsapraśvāsayor gativicchedaḥ prāṇāyāmaḥ || YS_2.49 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bāhyābhyantarastambhavṛttir deśakālasaṃkhyābhiḥ paridṛṣṭo dīrghasūkṣmaḥ || YS_2.50 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bāhyābhyantaraviṣayākṣepī caturthaḥ || YS_2.51 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tataḥ kṣīyate prakāśāvaraṇam || YS_2.52 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dhāraṇāsu ca yogyatā manasaḥ || YS_2.53 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "svaviṣayāsaṃprayoge cittasvarūpānukāra ivendriyāṇāṃ pratyāhāraḥ || YS_2.54 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tataḥ paramā vaśyatendriyāṇām || YS_2.55 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "deśabandhaś cittasya dhāraṇā || YS_3.1 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatra pratyayaikatānatā dhyānam || YS_3.2 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad evārthamātranirbhāsaṃ svarūpaśūnyam iva samādhiḥ || YS_3.3 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "trayam ekatra saṃyamaḥ || YS_3.4 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tajjayāt prajñālokaḥ || YS_3.5 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasya bhūmiṣu viniyogaḥ || YS_3.6 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "trayam antaraṅgaṃ pūrvebhyaḥ || YS_3.7 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad api bahiraṅgaṃ nirbījasya || YS_3.8 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vyutthānanirodhasaṃskārayor abhibhavaprādurbhāvau nirodhakṣaṇacittānvayo nirodhapariṇāmaḥ || YS_3.9 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tasya praśāntavāhitā saṃskārāt || YS_3.10 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sarvārthataikāgratayoḥ kṣayodayau cittasya samādhipariṇāmaḥ || YS_3.11 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tataḥ punaḥ śāntoditau tulyapratyayau cittasyaikāgratāpariṇāmaḥ || YS_3.12 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "etena bhūtendriyeṣu dharmalakṣaṇāvasthāpariṇāmā vyākhyātāḥ || YS_3.13 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śāntoditāvyapadeśyadharmānupātī dharmī || YS_3.14 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kramānyatvaṃ pariṇāmānyatve hetuḥ || YS_3.15 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pariṇāmatrayasaṃyamād atītānāgatajñānam || YS_3.16 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śabdārthapratyayānām itaretarādhyāsāt saṃkaras tatpravibhāgasaṃyamāt sarvabhūtarutajñānam || YS_3.17 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "saṃskārasākṣātkaraṇāt pūrvajātijñānam || YS_3.18 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pratyayasya paracittajñānam || YS_3.19 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na ca tat sālambanaṃ tasyāviṣayībhūtatvāt || YS_3.20 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāyarūpasaṃyamāt tadgrāhyaśaktistambhe cakṣuḥprakāśāsaṃprayoge 'ntardhānam || YS_3.21 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sopakramaṃ nirupakramaṃ ca karma tatsaṃyamād aparāntajñānam ariṣṭebhyo vā || YS_3.22 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "maitryādiṣu balāni || YS_3.23 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "baleṣu hastibalādīni || YS_3.24 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pravṛttyālokanyāsāt sūkṣmavyavahitaviprakṛṣṭajñānam || YS_3.25 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bhuvanajñānaṃ sūrye saṃyamāt || YS_3.26 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "candre tārāvyūhajñānam || YS_3.27 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "dhruve tadgatijñānam || YS_3.28 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nābhicakre kāyavyūhajñānam || YS_3.29 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kaṇṭhakūpe kṣutpipāsānivṛttiḥ || YS_3.30 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kūrmanāḍyāṃ sthairyam || YS_3.31 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "mūrdhajyotiṣi siddhadarśanam || YS_3.32 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prātibhād vā sarvam || YS_3.33 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "hṛdaye cittasaṃvit || YS_3.34 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sattvapuruṣayor atyantāsaṃkīrṇayoḥ pratyayāviśeṣo bhogaḥ parārthāt svārthasaṃyamāt puruṣajñānam || YS_3.35 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tataḥ prātibhaśrāvaṇavedanādarśāsvādavārtā jāyante || YS_3.36 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "te samādhāv upasargā vyutthāne siddhayaḥ || YS_3.37 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bandhakāraṇaśaithilyāt pracārasaṃvedanāc ca cittasya paraśarīrāveśaḥ || YS_3.38 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "udānajayāj jalapaṅkakaṇṭakādiṣv asaṅga utkrāntiś ca || YS_3.39 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "samānajayāj jvalanam || YS_3.40 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "śrotrākāśayoḥ saṃbandhasaṃyamād divyaṃ śrotram || YS_3.41 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kāyākāśayoḥ saṃbandhasaṃyamāl laghutūlasamāpatteś cākāśagamanam || YS_3.42 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "bahir akalpitā vṛttir mahāvidehā tataḥ prakāśāvaraṇakṣayaḥ || YS_3.43 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sthūlasvarūpasūkṣmānvayārthavattvasaṃyamād bhūtajayaḥ || YS_3.44 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tato 'ṇimādiprādurbhāvaḥ kāyasaṃpat taddharmānabhighātaś ca || YS_3.45 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "rūpalāvaṇyabalavajrasaṃhananatvāni kāyasaṃpat || YS_3.46 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "grahaṇasvarūpāsmitānvayārthavattvasaṃyamād indriyajayaḥ || YS_3.47 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tato manojavitvaṃ vikaraṇabhāvaḥ pradhānajayaś ca || YS_3.48 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sattvapuruṣānyatākhyātimātrasya sarvabhāvādhiṣṭhātṛtvaṃ sarvajñātṛtvaṃ ca || YS_3.49 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadvairāgyād api doṣabījakṣaye kaivalyam || YS_3.50 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sthānyupanimantraṇe saṅgasmayākaraṇaṃ punar aniṣṭaprasaṅgāt || YS_3.51 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kṣaṇatatkramayoḥ saṃyamād vivekajaṃ jñānam || YS_3.52 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "jātilakṣaṇadeśair anyatānavacchedāt tulyayos tataḥ pratipattiḥ || YS_3.53 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tārakaṃ sarvaviṣayaṃ sarvathāviṣayam akramaṃ ceti vivekajaṃ jñānam || YS_3.54 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sattvapuruṣayoḥ śuddhisāmye kaivalyam iti || YS_3.55 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "janmauṣadhimantratapaḥsamādhijāḥ siddhayaḥ || YS_4.1 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "jātyantarapariṇāmaḥ prakṛtyāpūrāt || YS_4.2 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nimittam aprayojakaṃ prakṛtīnāṃ varaṇabhedas tu tataḥ kṣetrikavat || YS_4.3 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "nirmāṇacittāny asmitāmātrāt || YS_4.4 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pravṛttibhede prayojakaṃ cittam ekam anekeṣām || YS_4.5 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatra dhyānajam anāśayam || YS_4.6 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "karmāśuklākṛṣṇaṃ yoginas trividham itareṣām || YS_4.7 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tatas tadvipākānuguṇānām evābhivyaktir vāsanānām || YS_4.8 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "jātideśakālavyavahitānām apy ānantaryaṃ smṛtisaṃskārayor ekarūpatvāt || YS_4.9 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tāsām anāditvaṃ cāśiṣo nityatvāt || YS_4.10 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "hetuphalāśrayālambanaiḥ saṃgṛhītatvād eṣām abhāve tadabhāvaḥ || YS_4.11 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "atītānāgataṃ svarūpato 'sty adhvabhedād dharmāṇām || YS_4.12 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "te vyaktasūkṣmā guṇātmānaḥ || YS_4.13 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "pariṇāmaikatvād vastutattvam || YS_4.14 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "vastusāmye cittabhedāt tayor vibhaktaḥ panthāḥ || YS_4.15 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na caikacittatantraṃ vastu tadapramāṇakaṃ tadā kiṃ syāt || YS_4.16 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "taduparāgāpekṣitvāc cittasya vastu jñātājñātam || YS_4.17 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "sadā jñātāś cittavṛttayas tatprabhoḥ puruṣasyāpariṇāmitvāt || YS_4.18 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "na tat svābhāsaṃ dṛśyatvāt || YS_4.19 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "ekasamaye cobhayānavadhāraṇam || YS_4.20 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "cittāntaradṛśye buddhibuddher atiprasaṅgaḥ smṛtisaṃkaraś ca || YS_4.21 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "citer apratisaṃkramāyās tadākārāpattau svabuddhisaṃvedanam || YS_4.22 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "draṣṭṛdṛśyoparaktaṃ cittaṃ sarvārtham || YS_4.23 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tad asaṃkhyeyavāsanābhiś citram api parārthaṃ saṃhatyakāritvāt || YS_4.24 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "viśeṣadarśina ātmabhāvabhāvanānivṛttiḥ || YS_4.25 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadā vivekanimnaṃ kaivalyaprāgbhāraṃ cittam || YS_4.26 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tacchidreṣu pratyayāntarāṇi saṃskārebhyaḥ || YS_4.27 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "hānam eṣāṃ kleśavad uktam || YS_4.28 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "prasaṃkhyāne 'py akusīdasya sarvathā vivekakhyāter dharmameghaḥ samādhiḥ || YS_4.29 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tataḥ kleśakarmanivṛttiḥ || YS_4.30 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tadā sarvāvaraṇamalāpetasya jñānasyānantyāj jñeyam alpam || YS_4.31 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "tataḥ kṛtārthānāṃ pariṇāmakramasamāptir guṇānām || YS_4.32 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "kṣaṇapratiyogī pariṇāmāparāntanirgrāhyaḥ kramaḥ || YS_4.33 ||"
    },
    {
      "tag": "l",
      "attributes": {},
      "text": "puruṣārthaśūnyānāṃ guṇānāṃ pratiprasavaḥ kaivalyaṃ svarūpapratiṣṭhā vā citiśaktir iti || YS_4.34 ||"
    }
  ]
}