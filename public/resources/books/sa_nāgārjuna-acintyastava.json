  "body": [
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pratītyajānāṃ bhāvānāṃ naiḥsvābhāvyaṃ jagāda yaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "taṃ namāmy asamajñānam acintyam anidarśanam || NagAst_01"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yathā tvayā mahāyāne dharmanairātmyam ātmanā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viditaṃ deśitaṃ tadvad dhīmadbhyaḥ karuṇāvaśāt || NagAst_02"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pratyayebhyaḥ samutpannam anutpannaṃ tvayoditam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svabhāvena na taj jātam iti śūnyaṃ prakāśitam || NagAst_03"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yadvac chabdaṃ pratītyeha pratiśabdasamudbhavaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyāmarīcivac cāpi tathā bhavasamudbhavaḥ || NagAst_04"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "māyāmarīcigandharvanagarapratibimbakāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yady ajātāḥ saha svapnair na syāt taddarśanādikam || NagAst_05"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetupratyayasaṃbhūtā yathaite kṛtakāḥ smṛtāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tadvat pratyayajaṃ viśvaṃ tvayoktaṃ nātha sāṃvṛtam || NagAst_06"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "asty etat kṛtakaṃ sarvaṃ yat kiṃcid bālalāpanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "riktamuṣṭipratīkāśam ayathārthaprakāśitam || NagAst_07"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kṛtakaṃ vastu no jātaṃ tadā kiṃ vārtamānikam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kasya nāśād atītaṃ syād utpitsuḥ kim apekṣate || NagAst_08"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svasmān na jāyate bhāvaḥ parasmān nobhayād api |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na san nāsan na sadasan kutaḥ kasyodayas tadā || NagAst_09"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ajāte na svabhāvo 'sti kutaḥ svasmāt samudbhavaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svabhāvābhāvasiddhyaiva parasmād apy asaṃbhavah || NagAst_10"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svatve sati paratvaṃ syāt paratve svatvam iṣyate |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āpekṣikī tayoḥ siddhiḥ pārāvāram ivoditā || NagAst_11"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yadā nāpekṣate kiṃ cit kutaḥ kiṃ cit tadā bhavet |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yadā nāpekṣate dīrghaṃ kuto hrasvādikaṃ tadā || NagAst_12"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "astitve sati nāstitvaṃ dīrghe hrasvaṃ tathā sati |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nāstitve sati cāstitvaṃ yat tasmād ubhayaṃ na sat || NagAst_13"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ekatvaṃ ca tathānekam atītānāgatādi ca |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃkleśo vyavadānaṃ ca samyaṅmithyā svataḥ kutaḥ || NagAst_14"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svata eva hi yo nāsti bhāvaḥ sarvo 'sti kas tadā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "para ity ucyate yo 'yaṃ na vinā svasvabhāvataḥ || NagAst_15"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "na svabhāvo 'sti bhāvānāṃ parabhāvo 'sti no yadā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvagrahagrahāveśaḥ paratantro 'sti kas tadā || NagAst_16"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ādāv eva samaṃ jātāḥ svabhāvena ca nirvṛtāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anutpannāś ca tattvena tasmād dharmās tvayoditāḥ || NagAst_17"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "niḥsvabhāvās tvayā dhīman rūpādyāḥ saṃprakāśitāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "phenabudbudamāyābhramarīcikadalīsamāḥ || NagAst_18"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "indriyair upalabdhaṃ yat tat tattvena bhaved yadi |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jātās tattvavido bālās tattvajñānena kiṃ tadā || NagAst_19"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jaḍatvam apramāṇatvam athāvyākṛtatām api |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viparītaparijñānam indriyānāṃ tvam ūcivān || NagAst_20"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ajñānenāvṛto yena yathāvan na prapadyate |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "lokas tena yathābhūtam iti matvā tvayoditam || NagAst_21"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "astīti śāśvatī dṛṣtir nāstīty ucchedadarśanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tenāntadvayanirmukto dharmo 'yaṃ deśitas tvayā || NagAst_22"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "catuṣkoṭivinirmuktās tena dharmās tvayoditāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vijñānasyāpy avijñeyā vācāṃ kim uta gocarāḥ || NagAst_23"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svapnendrajālikodbhūtaṃ dvicandrodvīkṣaṇaṃ yathā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhūtaṃ tadvastu no bhūtaṃ tathā dṛṣṭaṃ jagat tvayā || NagAst_24"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "utpannaś ca sthito naṣṭaḥ svapne yadvat sutas tathā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na cotpannaḥ sthito naṣṭa ukto loko 'rthatas tvayā || NagAst_25"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāraṇāt saṃbhavo dṛṣṭo yathā svapne tathetaraḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃbhavaḥ sarvabhāvānāṃ vibhavo 'pi matas tathā || NagAst_26"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "rāgādijaṃ yathā duḥkhaṃ saṃkleśasaṃsṛtī tathā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃbhārapūraṇān muktiḥ svapnavad bhāṣitā tvayā || NagAst_27"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jātaṃ tathaiva no jātam āgataṃ gatam ity api |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "baddho muktas tathā jñānī dvayam icchen na tattvavit || NagAst_28"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "utpattir yasya naivāsti tasya kā nirvṛtir bhavet |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māyāgajaprakāśatvād ādiśāntatvam arthataḥ || NagAst_29"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "utpanno 'pi na cotpanno yadvan māyāgajo mataḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "utpannaṃ ca tathā viśvam anutpannaṃ ca tattvataḥ || NagAst_30"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ameyair aprameyānāṃ pratyekaṃ nirvṛtiḥ kṛtā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "lokanāthair hi sattvānāṃ na kaś cin mocitaś ca taiḥ || NagAst_31"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "te ca sattvāś ca no jātā ye nirvānti na te sphuṭam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na kaś cin mocitaḥ kaiś cid iti proktaṃ mahāmune || NagAst_32"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "māyākārakṛtaṃ yadvad vastuśūnyaṃ tathetarat |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vastuśūnyaṃ jagat sarvaṃ tvayoktaṃ kārakas tathā || NagAst_33"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kārako 'pi kṛto 'nyena kṛtatvaṃ nātivartate |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "atha vā tatkriyākartṛ kārakasya prasajyate || NagAst_34"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nāmamātraṃ jagat sarvam ity uccair bhāṣitaṃ tvayā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "abhidhānāt pṛthagbhūtam abhidheyaṃ na vidyate || NagAst_35"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kalpanāmātram ity asmāt sarvadharmāḥ prakāśitāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kalpanāpy asatī proktā yayā śūnyaṃ vikalpyate || NagAst_36"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvābhāvadvayātītam anatītaṃ ca kutra cit |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na ca jñānaṃ na ca jñeyaṃ na cāsti na ca nāsti yat || NagAst_37"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yan na caikaṃ na cānekaṃ nobhayaṃ na ca nobhayam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anālayam athāvyaktam acintyam anidarśanam || NagAst_38"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yan nodeti na ca vyeti nocchedi na ca śāśvatam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tad ākāśapratīkāśaṃ nākṣarajñānagocaram || NagAst_39"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yaḥ pratītyasamutpādaḥ śūnyatā saiva te matā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tathāvidhaś ca saddharmas tatsamaś ca tathāgataḥ || NagAst_40"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tat tattvaṃ paramārtho 'pi tathatā dravyam iṣyate |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhūtaṃ tad avisaṃvādi tadbodhād buddha ucyate || NagAst_41"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "buddhānāṃ sattvadhātoś ca tenābhinnatvam arthataḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ātmanaś ca pareṣāṃ ca samatā tena te matā || NagAst_42"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvebhyaḥ śūnyatā nānyā na ca bhāvo 'sti tāṃ vinā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasmāt pratītyajā bhāvās tvayā śūnyāḥ prakāśitāḥ || NagAst_43"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "hetupratyayasaṃbhūtā paratantrā ca saṃvṛtiḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "paratantra iti proktaḥ paramārthas tv akṛtrimaḥ || NagAst_44"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svabhāvaḥ prakṛtis tattvaṃ dravyaṃ vastu sad ity api |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nāsti vai kapito bhāvo paratantras tu vidyate || NagAst_45"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "astīti kalpite bhāve samāropas tvayoditaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nāstīti kṛtakocchedād ucchedaś ca prakāśitaḥ || NagAst_46"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvajñānena nocchedo na ca śāśvatatā matā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vastuśūnyaṃ jagat sarvaṃ marīcipratimaṃ matam || NagAst_47"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mṛgatṛṣṇājalaṃ yadvan nocchedi na ca śāśvatam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tadvat sarvaṃ jagat proktaṃ nocchedi na ca śāśvatam || NagAst_48"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dravyam utpadyate yasya tasyocchedādikaṃ bhavet |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "antavān nāntavāṃś cāpi lokas tasya prasajyate || NagAst_49"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jñāne sati yathā jñeyaṃ jñeye jñānaṃ tathā sati |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yatrobhayam anutpannam iti buddhaṃ tadāsti kim || NagAst_50"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iti māyādidṛṣtāntaiḥ sphuṭam uktvā bhiṣagvaraḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "deśayām āsa saddharmaṃ sarvadṛṣṭicikitsakam || NagAst_51"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "etat tat paramaṃ tattvaṃ niḥsvabhāvārthadeśanā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhāvagrahagṛhītānāṃ cikitseyam anuttarā || NagAst_52"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmayājñika tenaiva dharmayajño niruttaraḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "abhīkṣṇam iṣṭas trailokye niṣkapāṭo nirargalaḥ || NagAst_53"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vastugrāhabhayocchedī kutīrthyamṛgabhīkaraḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nairātmyasiṃhanādo 'yam adbhuto naditas tvayā || NagAst_54"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śūnyatādharmagambhīrā dharmabherī parāhatā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "naiḥsvābhāvyamahānādo dharmaśaṅkhaḥ prapūritaḥ || NagAst_55"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dharmayautukam ākhyātaṃ buddhānāṃ śāsanāmṛtam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nītārtham iti nirdiṣṭaṃ dharmāṇāṃ śūnyataiva hi || NagAst_56"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yā tūtpādanirodhādisattvajīvādideśanā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "neyārthā ca tvayā nātha bhāṣitā saṃvṛtiś ca sā || NagAst_57"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prajñāpāramitāmbhodher yo 'tyantam pāram āgataḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sa puṇyaguṇaratnāḍhyas tvadguṇārṇavapāragaḥ || NagAst_58"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iti stutvā jagannātham acintyam anidarśanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yad avāptaṃ mayā puṇyaṃ tenāstu tvatsamaṃ jagat || NagAst_59"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "|| ity acintyastavaḥ samāptaḥ ||"
    },
    {
