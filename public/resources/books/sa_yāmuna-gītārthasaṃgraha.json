  "body": [
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_1"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svadharmajñānavairāgyasādhyabhaktyekagocaraḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nārāyaṇaḥ paraṃ brahma gītāśāstre samīritaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_1"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "sva-dharma-jñāna-vairāgya-sādhya-bhakty-eka-gocaraḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "nārāyaṇaḥ paraṃ brahma gītā-śāstre samīritaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_2"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānakarmātmike niṣṭhe yogalakṣe susaṃskṛte |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ātmānubhūtisiddhyārthe pūrvaṣaṭkena coditaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_2"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "jñāna-karma-ātmike niṣṭhe yoga-lakṣe su-saṃskṛte |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ātma-anubhūti-siddhya-arthe pūrva-ṣaṭkena coditaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_3"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "madhyame bhagavattattvayāthātmyāvāptisiddhaye |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānakarmābhinirvartyo bhaktiyogaḥ prakīrtitaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_3"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "madhyame bhagavat-tattva-yāthātmya-avāpti-siddhaye |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "jñāna-karma-abhinirvartyo bhakti-yogaḥ prakīrtitaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_4"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pradhānapuruṣavyaktasarveśvaravivecanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "karma dhīr bhaktir ityādipūrvaśeṣo 'ntimoditaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_4"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "pradhāna-puruṣa-vyakta-sarva-īśvara-vivecanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "karma dhīr bhaktir ity-ādi-pūrva-śeṣo 'ntima-uditaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_5"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "asthānasnehakāruṇyadharmādharmadhiyākulam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pārthaṃ prapannam uddiśya śāstrāvatraṇaṃ kṛtam ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_5"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "asthāna-sneha-kāruṇya-dharma-adharma-dhiyā-kulam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "pārthaṃ prapannam uddiśya śāstra-avatraṇaṃ kṛtam ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_6"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nityātmāsaṅgakarmehagocarā sāṃkhyayogadhīḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dvitīye sthitadhīlakṣyā proktā tanmohaśāntaye ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_6"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "nitya-ātma-asaṅga-karma-iha-gocarā sāṃkhya-yoga-dhīḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "dvitīye sthita-dhī-lakṣyā proktā tan-moha-śāntaye ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_7"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "asaktyā lokarakṣāyai guṇeṣv āropya kartrktām |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarveśvare vā nyasyoktā tṛtīye karmakāryatā ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_7"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "asaktyā loka-rakṣāyai guṇeṣv āropya kartrktām |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "sarva-īśvare vā nyasya uktā tṛtīye karma-kāryatā ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_8"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prasaṅgāt svasvabhāvoktiḥ karmaṇo 'karmatāsya ca |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhedā jñānasya māhātmyam caturthādhyāya ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_8"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "prasaṅgāt sva-svabhāva-uktiḥ karmaṇo 'karmata āsya ca |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "bhedā jñānasya māhātmyam caturtha-adhyāya ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_9"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "karmayogasya saukaryaṃ śaighryaṃ kāścana tadvidhāḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "brahmajñānprakāraś ca pañcamādhyāya ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_9"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "karma-yogasya saukaryaṃ śaighryaṃ kāścana tad-vidhāḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "brahma-jñān-prakāraś ca pañcama-adhyāya ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_10"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yogābhyāsavidhir yogī caturdhā yogasādhanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yogasiddhaḥ svayogasya pāramyaṃ ṣaṣṭha ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_10"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "yoga-abhyāsa-vidhir yogī caturdhā yoga-sādhanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "yoga-siddhaḥ sva-yogasya pāramyaṃ ṣaṣṭha ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_11"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svayāthātmyaṃ prakṛtyāsya tirodhiḥ śaraṇāgatiḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhaktabhedaḥ prabuddhasya śraiṣṭhyaṃ saptama ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_11"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "sva-yāthātmyaṃ prakṛtya āsya tirodhiḥ śaraṇā-gatiḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "bhakta-bhedaḥ prabuddhasya śraiṣṭhyaṃ saptama ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_12"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aiśvaryākṣarayāthātmyaṃ bhagavaccaraṇārthiṇām |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vedyopādeyabhāvānām aṣṭame bhede ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_12"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "aiśvarya-akṣara-yāthātmyaṃ bhagavac-caraṇa-arthiṇām |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "vedya-upādeya-bhāvānām aṣṭame bhede ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_13"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svamāhātmyaṃ manuṣyatve paratvaṃ ca mahātmanām |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "viśeṣo navame yogo bhaktirūpaḥ prakīrtitaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_13"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "sva-māhātmyaṃ manuṣyatve paratvaṃ ca mahā-ātmanām |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "viśeṣo navame yogo bhakti-rūpaḥ prakīrtitaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_14"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svakalyāṇaguṇānantyakṛtsnasvādhīnatāmatiḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhaktyutpattivivṛdhyarthā vistīrṇā daśamoditā ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_14"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "sva-kalyāṇa-guṇa-ānantya-kṛtsna-sva-adhīnatā-matiḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "bhakty-utpatti-vivṛdhy-arthā vistīrṇā daśama-uditā ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_15"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ekādaśe 'sya yāthātmyasākṣātkārāvalokanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dattam uktā vidiprāptyor bhaktyekopāyatā tathā ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_15"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "ekādaśe 'sya yāthātmya-sākṣātkāra-avalokanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "dattam uktā vidi-prāptyor bhakty-eka-upāyatā tathā ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_16"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhaktiśraiṣṭhyam upāyoktir aśaktasyātmaniṣṭhatā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatprakārās tv atiprītir bhakte dvādaśa ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_16"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhakti-śraiṣṭhyam upāya-uktir aśaktasyā atma-niṣṭhatā |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tat-prakārās tv atiprītir bhakte dvādaśa ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_17"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dehasvarūpam ātmāptihetur ātmaviśodhanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bandhahetur vivekaś ca trayodaśa udīryate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_17"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "deha-svarūpam ātma-āpti-hetur ātma-viśodhanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "bandha-hetur vivekaś ca trayo-daśa udīryate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_18"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "guṇabandhavidhau teṣāṃ kartṛtvam tannivartanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "gatitrayasvamūlatvaṃ caturdaśa udīryate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_18"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "guṇa-bandha-vidhau teṣāṃ kartṛtvam tan-nivartanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "gati-traya-sva-mūlatvaṃ catur-daśa udīryate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_19"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "acinmiśrād viśuddhāc ca cetanāt puruṣottamaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vyāpānād bharaṇāt svāmyād anyaḥ pañcadaśoditaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_19"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "acin-miśrād viśuddhāc ca cetanāt puruṣa-uttamaḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "vyāpānād bharaṇāt svāmyād anyaḥ pañca-daśa-uditaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_20"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "devāsuravibhāgoktipūrvikā śāstravaśyatā |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tattvānuṣṭhānavijñānasthemne ṣoḍaśa ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_20"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "deva-asura-vibhāga-ukti-pūrvikā śāstra-vaśyatā |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tattva-anuṣṭhāna-vijñāna-sthemne ṣoḍaśa ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_21"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aśāstram āsuraṃ kṛtsnam śāstrīyaṃ guṇataḥ pṛthak |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "lakṣaṇaṃ śāstrasiddhasya tridhā saptadaśoditam ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_21"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "aśāstram āsuraṃ kṛtsnam śāstrīyaṃ guṇataḥ pṛthak |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "lakṣaṇaṃ śāstra-siddhasya tridhā sapta-daśa-uditam ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_22"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "īśvare kartṛtābuddhiḥ sattvopādeyatāntime |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svakarmapariṇāmaś ca śāstrasārārtha ucyate ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_22"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "īśvare kartṛtā-buddhiḥ sattva-upādeyatā-antime |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "sva-karma-pariṇāmaś ca śāstra-sāra-artha ucyate ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_23"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "karmayogas tapastīrthadānayajñādisevanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānayogo jitasvāntaiḥ pariśuddhātmani sthitiḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_23"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "karma-yogas tapas-tīrtha-dāna-yajña-ādi-sevanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "jñāna-yogo jita-sva-antaiḥ pariśuddha-ātmani sthitiḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_24"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhaktiyogaḥ parāikāntyaprītyā dhyānādiṣu sthitiḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "trayānām api yogānāṃ tribhir anyonyasaṃgamaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_24"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhakti-yogaḥ para-aikāntya-prītyā dhyāna-ādiṣu sthitiḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "trayānām api yogānāṃ tribhir anyonya-saṃgamaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_25"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nityanaimittikānāṃ ca parārādhanarūpiṇam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ātmaṛṣṭes trayo 'py ete yogadvāreṇa sādhakāḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_25"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "nitya-naimittikānāṃ ca para-ārādhana-rūpiṇam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ātma-ṛṣṭes trayo 'py ete yoga-dvāreṇa sādhakāḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_26"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nirastanikhilājñāno dṛṣṭvātmānaṃ parānugam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pratilabhya parāṃ bhaktiṃ tayaivāvāpnoti tatpadam ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_26"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "nirasta-nikhila-ajñāno dṛṣṭvā ātmānaṃ para-anugam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "pratilabhya parāṃ bhaktiṃ taya aiva avāpnoti tat-padam ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_27"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhaktiyogas tadarthī cet samagrāiśvaryasādhanam |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ātmārthī cet trayo 'py ete tatkaivalyasya sādhakāḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_27"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhakti-yogas tad-arthī cet samagra-aiśvarya-sādhanam |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ātma-arthī cet trayo 'py ete tat-kaivalyasya sādhakāḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_28"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aikāntyaṃ bhagavaty eṣāṃ samānam adhikāriṇām |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yāvatprāpti parārthī cet tad evātyantam aśunute ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_28"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "aikāntyaṃ bhagavaty eṣāṃ samānam adhikāriṇām |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "yāvat-prāpti para-arthī cet tad eva atyantam aśunute ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_29"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "jñānī tu paramaikāntī tadāyattātmajīvanaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatsaṃśleṣaviyogaikasukhaduḥkhas tadekadhīḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_29"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "jñānī tu parama-ekāntī tad-āyatta-ātma-jīvanaḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tat-saṃśleṣa-viyoga-eka-sukha-duḥkhas tad-eka-dhīḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_30"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhagavaddhyānayogoktivandanastutikīrtanaiḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "labdhātmā tadgataprāṇamanobuddhīndriyakriyaḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_30"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhagavad-dhyāna-yoga-ukti-vandana-stuti-kīrtanaiḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "labdha-ātmā tad-gata-prāṇa-mano-buddhi-indriya-kriyaḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_31"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "nijakarmādibhaktyantaṃ kuryāt prītyaiva kāritaḥ |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "upāyatāṃ parityajya nyasyed deve tu tām abhīḥ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_31"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "nija-karma-ādi-bhakty-antaṃ kuryāt prītya aiva kāritaḥ |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "upāyatāṃ parityajya nyasyed deve tu tām abhīḥ ||"
            }
          ]
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {
        "{http://www.w3.org/XML/1998/namespace}id": "YGas_32"
      },
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aikāntyātyantadāsyaikaratis tatpadam āpnuyāt |"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tatpradhānam idaṃ śāstram iti gītārthasaṃgrahaṃ ||"
        }
      ]
    },
    {
      "tag": "note",
      "attributes": {
        "type": "analysis",
        "corresp": "#YGas_32"
      },
      "children": [
        {
          "tag": "lg",
          "attributes": {},
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "aikāntya-atyanta-dāsya-eka-ratis tat-padam āpnuyāt |"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "tat-pradhānam idaṃ śāstram iti gītārthasaṃgrahaṃ ||"
            }
          ]
        }
      ]
    }
  ]
}