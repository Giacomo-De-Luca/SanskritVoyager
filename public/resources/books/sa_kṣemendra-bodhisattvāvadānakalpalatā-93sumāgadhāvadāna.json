  },
  "body": [
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ślāghyā jayanti jinabhaktiviśeṣabhājāṃ śraddhāsudhāprasaranirjharaśīkarās te /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "niścetano 'py ucitacetanatām ivaiti yaḥ pūjyapūjanavidhau kusumādivargaḥ // KAvk_93.1 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śrāvastyāṃ vijanāsīnaṃ jinaṃ jetavane purā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anāthapiṇḍado 'bhyetya bhagavantam abhāṣata // KAvk_93.2 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tvadbhaktir iva sarvatra mahārhaguṇaviśrutā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhagavan prauḍhim āyātā kanyā mama sumāgadhā // KAvk_93.3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śrīmataḥ sārthanāthasya nagare puṇḍravardhane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sūnur vṛṣabhadattākhyas tatpāṇigraham iṣyate // KAvk_93.4 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kanyāṃ dadāmi tāṃ tasmai bhagavan yadi manyase /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tvadadhīnadhanaprāṇas tvadājñāśaraṇo hy aham // KAvk_93.5 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ity ukte tena bhagavān vātsalyavimalāśayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ko doṣas tanayā tasmai dīyatām ity abhāṣata // KAvk_93.6 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śāstuḥ śāsanam ādāya sādaraṃ pranipatya tam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anāthapiṇḍadaḥ śrīmān prayayau nijamandiram // KAvk_93.7 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tataḥ sumāgadhāṃ kanyāṃ vibhavena mahīyasā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "arhāyaṃ sa dadau tasmai bhūriratnāmbarapradaḥ // KAvk_93.8 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dattā dūrataraṃ deśam atha yāntī sumāgadhā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhagavaccaraṇasmṛtyā sabāṣpanayanābhavat // KAvk_93.9 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "cireṇa sā samāsādya nagaraṃ puṇḍravardhanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śuśrūṣābhiratā patyuḥ sadā bhartṛgṛhe 'vasat // KAvk_93.10 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kadācid atha tāṃ śvaśrūr bhojyasaṃbhārakāriṇī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "uvāca dhanavatyākhyā saṃkhyātītavyayodyatā // KAvk_93.ll //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pūjyopakaraṇaṃ sajjaṃ kuru sarvaṃ sumāgadhe /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "gṛhān naḥ prātar āgantā jagatpūjyaguṇo jinaḥ // KAvk_93.12 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "guravo gauravapadaṃ pūjyāḥ sarvajanasya te /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sameṣyanti kṣapaṇakāḥ saṃmohakṣapaṇodyatāḥ // KAvk_93.l3 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ity uktā sā tayā tatra babhūvārambhatatparā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "jñātvā bhikṣugaṇāyeva tāṃ pūjāṃ parikalpitām // KAvk_93.14 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "athāpare 'hni viviśur nagnāḥ kṣapaṇakā gṛham /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ulluñcitakacaśmaśrusaṃkleśaniśitavratāḥ // KAvk_93.15 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tān alajjān avasanān avalokya sumāgadhā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "māṣaśaṣpāśanābhyāsapīvarān mahiṣān iva // KAvk_93.16 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "lajjitā vāsasācchādya vadanaṃ gurusaṃnidhau /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "khedanirvedavinatā śvaśrūjanam abhāṣata // KAvk_93.17 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aho vatāyam ācāraḥ sucirād avalokitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "digambarāṇām apy agre yad ihāste vadhūjanaḥ // KAvk_93.18 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ete bhavadgṛhe 'dāntā bhuñjante śṛṅgavarjitāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "amānuṣatvān naiteṣāṃ lajjante nūnam aṅganāḥ // KAvk_93.19 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "asthāne bhavatāṃ bhaktiḥ ko 'yam ucchṛṅkhalaḥ kramaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "na tyaktam aśanaṃ yena sa kathaṃ vastram ujjhati // KAvk_93.20 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "keśonmūlanakarmaṇaiva niśitaṃ nairghṛṇyam āveditaṃ"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kaupīnāṃśukavarjanena sujane śīlasya vārtaiva kā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dambhārambhabhayaṃkare ca vadane krodhaḥ svayaṃ lakṣyate"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nagnānām aśanaiṣiṇāṃ niyaminām eṣāṃ paśūnām iva // KAvk_93.21 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yatraite paśavaḥ pūjyās tatrotsāryā bhavanti ke /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "athavā deśadoṣo 'yaṃ gatānugatikā sthitiḥ // KAvk_93.22 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iti bruvāṇāṃ tāṃ śvaśrūr viṣaṇṇā pratyabhāṣata /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pitus te bhavane bhadre pūjyate vada kīdṛśaḥ // KAvk_93.23 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāvadan matpitur gehe pūjyate bhagavān jinaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāruṇyāt sarvajagatāṃ kuśalātiśayodyataḥ // KAvk_93.24 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "dhyānādhīnaḥ stimitanayanaḥ pūrṇalāvaṇasindhur"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nāsāvaṃśaṃ vipulasaralaṃ setubhūtaṃ dadhānaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhūṣāśūnyaprasṛtarucimatkarṇapāśābhirāmaḥ"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kāntyaivāsau kim api viduṣāṃ śāntim antas tanoti // KAvk_93.25 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śirasi sahajaprājyālokaprasekamayaṃ maṇiṃ"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "karipatikarākārau bāhūkavat kanakadyutiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "karatalagatām lekhāṃ śaṅkhadhvajāmbujamālikāṃ"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śamamayamahāsāṃrājyārhaṃ bibharti sa lakṣaṇam // KAvk_93.26 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tasyābhilāṣajanakasya mahāmunīnāṃ sarvābhilāṣakalanārahitaḥ svabhāvaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "niḥśeṣitasmṛtibhuvaḥ pramadāśrayasya rāgojjhitasya sutarām adharaḥ sarāgaḥ // KAvk_93.27 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mūrtir nirbharasaṃgamapraṇayinī maitrī manaḥśāyinī"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kṣāntis tanmayakāriṇī hṛdi dayā gāḍhaṃ samāśleṣiṇī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sarvāśābharaṇasya bhūridayitāsaktasya saṃlakṣyate"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasyāpūrvamaṇer ananyamahimā vairāgyagarbhaḥ śamaḥ // KAvk_93.28 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pūjyaḥ sa bhavane 'smākaṃ yasya pravrajyayā satām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dhatte śīladukūlānām nirāvaraṇatāṃ manaḥ // KAvk_93.29 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "viśvarakṣāmaṇer yasya smṛtyāpi bhavabhoginā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "rāgadveṣogradaṃṣṭreṇa bādhyate na punar janaḥ // KAvk_93.30 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iti tasyā vacaḥ śvaśrūḥ śrutvā śrotrarasāyanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sadyaḥ prasādaviśadā tāṃ jagāda pramodinī // KAvk_93.31 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "api taddarśanopāyaḥ kaścid asti varānane /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "api tvatpuṇyasaṃbandhād vayam apy amṛtāspadam // KAvk_93.32 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iti sānunayaṃ śvaśrvā sā sādaradhiyārthitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "taṃ vaḥ saṃdarśayāmīti babhāṣe bhaktimāninī // KAvk_93.33 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "mahāpratijñāsaṃbhārabharanirvahaṇaiṣiṇī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sā saṃśayatulāruḍhā kṣaṇaṃ dhyānaparābhavat // KAvk_93.34 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tataḥ prāsādam āruhya bhagavatsevitāṃ diśam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "praṇipatyāsṛjat pūjyapūjārhakusumāñjalim // KAvk_93.35 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "puṣpadhūpodakair arcāṃ sā kṛtvā tatpadonmukhī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ānandabāṣpasaṃruddhavistīrṇanayanāvadat // KAvk_93.36 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anukampyaiva bhagavan ratnatrayavivarjitā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tavāśramamṛgīvāhaṃ dūraṃ deśam imam gatā // KAvk_93.37 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tvatpādapadmayugalaṃ śaraṇam prapannāṃ dūrasthitām api dṛśā spṛśa māṃ dayālo /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vātsalyapeśaladhiyāṃ mahatāṃ pravāsadurīkṛteṣu karuṇā na tanutvam eti // KAvk_93.38 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhagavan dāsasutayā tvaṃ mayādya nimantritaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prātar āgamanenaiva mānaṃ vitara me vibho // KAvk_93.39 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ity udīrya tayotsrṣṭā vicitrakusumāvalī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sajīviteva prayayau nabhasā bhaktidūtikā // KAvk_93.40 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sā śvetaraktaharitāsitapuṣpapālī khe dhūpadhūmaśabalā śanakaiḥ prayāntī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bālāmbudapraṇayinī suciraṃ cakāśe saṃcālicāpalatikeva śacīdhavasya // KAvk_93.41 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atha jetavanaṃ prāpya kṣaṇena kusumāvalī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sā bhaktiśālinī śāstuḥ papāta caraṇābjayoḥ // KAvk_93.42 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhagavān api sarvajñaḥ sarvaṃ jñātvā samīhitam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sumāgadhāyāḥ kāruṇyād ānandam avadat puraḥ // KAvk_93.43 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "prātar gantavyam asmābhir nagaraṃ puṇḍravardhanam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sumāgadhā prārthayate sasaṃghasya mamārcanam // KAvk_93.44 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śataṣaṣṭyadhikaṃ tatra yojanānām itaḥ param /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ekenāhnā ca gantavyaṃ na vilambo 'tra yujyate // KAvk_93.45 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vyomnā maharddhyā śaknoti gantuṃ yo yaḥ prabhāvavān /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nimantraṇaśalākāṃ tvaṃ tasmai tasmai samarpaya // KAvk_93.46 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "preritaḥ sugateneti sa bhikṣubhyo nyavedayat /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ekāhagamanaprāpyaṃ śalākābhir nimantraṇam // KAvk_93.47 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śalākāsu gṛhītāsu tatra sarvair maharddhibhiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pūrṇaḥ kumbhopadhānīyaḥ sthaviro 'py agrahīt kramāt // KAvk_93.48 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tena prāptaprabhāvena śalākāyāṃ prasārite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pāṇau prāha tam ānandaḥ kiṃcit smitasitānanaḥ // KAvk_93.49 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anāthapiṇḍadagṛhaṃ na gantavyaṃ padadvayam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṣaṣṭiyojanaśataṃ dinārdhenaiva laṅghyate // KAvk_93.50 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ity uktaḥ sthaviras tena vailakṣyavinatānanaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "acintayat svavargāgre nyūnabhāvo hi duḥsahaḥ // KAvk_93.5l //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anādikālopacitāḥ kleśajanmajarādayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hantuṃ sapatnaiḥ śakyante prāptum ṛddhipadaṃ kiyat // KAvk_93.52 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iti cintayatas tasya tīvrasaṃvegayā dhiyā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prādur āsīt kṣaṇenaiva maharddhiḥ śuddhacetasaḥ // KAvk_93.53 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atha rātryāṃ vyatītāyāṃ prabhāte sarvabhikṣavaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "nānātridaśaveśeṇa vimānair nabhasā yayuḥ // KAvk_93.54 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atrāntare mahārambhasambhāraparipūrite /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sumāgadhā bhartṛgṛhe bhagavaddarśanotsukā // KAvk_93.55 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saha prāsādam āruhya śvaśrūśvaśurabhartṛbhiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tasthau kusumadhūpārdhyāracanāsaṃgrahonmukhī // KAvk_93.56 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "bhikṣur ājñātakauṇḍinyas tato 'śvaratham āsthitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "divyarddhivividhāścaryaḥ prathamaṃ pratyadṛśyata // KAvk_93.57 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "taṃ dṛṣṭvā sūryasaṃkāśaṃ vismitāḥ śvaśurādayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ūcuḥ sumāgadhāṃ prītyā kim eṣa bhagavān iti // KAvk_93.58 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sāvadad bhagavān nāyaṃ dṛśyate taraṇiprabhaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "ayam ājñātakauṇḍinyo bhikṣur akṣuṇṇadīdhitiḥ // KAvk_93.59 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āpatatsu krameṇātha ratheṣu śvaśurādayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kim ayaṃ kim ayaṃ bhadre bhagavān iti tāṃ jaguḥ // KAvk_93.60 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sābravīn naiva śāstāyam ete tacchāsanocitāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhikṣavaḥ praśamaślāghyās tapodīptataratviṣaḥ // KAvk_93.6l //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yaḥ kāntahemadrumaramyaśailaśrṅgādhirūḍhaḥ parato 'bhyupaiti /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āścaryakṛn mūrta iva prabhāvaḥ śrīmān mahākāśyapa eṣa bhikṣuḥ // KAvk_93.62 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pañcānanasyandanam āsthito yaḥ satoyajīmūtagabhīraghoṣam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vyomnā samabhyeti gunaiḥ pragītaḥ sa eṣa bhikṣur bhuvi śāriputraḥ // KAvk_93.63 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āruhya kailāsam ivātiśubhraṃ dvipam caturdantam anantakāntiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āyāti yaḥ puṇyavatāṃ jagatsu maudgalyanāmā ca sa eṣa bhikṣuḥ // KAvk_93.64 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vaidūryanālaṃ kanakāravindam āruhya ratnāṅkurakeśarāḍhyam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "upaiti yaḥ saurabhapūritāśaḥ sa eṣa bhikṣuḥ prathito 'niruddhaḥ // KAvk_93.65 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yaś cāmbarāgraṃ garuḍādhirūḍhaḥ pakṣānilotsāritavārivāhaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "vigāhate sphitavanena bhikṣur maitrāyaṇīsūnur ayaṃ sa pūrṇaḥ // KAvk_93.66 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "anantam āsthāya nitāntaśāntam īrṣyāpathaṃ sattvamahodadhir yaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prabhāmṛtaiḥ sarpati tarpitāśaḥ prabhāvavān eṣyajid eṣa bhikṣuḥ // KAvk_93.67 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vilolavallīvalayābhirāmaṃ viśālam āruhya suvarṇatālam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yaḥ puṇyapūrṇadyutir abhyupaiti sa eṣa bhikṣur matimān upālī // KAvk_93.68 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "āruhya vaiḍūryavimānaśṛṅgaṃ suvarṇaratnojjvalapatralekham /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "limpann ivāgacchati yaḥ prabhābhiḥ kātyāyano nāma sa eṣa bhikṣuḥ // KAvk_93.69 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śarīriṇaṃ dharmam ivādhiruhya vigāhate khaṃ vṛṣavāhano yaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "pṛṣṭhaḥ pratiṣṭhāpariniṣṭhitānāṃ gariṣṭhadhīḥ kauṣṭhila eṣa bhikṣuḥ // KAvk_93.70 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vimānahaṃsadyutibhir muhūrtaṃ smitormiramyaṃ muhur antarīkṣam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kurvan samabhyeti taponidhir yaḥ pilindavatsāhvaya eṣa bhikṣuḥ // KAvk_93.7l //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yo 'yaṃ samutphullalatāvitānavanāntarāle viharann upaiti /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sa śroṇakoṭiḥ śruta eṣa bhikṣur akṣuṇṇalakṣmīr gṛhanirvyapekṣaḥ // KAvk_93.72 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "yaś cakravartī divi bhāti so 'yaṃ śāstuḥ suto rāhulakābhidhānaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "hemaprabhābhūṣitadigvibhāgaḥ saṃlakṣyate merur ivānyarūpaḥ // KAvk_93.73 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ete giribhyo 'tha digantarebhyaḥ kṣmāmaṇḍalād vyomataṭāntarāc ca /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "āyānty asaṃkhyādbhutabhikṣusaṃghā vicitraratnāsanavāhanasthāḥ // KAvk_93.74 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tayā krameṇeti nivedyamānaṃ te bhikṣusaṃghaṃ vimukhaṃ vilokya /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "yayuḥ praharṣādbhutasaṃbhramāṇāṃ vidheyatāṃ tulyam ananyalakṣmyāḥ // KAvk_93.75 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atha jvalatkāñcanacūrṇavarṇaṃ jagad babhūvārkaśataprakāśam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "aśeṣasaṃtāpaviśeṣaśāntyā śītāṃśumālāśataśītalaṃ ca // KAvk_93.76 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "atha dhanapatiśakrabrahmamukhyair amartyair vipulagaganayātrādattasevānuyātraḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "amarapurapurastrīkīrṇapuṣpaprabhāvān nayanapatham ayāsīt puṇyabhājāṃ jinendraḥ // KAvk_93.77 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "aṣṭādaśadvārapathā puraṃ tat sa tulyam aṣṭādaśamūrtir eva /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "praviśya cakre śaśikāntaratnaśikhāmayaṃ veśma sumāgadhāyāḥ // KAvk_93.78 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "abhyarcitas tatra bahuprakāraiḥ pūrṇopacāraiḥ praṇipatya sarvaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "apūjayat paurajanaḥ samantād bahiś ca bhittipratibimbitaṃ tam // KAvk_93.79 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sumāgadhāyā dayayā dayāluḥ pūjāṃ gṛhītvā bhagavān sasaṃghaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anugrahālokanasaṃvibhāgaiḥ sarvānvavāyā vidadhe prasādam // KAvk_93.80 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sumāgadhā saśvaśurādivargā sahāparaiḥ paurajanaiś ca sarvāḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "śāstus tayā deśanayā babhūvuḥ śuddhāśayās tatkṣaṇadṛṣṭasatyāḥ // KAvk_93.81 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sumāgadhāyāḥ kuśalānubandhaṃ puṇyaṃ prabhāvaṃ vipulaṃ vilokya /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "te bhikṣavas tatra kutūhalena papracchū ramyaṃ jinam ādivṛttam // KAvk_93.82 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "pṛṣṭaḥ sa taiḥ saṃsadi sarvadarśī sumāgadhāyāḥ kuśalasya hetum /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dantaprabhābhiḥ kakubhāṃ mukheṣu diśan prakāśam bhagavān babhāṣe // KAvk_93.83 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vārāṇasyām abhūt pūrvaṃ kṛkeḥ kantasya bhūpateḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sutā kāñcanamālākhyā kucakāñcanamālikā // KAvk_93.84 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kāśyapākhyasya śāstuḥ sā satataṃ bhaktiśālinī /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "paricaryāṃ vyadhāt sārdhaṃ sakhīnāṃ pañcabhiḥ śataiḥ // KAvk_93.85 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sa kadācin narapatir vikṛtasvapnadarśanāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhayasaṃśayasaṃbhrāntaḥ papraccha phalakovidān // KAvk_93.86 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "te taṃ rājasutādveṣān nimittajñā babhāṣire /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "atipriyasya hṛdayaṃ hutvāgnau labhyate śivam // KAvk_93.87 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "iti teṣām anādṛtya vacaḥ krūrataraṃ nṛpaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhagavantaṃ yayau draṣṭuṃ kāśyapaṃ duhitur girā // KAvk_93.88 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sa tam etyāvadat svapnaḥ savikāraḥ paraṃ mayā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dṛṣṭo 'dya sarvaṃ sarvajña tatphalaṃ vaktum arhasi // KAvk_93.89 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "vātāyanena nirgacchan ruddhapuccho mayā gajaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tṛṣitasya tathā paścāt kupo dhāvan vilokitaḥ // KAvk_93.90 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tṛptaś ca śaktuprasthena mauktiprasthavikrayaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "samīkṛtāni dṛṣṭāni kudārūṇi ca candanaiḥ // KAvk_93.9l //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kalabhena mahāhastī samāhūtas tathāhave /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "parān aśuciliptāṅgaḥ pralimpan viplutaḥ kapiḥ // KAvk_93.92 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sphītarājyām abhiṣiktaś ca kucāpalanidhiḥ kapiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "paṭo 'ṣṭādaśabhiḥ kṛṣṭaḥ puruṣair apy asaṃkṣayaḥ // KAvk_93.93 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ramyapuṣpaphalārāmaś caurair api viluṇṭhitaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "dveṣopahāsakalahāsaktaś ca vipulo janaḥ // KAvk_93.94 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "svapnādbhutānām eteṣāṃ manye ghorataraṃ phalam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "iti pṛṣṭaḥ kṣitibhujā bhagavān kāśyapo 'bravīt // KAvk_93.95 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "śatāyuṣi jane śāstā śāntaḥ śākyamunir jinaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bhaviṣyaty amṛtāmbhodhiḥ sa drṣṭaḥ kuñjaras tvayā // KAvk_93.96 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tasyāpi paścime kāle śrāvakāḥ kalisaṃśrayāt /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tyaktaśīlaguṇācārā bhaviṣyanti saviplavāḥ // KAvk_93.97 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "apakvālpavivekānāṃ balāt te gṛhavāsinām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "svayaṃ sevāṃ samālambya kariṣyanty eva deśanām // KAvk_93.98 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "arthanīyo 'rthibhāvena yasmāt sevāsu dhāvati /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "tṛṣitasya vrajan paścāt kūpas tasmād vilokitaḥ // KAvk_93.99 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "te kariṣyanti lobhāndhaḥ saṃmohopahatāḥ param /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "bodhyaṅgamuktāprasthasya śaktuprasthena vikrayam // KAvk_93.lOO //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tīrthavākyakudārūṇi buddhabhāṣitacandanaiḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sāmyam āpādayiṣyanti te maugdhyād aviśeṣiṇaḥ // KAvk_93.101 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "kvacid bhadraṃ samāsādya vinītaṃ bhikṣukuñjaram /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "duḥśīlakalabho bhikṣuḥ spardhayā dhik kariṣyati // KAvk_93.102 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "cāpalāśuciliptāṅgaḥ suśīlān bhikṣumarkaṭaḥ /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "anulimpan svadoṣeṇa kariṣyaty ātmasannibhān // KAvk_93.103 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "ṣaṇḍakasyābhiṣekaś ca bhaviṣyati kaper iva /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃbuddhaśāsanapaṭaṃ kṛṣyamāṇaṃ na naṃkṣyati // KAvk_93.104 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃghadravyaphalārāmeṣv api yāsyanti cauratām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "mithaḥ kalahaśīlāś ca bhaviṣyanty apavādinaḥ // KAvk_93.105 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "tava svapnavipākānte phalāny etāni bhūtale /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "iti śāstur vacaḥ śrutvā vismito 'bhūn mahīpatiḥ // KAvk_93.106 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "sānugasyātha nṛpateḥ sa śāstā dharmadeśanām /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "kṛtvā kāñcanamālāyā dideśa kuśalārhatām // KAvk_93.l07 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "janmāntale cakārārcāṃ stūpe nāraṅgamālayā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sā tena sukṛtenaiva jātā hemasrajāṅkitā // KAvk_93.108 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "seyaṃ sumāgadhā puṇyaprabhāveṇa mahīyasā /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "prāptādya janakāyasya samyak kuśalasetutām // KAvk_93.109 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "adhidhāyeti bhagavān nabhasā bhikṣubhiḥ saha /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "rucā saṃpūrayann āśāṃ yayau jetavanaṃ jinaḥ // KAvk_93.ll0 //"
        }
      ]
    },
    {
      "tag": "lg",
      "attributes": {},
      "children": [
        {
          "tag": "l",
          "attributes": {},
          "text": "puṃsāṃ satkulabhūtaye balavatī mithyaiva putraspṛhā"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sūnuś ced aguṇas tad eva viphalaṃ tat kiṃ na sarvaṃ kulam /"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "sā kanyā kila jāyate guṇavatī puṇyaprabhāvād yayā"
        },
        {
          "tag": "l",
          "attributes": {},
          "text": "saṃsārorusaritpatau kulayugam nāv eva saṃtāryate // KAvk_93.111 //"
        }
      ]
    },
    {
      "tag": "p",
      "attributes": {},
      "text": "iti kṣemendraviracitāyāṃ bodhisattvāvadānakalpalatāyāṃ sumāgadhāvadānaṃ trinavatitamaḥ pallavah."
    }
  ]
}