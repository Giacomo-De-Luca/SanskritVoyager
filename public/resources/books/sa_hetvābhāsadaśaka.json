  "body": [
    {
      "tag": "div",
      "attributes": {},
      "children": [
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_1"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "tvaṃ ruṣṭā mayi cen namāmi bhavatīṃ neyaṃ tvayā namyatāṃ",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "namyatvaṃ tvayi bhāti pārthivatayā manye mahārājavat /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "kumbhādiṣv ativartanāt tvaduditaṃ sādhāraṇaṃ sādhanaṃ",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "vāgvādeṣv iti nirjito dayitayā rāmaḥ sukhaṃ rātu vaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_2"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "kṛṣṇaḥ puṣṇātu saukhyaṃ sumukhi sumukhatāṃ yāhi yāhi priyāṃ tām",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "anyāsaktaṃ kuto māṃ kalaṣasi dayite kevalaṃ tvatsvabhāvāt /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "yady evaṃ mugdhatā te karaṇam idam asādhāraṇaṃ pakṣamātre",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "vṛttatvād ity akasmāt praṇayakalahitāṃ rādhikāṃ sāntvayan vaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_3"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "sarvaṃ vastu sukhāvahaṃ tava punaḥ kautaskutīyaṃ vyathā",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "kasmād evam udīryate jagad idaṃ yat tanmayaṃ bhāṣase /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "hetus savyabhicāra eṣa virahāt dṛṣṭāntayor ity ayaṃ",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "saṃlāpas saha lakṣmaṇena viyujaḥ sītāpateḥ pātu vaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_4"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "bhīto 'haṃ virahī tuṣārakiraṇāt so 'yaṃ samujjṛmbhate",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "vijñātaḥ katham eṣa te mama yatas tāpaṃ karoty uccakaiḥ /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "liṅgāt ko 'numinoti sādhyavirahavyāptād iyaṃ mugdhatā",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "mā bhaiṣīr iti pātu vo raghupatis saumitriṇā sāntvitaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_5"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "kā bhītis tava candrato raghupate doṣākaratvād asāv",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "āsaktaḥ parapīḍane piśunavan naivaṃ kutaḥ kathyate /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "hetuḥ satpratipakṣa eṣa yad ayaṃ sadvṛttatāṃ gāhate",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "sodaryeṇa nirastabhīr iti sukhaṃ puṣṇātu vo rāghavaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_6"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "mithyeyaṃ virahavyathā tava mudaṃ datte purovartinī",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "māyāmaithilakanyakā priyatamābhāvena satyeva sā /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "ity uktaḥ sahajena sāntvanavidhau hetor amuṣyāśrayā",
              "children": [
                {
                  "tag": "orig",
                  "attributes": {},
                  "text": "-"
                },
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "siddhatvaṃ pratipādayann avatu vaḥ śrījānakīvallabhaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_7"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "cittaṃ me cittajanmā vidalayati śaraiḥ svāśrayaṃ nāśayet kaḥ",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "sthūlatvāc cetaso 'sau kim iva na kurute nirvicārapravṛttaḥ /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "āhuś ceto 'ṇumānaṃ katham iha bhavati sthūlatā tatsvarūpā",
              "children": [
                {
                  "tag": "orig",
                  "attributes": {},
                  "text": "-"
                },
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "siddho 'yaṃ hetur evaṃ daśarathasutayoḥ pātu saṃbhāṣitaṃ vaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_8"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "vadhyetāṃ śukasāraṇau na hi tayor vadhyatvam asty eva tat",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "rakṣastvād daśakandharādivad ayaṃ hetus sahopādhikaḥ /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "sadrohatvam upādhir atra sugamaḥ śeṣas tvayā cintyatām",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "ity ādityasutoktikhaṇḍanakaraḥ śrīrāghavaḥ pātu vaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_9"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "jayyo rāghava eṣa mānuṣatayā tadbhakṣibhīr yātubhiḥ",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "naivaṃ bādhita eva hetur uditaḥ pratyakṣamānena hi /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "kva pratyakṣam idaṃ kharādinihatāv ity agrajasyāgrato",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "diśyāt tanmatabhidvibhīṣaṇasamudgītas sukhaṃ rāghavaḥ //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_10"
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "ajñātvā hṛdgataṃ te katham abhilaṣitaṃ prārthaye bhāmini tvāṃ",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "nanv etasmāt kaṭākṣād anuminu hṛdaye rāga eveti cen na /"
                }
              ]
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "hetus sopādhiko 'yaṃ bhavati gurutaro hrībharo 'sminn upādhiḥ",
              "children": [
                {
                  "tag": "lb",
                  "attributes": {}
                },
                {
                  "tag": "text",
                  "text": "sādhyavyāpī priyādau sa khalu raṇamukhe sādhanāvyāpakaś ca //"
                }
              ]
            }
          ]
        },
        {
          "tag": "lg",
          "attributes": {
            "{http://www.w3.org/XML/1998/namespace}id": "hetvād_col",
            "n": ""
          },
          "children": [
            {
              "tag": "l",
              "attributes": {},
              "text": "koṭiliṃgapure vāsī yuvarājo mahākaviḥ /"
            },
            {
              "tag": "l",
              "attributes": {},
              "text": "hetvābhāsam udājahre daśaślokaiḥ satāṃ mude //"
            }
          ]
        }
      ]
    }
  ]
}